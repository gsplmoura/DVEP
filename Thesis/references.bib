}
@article{Mielus2019,
abstract = {Objectives: Cystic fibrosis (CF) is a progressive disease which influences lung function and survival. The optimal nutritional status and recent body compositions, remarkably higher fat free mass (FFM) than the fat mass (FM) are important factors interrelated with better outcomes. The aim of our study was to evaluate nutritional status,body composition and pulmonary function in CF pediatric patients and establish if exist any gender differences and aged related trends in the assessed parameters. Methods: In 2018, patients between 8 and 18 years in stable condition, during the annual assessment, with FEV1% > 40% were included to the study. BMI z-scores were calculated and compared to the national reference data. Body composition by bio-impedance analysis and pulmonary function (PF) were determined. Fat free mass index (FFMI) and the fat mass index (FMI) were calculated according to the following formula: FFMI = FFM/height2, FMI = FM/height2, respectively. Results: One hundred nine CF subjects (53% male), with mean age 12,99 ± 3,2 years were assessed. 50% of them were homozygotes F508del and 90% with pancreatic insufficiency. Normal BMI z-score was observed in 75%. Mean FEV% was 91,85 ± 15,43; mild PF had 90% of the group. No differences in BMI z-score, FEV%, FVC%, PEF% between girls and boys were noticed, however PF were significantly worsening with age (F = 2,221, p =,00043), particularly between 8 and 9 years old. BMI z-score fluctuating with age with higher values between 8 and 9 years and lower between 17 and 18 years. FFMI increased with age (p < 0001) in all patients and was higher in boys (p < 0,001), but FMI was greater in girls (p < 0,001). Conclusion: Most of the patients characterized a good nutritional and pulmonary status. Body composition was changing with age. Differences in the body content between girls and boys were observed, not like in BMI z-score values. Pulmonary function does not depend on gender, nevertheless decreases early in the middle childhood.},
author = {Mielus, M and Lurzynska, M and Barloga, P and Borawska-Kowalczyk, U and Oltarzewski, M and Sands, D},
doi = {10.1016/S1569-1993(19)30609-5},
issn = {1873-5010},
journal = {Journal of Cystic Fibrosis},
keywords = {adolescent,adult,body composition,body mass,child,childhood,conference abstract,controlled study,cystic fibrosis,fat free mass,female,forced expiratory volume,forced vital capacity,girl,height,homozygote,human,impedance,lung function,major clinical study,male,nutritional status,pancreatic insufficiency,pediatric patient,school child,sex difference},
language = {English},
pages = {S147},
title = {{Nutritional status, body composition and pulmonary function in cystic fibrosis children and adolescents - age and gender relationship}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2001976146&from=export http://dx.doi.org/10.1016/S1569-1993(19)30609-5},
volume = {18},
year = {2019}
}
@article{Toth2016,
abstract = {Introduction: cystic fibrosis (cf) is the most common autosomal recessive metabolic disease. all symptoms associated with the gastrointestinal system have adverse effects on the nutritional status, which, in turn, is closely related to the prognosis for the patient. insufficient energy intake results in malnutrition, leading to worse respiratory function, and the early onset of comorbidities; it is accompanied with worse quality of life and a shortened lifespan. Aim: the aim of our research was to assess the quality of life and nutritional status of adult patients with cf in Hungary and to determine the correlations between nutritional status, lung function and the quality of life. in Hungary, we are the first to study the relationship between nutritional status and the quality of life in adult cf patients. Material and methods: 57 patients with cf from Hungary of both sexes were studied, all of them aged over 18. an assessment of the quality of life with a questionnaire was conducted among them. their nutritional status was measured with the bioelectrical impedance analysis. the most recently measured feV1 (forced expiratory volume in 1 second) values for the patients were analyzed. in order to assess the quality of life, the validated hungarian translation of the disease-specific cystic fibrosis Questionnaire-Revised (CFQ-R) - teen/adult version for ages 14 through adulthood - was used. Results: the nutritional status is worse in female patients than in male. undergoing lung transplantation did not have any impact on the nutritional status. however, we found significant differences in the feV1 values between the patients who underwent transplantation and those who did not. no strong correlation was found between any parameters of the nutritional status, feV1 and the domains of the quality of life. in the mean values of the quality of life domains, the highest score appeared in the eating domain, while the lowest one was found in the weight domain. we did not find any significant differences between the quality of life of male and female patients, except for the respiratory symptoms domain, where women's scores were lower. however, this result was not confirmed by feV1 data. transplant patients' quality of life was higher than non-transplant ones in almost all domains, with 6 domains where statistically significant differences were found. the transplant cf patients, however, had lower scores in the weight and body image domains. cf patients with lower bMi had lower quality of life than their peers. Muscle mass percentage did not differ between groups. feV1 showed a statistically significant correlation with six of the cfQ-R domains. Conclusions: our research is the first disease-specific assessment of the quality of life among hungarian adult patients with cf. in Hungary, we are the first to study the relationship between nutritional status and the quality of life in adult cf patients. we conclude that early dietetical interventions should be implemented to improve patients' condition and the quality of life.},
address = {T. T{\'{o}}th, Department of Dietetics and Nutrition Sciences, Faculty of Health Sciences, Semmelweis University, 17 Vas ut., Budapest, Hungary},
author = {T{\'{o}}th, T and M{\'{a}}k, E and Gall{\'{o}}, N and Szabolcs, I},
doi = {10.5604/14270994.1206757},
issn = {1731-2507},
journal = {New Medicine},
keywords = {Cystic Fibrosis Questionnaire-Revised,Hungary,adult,article,body fat,body image,body mass,body weight,cystic fibrosis,eating,emotion,female,forced expiratory volume,human,impedance,lung function,lung transplantation,major clinical study,male,muscle mass,nutritional status,quality of life,questionnaire,sex difference,social interaction},
language = {English},
number = {2},
pages = {53--58},
title = {{Research on the quality of life of adult patients with cystic fibrosis in Hungary}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L611664335&from=export http://dx.doi.org/10.5604/14270994.1206757},
volume = {20},
year = {2016}
}
@article{Paglialonga2013a,
abstract = {Background: Pediatric patients on chronic hemodialysis (HD) are at high risk of inactivity and poor physical fitness. The aim of this study was to assess the main determinants of exercise capacity in a cohort of children and young adults on chronic HD. Methods: Twelve patients on chronic HD (median age 15.6 years; range 9.1-24.2), underwent a 6-minute walking test (WT), spirometry, a one-minute chair stand test (CST), and the measurement of lower extremity strength (LES). Demographic data, anthropometric data (dry weight, height, BMI, skinfold thickness, all expressed as standard deviation scores, SDS), biochemistry (serum albumin, hemoglobin, creatinine, C-reactive protein, bicarbonate), bioimpedance analysis, HD adequacy indices (spKt/V and eKt/V), left ventricular mass index, and medications were also recorded. Results: There was a significant correlation between the distance covered during the WT (median 552 m, range 186-670), forced vital capacity (FVC 87.8% of predicted, range 49.7-136), forced expiratory volume in one second (86.7%, range 54.7-126.7) and peak expiratory flow (75.5%, 49.7-105.1). All of these indices positively correlated with the weight SDS (r 0.69-0.85), and pre-HD serum creatinine (0.57-0.77) and serum albumin levels (0.60-0.77), and negatively correlated with weekly ery thropoietin dose per kg body weight (from -0.64 to -0.83), with p values ranging from <0.05 to <0.0005. LES (median 11.5 kg, range 3-15) positively correlated with the number of stands at the CST (median 33, range 18-47; r 0.73, p < 0.05) and serum albumin levels (r 0.83, p < 0.01). Distance at the WT, FVC, LES and the number of stands at CST all negatively correlated with C-reactive protein levels (r from -0.81 to -0.67, p < 0.05). Conclusion: In conclusion, our findings show that the main factors influencing the exercise capacity of children and young adults on chronic HD are protein-energy wasting and chronic inflammation.},
address = {F. Paglialonga, Pediatric Nephrology and Dialysis Unit, Fondazione IRCCS Ca' Granda Ospedale Maggiore Policlinico, Milan, Italy},
author = {Paglialonga, F and Lopopolo, A and Scarfia, R V and Brivio, A and Galli, M A and Grassi, M R and Salera, S and Edefonti, A},
doi = {10.1111/hdi.12006},
issn = {1492-7535},
journal = {Hemodialysis International},
keywords = {C reactive protein,adult,bicarbonate,body weight,child,chronic inflammation,creatinine,creatinine blood level,dialysis,drug therapy,dry weight,erythromycin ethylsuccinate,exercise,fitness,forced expiratory volume,forced vital capacity,heart left ventricle mass,height,hemodialysis,hemoglobin,human,leg,patient,peak expiratory flow,protein,risk,serum albumin,skinfold thickness,spirometry,statistical significance,walking,weight},
language = {English},
number = {1},
pages = {163},
title = {{Determinants of exercise capacity in pediatric patients on chronic hemodialysis}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L71022204&from=export http://dx.doi.org/10.1111/hdi.12006},
volume = {17},
