journal = {Medicine (United States)},
keywords = {adult,age,article,body mass,breast cancer-related lymphedema,cancer size,cancer survival,controlled study,disease severity,extracellular fluid,feasibility study,female,human,impedance,major clinical study,middle aged,total body water},
language = {English},
