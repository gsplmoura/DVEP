pmid = {32419425},
publisher = {Turkish Journal of Pediatrics},
title = {{Wernicke's encephalopathy manifesting with diplopia after ileojejunostomy: Report of a pediatric case with hirschsprung disease}},
volume = {62},
year = {2020}
