journal = {European Journal of Clinical Nutrition},
keywords = {APACHE,APACHE IV,adult,age,article,bioelectrical impedance analysis derived phase ang,body mass,female,gender,hospital admission,human,intensive care,major clinical study,male,middle aged,mortality,mortality rate,observational study,patient assessment,prediction,prospective study,risk assessment},
language = {English},
