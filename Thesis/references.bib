@phdthesis{Alencar2019,
abstract = {O diabetes mellitus tipo 2 est{\'{a}} intimamente ligado a forma{\c{c}}{\~{a}}o de esp{\'{e}}cies reativas de oxig{\^{e}}nio, as quais aumentam os produtos de glica{\c{c}}{\~{a}}o avan{\c{c}}ada e consequentemente a inflama{\c{c}}{\~{a}}o e o desenvolvimento das complica{\c{c}}{\~{o}}es associadas a doen{\c{c}}a. Nesse contexto, destaca-se o papel do microbioma intestinal, visto que esta pode ser capaz de modificar fatores ligados ao sistema imune e sua modula{\c{c}}{\~{a}}o pode ser uma boa alternativa para melhora do quadro inflamat{\'{o}}rio e complica{\c{c}}{\~{o}}es da doen{\c{c}}a. A castanha-do-brasil, fonte de sel{\^{e}}nio, al{\'{e}}m de outros nutrientes, poderia atuar no sistema de defesa antioxidante e anti-inflamat{\'{o}}rio, al{\'{e}}m de influenciar a microbiota intestinal, resultando benef{\'{i}}cios aos pacientes. Portanto, este estudo teve como objetivo avaliar estes efeitos da suplementa{\c{c}}{\~{a}}o de indiv{\'{i}}duos com diabetes mellitus tipo 2, com uma n{\'{o}}z de castanha-do-brasil (Bertholletia excelsa H.B.K.) por dia, durante 60 dias. Para tanto, foi delineado um ensaio cl{\'{i}}nico, longitudinal, com 29 pacientes volunt{\'{a}}rios com diabetes mellitus tipo 2, atendidos no Ambulat{\'{o}}rio de Endocrinologia do Hospital das Cl{\'{i}}nicas da Faculdade de Medicina da Universidade de S{\~{a}}o Paulo. Os participantes foram avaliados antes e ap{\'{o}}s a interven{\c{c}}{\~{a}}o, e realizaram uma coleta de fezes (caracteriza{\c{c}}{\~{a}}o da microbiota intestinal e concentra{\c{c}}{\~{a}}o de sel{\^{e}}nio) e uma coleta de sangue (an{\'{a}}lises de status de sel{\^{e}}nio, marcadores inflamat{\'{o}}rios, glic{\^{e}}micos e lip{\'{i}}dicos, e da permeabilidade intestinal), a avalia{\c{c}}{\~{a}}o da ingest{\~{a}}o alimentar foi por meio de Registro Alimentar de 3 dias Inicialmente, determinou-se a composi{\c{c}}{\~{a}}o centesimal e a concentra{\c{c}}{\~{a}}o de sel{\^{e}}nio da castanha-do-brasil utilizada. A an{\'{a}}lise bioinform{\'{a}}tica foi conduzida usando o Quantitative Insight into Microbial Ecology (QIIME, vers{\~{a}}o 1.9.0). A an{\'{a}}lise estat{\'{i}}stica foi realizada com os softwares Statistical Package for the Social Sciences, vers{\~{a}}o 23.0, e R vers{\~{a}}o 1.1.463. A concentra{\c{c}}{\~{a}}o de sel{\^{e}}nio na castanha in natura foi de 79,8µg/g. A idade m{\'{e}}dia dos participantes foi 54,9±3,6 anos. A ingest{\~{a}}o de castanha-do-brasil foi capaz de melhorar significativamente os par{\^{a}}metros: concentra{\c{c}}{\~{o}}es de sel{\^{e}}nio no plasma (&#916; 95,82, p<0,001), nos eritr{\'{o}}citos (&#916; 163,16, p<0,001) e nas fezes (&#916; 166,31 p<0,001), assim como na selenoprote{\'{i}}na P (&#916; 5,36 p<0,026) e atividade da enzima glutationa peroxidase (&#916; 30,61 p<0,040) e redu{\c{c}}{\~{a}}o da hemoglobina glicada (&#916; -0,8, p=0,001). No entanto, n{\~{a}}o houve altera{\c{c}}{\~{a}}o do perfil lip{\'{i}}dico, inflamat{\'{o}}rio, ingest{\~{a}}o alimentar e marcadores antropom{\'{e}}tricos. Apesar de n{\~{a}}o verificarmos mudan{\c{c}}a na composi{\c{c}}{\~{a}}o global da microbiota intestinal, a diversidade interpessoal ({\ss} diversidade) variou conforme a concentra{\c{c}}{\~{a}}o de selenoprote{\'{i}}na P (p=0,03) e porcentagem de hemoglobina glicada (p=0,04). A suplementa{\c{c}}{\~{a}}o com castanha-do-brasil melhorou o perfil glic{\^{e}}mico e o status de sel{\^{e}}nio. Apesar de n{\~{a}}o ser observada influ{\^{e}}ncia na composi{\c{c}}{\~{a}}o global da microbiota, a segrega{\c{c}}{\~{a}}o observada indica certa resist{\^{e}}ncia da microbiota frente a interven{\c{c}}{\~{a}}o com a castanha},
author = {de Alencar, Luciane Luca},
file = {:Users/gustavosplmoura/Library/Mobile Documents/com$\sim$apple$\sim$CloudDocs/Medicina/2024_Mendeley/Alencar. 2019. A suplementa{\c{c}}{\~{a}}o de pacientes com diabetes mellitus tipo 2 com castanha-do-brasil, pode alterar o estado nutricional.pdf:pdf},
keywords = {Bertholletia/adverse effects,Bertholletia/efeitos adversos,Diabetes Mellitus,Diabetes Mellitus Tipo 2/tratamento farmacol{\'{o}}gico,Dietary Supplements/adverse effects,Female,Feminino,Gastrointestinal Microbiome,Glycemic Index,Humanos,Humans,Inflama{\c{c}}{\~{a}}o/classifica{\c{c}}{\~{a}}o,Inflammation/classification,Intestines,Male,Masculino,Microbioma Gastrointestinal,Middle Aged,Pessoa de Meia-Idade,Selenium/agonists,Sel{\^{e}}nio/agonistas,Suplementos Nutricionais/efeitos adversos,Type 2/drug therapy,{\'{I}}ndice Glic{\^{e}}mico},
pages = {129},
title = {{A suplementa{\c{c}}{\~{a}}o de pacientes com diabetes mellitus tipo 2 com castanha-do-brasil, pode alterar o estado nutricional relativo ao sel{\^{e}}nio, o grau de inflama{\c{c}}{\~{a}}o e a microbiota intestinal?}},
url = {http://www.teses.usp.br/teses/disponiveis/9/9132/tde-26082019-093652/pt-br.php%0Ahttp://fi-admin.bvsalud.org/document/view/va2en},
year = {2019}
}
