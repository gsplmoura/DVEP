title = {{Phase angle in patients with colorectal cancer}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2008505729&from=export http://dx.doi.org/10.1016/j.clnesp.2020.09.427},
volume = {40},
year = {2020}
}
@article{Morin2020,
abstract = {Bio-impedance analysis (BIA) is a common technique used to estimate body composition. BIA measures the electrical impedance in water contained in a subject's body and uses this measurement to predict body composition. Phase angle is one of the components of body composition and is a major indicator of body fat percentage. Although BIA is less accurate when predicting body composition compared to dual-energy X-ray absorptiometry (DXA) or bod pods, it is cheaper, more portable, and does not require a licensed operator. For those reasons, athletes commonly use BIA to estimate body fat composition. Current BIA techniques are shown to be inaccurate for very athletic individuals [1] since it is calibrated based on the general population. Previous studies have calculated phase angle distribution in elite athletes using DXA data, but none have examined BIA. Our research uses empirical Bayesian analysis with BIA data to create reference percentiles for phase angle by sport and gender among elite European athletes. .},
author = {Morin, T V and Baller, D P and Clark, N J and Watts, K L},
doi = {10.1096/fasebj.2020.34.s1.09458},
issn = {1530-6860},
journal = {FASEB Journal},
keywords = {Bayes theorem,adult,athlete,body composition,body fat,conference abstract,controlled study,dual energy X ray absorptiometry,elite athlete,female,gender,human,impedance,male},
language = {English},
number = {SUPPL 1},
title = {{Estimating Phase Angle Percentiles in Elite Athletes}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L637861454&from=export http://dx.doi.org/10.1096/fasebj.2020.34.s1.09458},
volume = {34},
year = {2020}
}
@article{Harter2017,
abstract = {Introduction: Phase angle (PA) has been considered as a prognostic factor for several clinical conditions, probably because of its relationship to fat-free mass and function. More recently, decreased muscle mass and functional assessment were used to define sarcopenia. The purpose of this work was to identify the relationship of PA, sarcopenia and its components in cancer patients. Methods: Fifty-nine cancer patients hospitalized for elective surgery were studied. Bioelectrical impedance analysis (BIA Quantum II, RJL Systems{\textregistered}) was performed in all patients and skeletal muscle index, using Janssen equation, and PA and Standardized Phase Angle (SPA) were estimated, using reference values from this population. The handgrip strength was obtained using Jamar{\textregistered} hydraulic dynamometer, and gait speed was assessed by using 4-m test. The diagnosis of sarcopenia was performed according the European Working Group on Sarcopenia in Older People. Malnutrition was assessed by Patient-generated Subjective Global Assessment. The outcomes evaluated were postoperative complications (Clavien-Dindo classification) and length of stay (LOS). Results: Sarcopenia was present in 17% of the patients and malnutrition in 30.5%. SPA was significantly lower in malnourished (-0.20 vs. 0.62 p = 0.028) and sarcopenic patients (-0.39 vs. 0.59 p = 0.010). Patients with a lower handgrip strength (-0.31 vs. 0.69 p = <0.001) and slower gait speed (-0.30 vs. 0.60 p = 0.016) also showed a lower SPA. SPA and PA from non-sarcopenic patients with impaired functions also showed a lower SPA and PA than those with normal function. Patients with serious postoperative complications (-0.71 vs. 0.41 p = 0.007) and longer LOS (-0.16 vs. 0.64 p = 0.030) presented lower SPA. The same results were found when PA (adjusted for sex and age) was used. Conclusions: PA was decreased in all the sarcopenia components, and it has been associated with impaired nutritional and functional status in this sample.},
author = {H{\"{a}}rter, J and Orlandi, S P and Gonzalez, M C},
doi = {10.1002/jcsm.12255},
issn = {2190-6009},
journal = {Journal of Cachexia, Sarcopenia and Muscle},
keywords = {adult,cancer patient,classification,clinical assessment,controlled study,diagnosis,dynamometer,elective surgery,female,functional assessment,functional status,gait,grip strength,human,impedance,length of stay,major clinical study,male,malnutrition,muscle mass,postoperative complication,reference value,sarcopenia,skeletal muscle,velocity},
language = {English},
number = {6},
pages = {1023},
title = {{Phase angle and sarcopenia: Are they related?}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L619558234&from=export http://dx.doi.org/10.1002/jcsm.12255},
volume = {8},
year = {2017}
}
@article{Stobaus2012,
abstract = {Phase angle (PhA), a parameter of bioelectrical impedance analysis, is a well-known predictor of morbidity and mortality in various diseases. The causes of decreased PhA are, however, not yet completely understood. We therefore investigated determinants of PhA in 777 hospitalised patients in a retrospective analysis. PhA was assessed by bioelectrical impedance analysis at 50 KHz. Subjective global assessment (SGA) was used to evaluate nutritional status. Age, sex, BMI as well as nutritional status (SGA), benign or malignant disease and C-reactive protein (CRP) were investigated as potential determinants of PhA and standardised PhA (SPhA) = (observed PhA - mean PhA of reference values)/standard deviation of reference values in a general linear model regression analysis. Next to age (estimated effect size, 46{\textperiodcentered}6%; P<0{\textperiodcentered}0001), malnutrition (39{\textperiodcentered}1%; P<0{\textperiodcentered}0001) emerged as a major PhA determinant in our study population. Moreover, sex (6{\textperiodcentered}4%; P<0{\textperiodcentered}0001), CRP (4{\textperiodcentered}4%; P<0{\textperiodcentered}0001) and BMI (3{\textperiodcentered}5%; P < 0{\textperiodcentered}0001) exhibited a significant influence on PhA, whereas malignant disease showed no significant effect in this model. The only significant determinants of SPhA were malnutrition (85{\textperiodcentered}4%; P<0{\textperiodcentered}0001) and inflammation (9{\textperiodcentered}6 %; P<0{\textperiodcentered}0001). In conclusion, next to the established predictors, malnutrition and inflammation have a strong impact on PhA in sick individuals, which partly explains its prognostic power. When investigating the SPhA, only malnutrition and inflammation were found to be significant predictors, as a result of which the SPhA is considered a more suitable indicator of nutritional and health status.},
author = {Stob{\"{a}}us, N and Pirlich, M and Valentini, L and Schulzke, J D and Norman, K},
doi = {10.1017/S0007114511004028},
issn = {1475-2662},
journal = {The British journal of nutrition},
keywords = {adult,aged,article,body composition,cross-sectional study,diagnostic agent,female,human,impedance,inflammation,male,malnutrition,middle aged,nutritional status,prognosis,retrospective study,statistical model},
language = {English},
number = {8},
pages = {1217--1220},
title = {{Determinants of bioelectrical phase angle in disease.}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L365235209&from=export http://dx.doi.org/10.1017/S0007114511004028},
volume = {107},
year = {2012}
}
