@article{King2018,
abstract = {Objective: Lumacaftor-Ivacaftor is associated with reduced hospitalisations in cystic fibrosis (CF), but its impact on body composition is not yet widely studied. we aimed to evaluate changes in body composition in adults with CF with FEV1< 40% predicted over 12 months of Lumacaftor-Ivacaftor treatment. Methods: Datawere analysed for 20 CF adults (10 male, mean ± SD baseline age 33.5 ± 8.8 years, mean ± SD FEV1 33.6 ± 6.5%predicted, mean ± SD BMI 19.9 ± 2.2 kg/m2), who received Lumacaftor-Ivacaftor for 12 months, and had body composition measured (multifrequency bioelectrical impedance analysis, SECA, Germany) at 0, 6 and 12 months. Changes($\Delta$) inweight, fatfree mass (FFM) and fat mass (FM) were determined. Paired t-tests and Pearson's correlations were undertaken. Results: At six months, mean ± SD weight and fat mass significantly increased ($\Delta$Weight0-6 mth: 2.3 ± 3.9 kg (p = 0.02); $\Delta$FM0-6 mth: 2.1 ± 2.7 kg (p = 0.003)), with no further increase by 12 months ($\Delta$Weight6-12 mth: 0.3 ± 3.1 kg (p = 0.66); $\Delta$FM6-12 mth: 0.3 ± 2.7 kg (p = 0.27)). No changes were seen in mean FFM ($\Delta$FFM0-12 mth: 0.4 ± 1.8 kg (p = 0.62). Lower baseline BMI correlated with greater weight gain (r = -0.50, p = 0.02) and FM (r = -0.46, p = 0.04). No-one became overweight; 50% gained >5% weight. No-one lost >5% FFM whilst 20% gained >5% FFM. Lower baseline BMI correlated with greater weight and FM gains at 12 months (r = -0.50, p = 0.02 and r = -0.46, p = 0.04). $\Delta$FFM0-12 mth correlated with baseline FEV1% (r = 0.46, p = 0.04) and $\Delta$FEV1 %0-12 mth (r = 0.45, p = 0.047). Conclusions: Weight and fat mass gains seen after six months on Lumacaftor-Ivacaftor plateau by 12 months. Underweight patients are more likely to achieve body composition gains. Preservation of FFM contrasts with earlier work showing declining FFM in association with severe lung disease. Mechanisms underlying body composition changes require further investigation, including mediation via improved appetite, salt/hydration, exercise or via amelioration of catabolism related to reduced exacerbations.},
address = {S.J. King, Alfred Hospital, Nutrition Department, Melbourne, Australia},
author = {King, S J and Williams, E and Allen-Graham, J and Finlayson, F and Button, B M and Clark, D and Kotsimbos, T and Keating, D and Tierney, A C and Wilson, J W},
issn = {1873-5010},
journal = {Journal of Cystic Fibrosis},
keywords = {Germany,adult,appetite,body composition,body mass,body weight gain,catabolism,clinical article,conference abstract,controlled study,cystic fibrosis,disease exacerbation,exercise,fat mass,forced expiratory volume,human,hydration,impedance,ivacaftor plus lumacaftor,lung disease,male,obesity,underweight},
language = {English},
pages = {S113},
title = {{Body composition changes in the first year of treatment with lumacaftor-ivacaftor in adults with severe cystic fibrosis lung disease}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L622930847&from=export},
volume = {17},
year = {2018}
}
@article{Paglialonga2013,
abstract = {Objective: Pediatric patients on chronic hemodialysis (HD) are at high risk of inactivity and poor physical fitness. The aim of this study was to assess the main correlates of exercise capacity in a cohort of children and young adults on chronic HD. Methods: Twelve patients on chronic HD (median age 15.6years; range 9.1-24.2) underwent a 6-minute walking test (WT), spirometry, a 1-minute chair stand test, and the measurement of lower extremity strength. Demographic data, anthropometry (dry weight, height, body mass index, and skinfold thickness, all expressed as standard deviation scores [SDS]), biochemistry (serum albumin, hemoglobin, creatinine, C-reactive protein, bicarbonate), bioimpedance analysis, HD adequacy indices (spKt/V and eKt/V), left ventricular mass index, and medications were also recorded. Results: There was a significant correlation among the distance covered during the WT (median 552m, range 186-670), forced vital capacity (87.8% of predicted, range 49.7-136), forced expiratory volume in 1 second (86.7%, range 54.7-126.7), and peak expiratory flow (75.5%, 49.7-105.1). All of these indices positively correlated with the weight SDS (r 0.69-0.85), pre-HD serum creatinine (0.57-0.77), and serum albumin (0.60-0.77) and negatively correlated with weekly erythropoietin dose per kilogram of body weight (from -0.64 to -0.83), with P values ranging from < .05 to < .0005. Lower extremity strength (median 11.5kg, range 3-15) positively correlated with the number of stands at the chair stand test (median 33, range 18-47; r 0.73, P < .05) and serum albumin (r 0.83, P < .01). Distance at the WT, forced vital capacity, lower extremity strength, and the number of stands at the chair stand test all negatively correlated with C-reactive protein levels (r from -0.81 to -0.67, P < .05). Conclusion: Our findings show that protein-energy wasting and chronic inflammation are strongly correlated with the exercise capacity of children and young adults on chronic HD. {\textcopyright} 2013 National Kidney Foundation, Inc.},
address = {F. Paglialonga, Pediatric Nephrology and Dialysis Unit, Fondazione IRCCS Ca' Granda Ospedale Maggiore Policlinico, Via Commenda 9, 20122 Milano, Italy},
author = {Paglialonga, F and Lopopolo, A and Scarfia, R V and Galli, M A and Consolo, S and Brivio, A and Grassi, M R and Salera, S and Edefonti, A},
doi = {10.1053/j.jrn.2013.04.006},
issn = {1051-2276},
journal = {Journal of Renal Nutrition},
keywords = {C reactive protein,adolescent,article,bicarbonate,blood,body mass,child,chronic kidney failure,creatinine,cross-sectional study,erythropoietin,exercise test,exercise tolerance,female,hemoglobin,human,impedance,male,metabolism,pathophysiology,renal replacement therapy,serum albumin,skinfold thickness,walking,young adult},
