author = {Mancini, B and Howrylak, J and Bonavia, A},
doi = {10.1097/01.ccm.0000910536.47592.6d},
issn = {1530-0293},
journal = {Critical Care Medicine},
keywords = {APACHE,Charlson Comorbidity Index,ECOG Performance Status,adult,artificial ventilation,conference abstract,controlled study,critical illness,critically ill patient,female,follow up,human,inflammation,major clinical study,male,medical intensive care unit,middle aged,mortality,nursing,observational study,outcome assessment,persistent inflammation, immunosuppression and cat,prospective study,rehabilitation,sepsis,surgery,survivor,tissue injury,university hospital,ventilated patient,ventilator},
language = {English},
