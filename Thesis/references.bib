issn = {1532-1983},
journal = {Clinical Nutrition},
keywords = {Roux-en-Y gastric bypass,age,article,bariatric surgery,bioelectrical impedance,body mass,body weight,caloric restriction,correlation analysis,disease severity,exercise,fat mass,human,impedance,laparoscopic sleeve gastrectomy,muscle mass,muscle strength,obesity,sex difference,systematic review},
language = {English},
