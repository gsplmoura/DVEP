}
@article{DiVincenzo2021a,
abstract = {Introduction: Bioimpedance analysis (BIA)-derived phase angle (PhA) describes changes in the ratio body cell mass/fat-free mass (and therefore in muscle quality, as well). Obesity is associated with alterations in muscle structure, e.g. myosteatosis, and metabolic function. The aim of this systematic review was to evaluate whether and to which extent PhA varies in adult patients with obesity. We focused on: a) changes in PhA due to obesity; b) changes in PhA after bariatric interventions or training programs in patient with obesity. Methods: A systematic literature search until February 2021 using PubMed, Embase, Scopus, and Web of Science was performed according to PRISMA criteria. Selection criteria included studies on adult patients with obesity without comorbidities other than metabolic diseases. Results: A total of 278 articles were identified through the initial search. After removing duplicates and excluding studies that did not fulfil the inclusion criteria by screening titles and abstracts, the full text of the remaining 80 potentially relevant studies was examined. Twenty-one papers (11 cross-sectional and 10 longitudinal) met the inclusion criteria. Few studies have shown that PhA is lower in patients with obesity than in controls. Only one paper thoroughly evaluated PhA in different age groups, showing a decrease after the fourth decade of life in both genders. Four out of five studies reported consistently greater mean values in men than in women. In two studies PhA was lower in patients with obesity grade-III than in grade I-II. Additionally, patients with low PhA had higher BMI and PhA was significantly and inversely correlated with fat mass. Longitudinal studied showed that PhA markedly decreased after different bariatric surgery and slightly increased after training programs. Finally, no reference values (i.e. percentiles) are available for PhA in male or in female patients with obesity. Conclusion: So far, a relatively low number of studies have evaluated BIA-derived PhA in patients with obesity with partial, sometimes contradictory, and preliminary results. PhA may be useful to evaluate muscle quality in patients with obesity but further studies are needed to associate this variable with change in muscle structure and strength, as well as metabolic function.},
address = {O. Di Vincenzo, Department of Clinical Medicine and Surgery, Federico II University of Naples, Italy},
author = {{Di Vincenzo}, O and Marra, M and Cioffi, I and Sacco, A and Sammarco, R and Pasanisi, F and Scalfi, L},
doi = {10.1159/000516203},
issn = {1662-4033},
journal = {Obesity Facts},
keywords = {Embase,Medline,Scopus,Web of Science,adult,bariatric surgery,body mass,clinical evaluation,comorbidity,conference abstract,fat mass,female,gender,groups by age,human,impedance,male,metabolic disorder,muscle,obesity,preliminary data,reference value,systematic review,training},
language = {English},
number = {SUPPL 1},
pages = {161--162},
title = {{Bioelectrical impedance analysis (BIA)-derived phase angle in adult patients with obesity: A systematic review}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L635120605&from=export http://dx.doi.org/10.1159/000516203},
volume = {14},
year = {2021}
}
@article{Barbosa-Silva2005,
abstract = {Background: Phase angle is an indicator based on reactance and resistance obtained from bioelectrical impedance analysis (BIA). Although its biological meaning is still not clear, phase angle appears to have an important prognostic role. Objective: The aim of this study was to estimate population averages and SDs of phase angle that can be used as reference values. Design: BIA and other methods used to evaluate body composition, including hydrodensitometry and total body water, were completed in 1967 healthy adults aged 18-94 y. Phase angle was calculated directly from body resistance and reactance, and fat mass (FM) was estimated from the combination of weight, hydrodensitometry, and total body water by using the 3-compartment Siri equation. Phase angle values were compared across categories of sex, age, body mass index (BMI), and percentage FM. Results: Phase angle was significantly (P < 0.001) smaller in women than in men and was lower with greater age (P < 0.001). Phase angle increased with an increase in BMI and was significantly inversely associated with percentage fat in men. Phase angle was significantly predicted from sex, age, BMI, and percentage FM in multiple regression models. Conclusions: Phase angle differs across categories of sex, age, BMI, and percentage fat. These reference values can serve as a basis for phase angle evaluations in the clinical setting. {\textcopyright} 2005 American Society for Clinical.},
