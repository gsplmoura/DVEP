}
@article{Hollander2018,
abstract = {Objectives: Bio-electrical Impedance Analysis (BIA) is a widely used method to assess body composition. The present study aimed at testing whether measurements in the non-fasting state deviated from those in the fasting state in Dutch adult CF patients and evaluate the relationship between pulmonary function and Fat Free Mass (FFM) compared to Body Mass Index (BMI). Methods: Data from 47 CF patientswere collected from September 2017 to January 2018. Body composition was measured by Body stat{\textregistered} 500 and pulmonary function by forced expiratory volume in 1 second (FEV1) percentage predicted. Differences between the fasting and non-fasting state were analysed with paired sample t-tests. Correlations were tested with Pearson's r for parametric data. Results: 33 adult CF patients (16 men) had both fasting and non-fasting BIA data. Fasting mean FFM was 51.72 kg, which was only 0.1 kg (0.2%) lower than the non-fasting mean FFM of 51.82 kg (p = 0.508). Fasting mean Fat Mass (FM)was 16.58 kg, whichwas 0.1 kg (0.5%) more than the non-fasting mean FM of 16.47 kg (p = 0.483). Data of 47 non-fasting CF patients (21 men, mean BMI 22.3 kg/m2, IQR [20.3-23.6], mean FFM 50.0 ± 10.27, mean FEV1% 64.61 ± 22.83) showed that both FFM and BMI were positively and significantly correlated with pulmonary function (r = 0.444, p = 0.002, 95% CI 0.18 to 0.65 and r = 0.359, p = 0.017, 95% CI 0.08 to 0.59, respectively). Conclusions: These results suggest that the differences in FFM and FM between the fasting and non-fasting state are not clinically relevant. In addition, FFM is positively associated with pulmonary function with little difference from BMI possibly due to the adequate nutritional status of the study population. Including more patients with a variety in nutritional status is necessary to be able to confirm these findings.},
address = {F.M. Hollander, University Medical Center Utrecht, Internal Medicine and Dermatology, Department of Dietetics, Utrecht, Netherlands},
author = {Hollander, F M and Roelofs, J and {Van Der Boom}, S T and Burghard, M and {De Roos}, N M and Heijerman, H G M},
issn = {1873-5010},
journal = {Journal of Cystic Fibrosis},
keywords = {Dutchman,adult,body composition,body mass,body weight,clinical article,conference abstract,controlled study,cystic fibrosis,diet restriction,fat free mass,female,forced expiratory volume,human,impedance,lung function,male,nutritional status},
language = {English},
pages = {S112},
title = {{Bio-electrical impedance analysis and relationship with pulmonary function in cystic fibrosis patients}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L622930824&from=export},
volume = {17},
year = {2018}
}
@article{Alicandro2015,
abstract = {Background: The accuracy of body composition estimates based on skinfold thickness measurements and bioelectrical impedance analysis (BIA) is not yet adequately explored in cystic fibrosis (CF). Using DXA as reference method we verified the accuracy of these techniques and identified predictors of body composition specific for CF. Methods: One hundred forty-two CF patients (age range: 8-31. years) underwent a DXA scan. Body fat percentage (BF%) was estimated from skinfolds, while fat free mass (FFM) from single-frequency 50. kHz BIA. Results: Bland-Altman analysis showed poor intra-individual agreement between body composition data provided by DXA and BF% estimated from skinfolds or FFM estimated from BIA. The skinfolds of the upper arm were better predictors of BF% than BMI, while compared to other BIA measurements the best predictor of FFM was the R-index (Height2/Resistance). Conclusions: Due to poor accuracy at individual level, the estimates of body composition obtained from these techniques cannot be part of the standard nutritional assessment of CF patients until reliable CF-specific equations will become available. BMI has limited value in predicting body fatness in CF patients and should be used in combination with other predictors. Skinfolds of the upper arm and R-index are strongly related to BF% and FFM and should be tested in a large CF population to develop specific predictive equations.},
address = {C. Colombo, Cystic Fibrosis Center, Fondazione IRCCS Ca' Granda, Ospedale Maggiore Policlinico, University of Milan, Via Commenda 9, Milan, Italy},
author = {Alicandro, G and Battezzati, A and Bianchi, M L and Loi, S and Speziali, C and Bisogno, A and Colombo, C},
doi = {10.1016/j.jcf.2015.07.011},
issn = {1873-5010},
journal = {Journal of Cystic Fibrosis},
keywords = {adolescent,adult,article,bioelectrical impedance analysis,body composition,body fat,child,cystic fibrosis,dual energy X ray absorptiometry,fat free mass,female,human,impedance,major clinical study,male,physical phenomena,school child,skinfold,skinfold thickness},
