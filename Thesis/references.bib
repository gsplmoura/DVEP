journal = {American Journal of Respiratory and Critical Care Medicine},
keywords = {American,Student t test,adolescent,autosomal recessive inheritance,body composition,body weight gain,child,classification,cystic fibrosis,diet,fat mass,food,gastrointestinal disease,hospital discharge,hospitalization,human,impedance,longitudinal study,lung disease,lung function,male,nutrient,nutritional deficiency,nutritional status,patient,risk,society,weight},
language = {English},
title = {{Variations in body composition, assesed by bioelectrical impedance, in children and adolescents with cystic fibrosis during hospitalization}},
