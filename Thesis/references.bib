@article{Kim2020,
abstract = {Evidence Based Medicine seeks to use the best available evidence in making clinical decisions. Systematic review of randomized controlled trials (RCTs) and meta-analysis offer best evidence in the hierarchy of evidence. In this context, we conducted systematic reviews of RCTs and meta-analysis to investigate the efficacy and safety of herbal medicine and mahuang in the treatment of obesity. Firstly, we searched Embase, Pubmed, and Cochrane Central Register of Controlled Trials for RCTs of herbal medicine in treatment of obesity up to June 2017. The search strategy followed the PRISMA and 11 intervention arms were analyzed. The methodological quality and publication bias were assessed using the risk of bias tool and funnel plot. Statistical pooling of data based on the random-effects models. We found the overall effect of herbal medicine was higher (Hedges' g =.155) than that of placebo and the effect of Ephedra was the highest (Hedges' g =.262) in subgroup analysis. In terms of adverse events (AEs), herbal medicine improved AST, ALT, and BUN level, indicating herbal medicine for obesity may not have toxic effect on the liver and kidney. Secondly, we searched NLM Medline, Cochrane library, Scopus, Science Direct, Research Information Sharing Service, and Korean Studies Information Service System for RCTs investigating the efficacy and safety of mahuang and ephedrine on the treatment of obesity from 2000 to 2017. We identified 172 studies and finally analyzed 16 RCTs. Mahuang and ephedrine significantly reduced body weight and fat mass with no serious AEs in all studies. The common complaints were insomnia, anxiety, nervousness, palpitations, nausea, vomiting, constipation, headache, and dizziness. In particular, reported symptoms are mostly sympathetic/digestive-related and can be predicted and managed, suggesting appropriate dose of mahuang for healthy adult was safe. These findings provide evidence that herbal medicine and mahuang is effective and safe in the treatment of obesity.},
author = {Kim, S.-Y. and Lim, Y.-W. and Park, Y.-B.},
doi = {10.1016/j.imr.2020.100598},
issn = {2213-4239},
journal = {Integrative Medicine Research},
