author = {Charatsi, A M and Dusser, P and Freund, R and Maruani, G and Rossin, H and Boulier, A and {Le Bourgeois}, M and Chedevergne, F and de Blic, J and Letourneur, A and Casimir, G and Jais, J P and Sermet-Gaudelus, I},
