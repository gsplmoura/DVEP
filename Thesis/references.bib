issn = {2077-0383},
journal = {Journal of Clinical Medicine},
keywords = {amino acid,anabolic agent,catabolism,chronic disease,critical illness,diet supplementation,energy expenditure,human,inflammation,nutrition supplement,omega 3 fatty acid,outcome assessment,persistent inflammation, immunosuppression and cat,phenotype,probiotic agent,protein,randomized controlled trial (topic),review,sepsis,survival},
language = {English},
number = {11},
title = {{Chronic critical illness and pics nutritional strategies}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2007320788&from=export http://dx.doi.org/10.3390/jcm10112294},
