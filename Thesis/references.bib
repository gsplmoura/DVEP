keywords = {Hologic QDR 4500 W,X ray bone densitometer,article,bioelectrical impedance analysis,breathing muscle,child,clinical practice,cystic fibrosis,diaphragm disease,disease severity,dual energy X ray absorptiometry,fat free mass,female,functional residual capacity,human,impedance,lung function,lung function test,major clinical study,male,prospective study,respiratory tract parameters},
