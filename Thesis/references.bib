@article{Barrea2020a,
abstract = {INTRODUCTION: Prader-Willi Syndrome (PWS) represents the most common form of genetic obesity. Studies of body composition in obese adults with PWS are limited. It is generally well-accepted that obese individuals with PWS present with higher fat mass and lower lean mass than their obese counterparts. However, no study focused on body composition evaluated by bioelectrical impedance analysis (BIA), in particular, the phase angle, a direct BIA measurement that represents an indicator of cellular health in chronic inflammatory states, such as obesity, and a marker frequently associated with muscle strength and sarcopenia. The aim of this cross-sectional study was to determine differences in body composition in obese adults with PWS compared to obese adults without PWS. METHODS: In this case-controlled, cross-sectional study, we enrolled 15 obese adults with PWS (six males and nine females, age 28.80 ± 6.76 years) and 15 controls matched for age, sex and body mass index (BMI). Anthropometric measures (weight, height and waist circumference) and bioelectrical variables (Fat Mass, Lean Body Mass, Muscle Mass, Body Cell Mass Index, BCMI; Intra Cellular Water, ICW; Extra Cellular Water, ECW) were evaluated. Phase angle was calculated by BIA phase-sensitive system (50-kHz BIA 101 RJL, Akern Bioresearch, Florence, Italy Akern). Muscle strength was measured by hand grip strength using a grip strength dynamometer (Kern & Sohn GmbH). RESULTS: Compared to matched non-PWS subjects, the obese adults with PWS showed no differences in waist circumference (p = 0.248), fat mass (p = 0.256), lean mass (p = 0.198) and muscle mass (p = 0.809). However, obese adults with PWS were characterized by significant differences in the other bioelectrical variables compared with controls, with a lower phase angle (4.46 ± 0.77 vs. 5.63 ± 0.32; p < 0.001), BCMI (10.12 ± 1.75 vs. 11.13 ± 0.96; p = 0.040), ICW% (45.50 ± 5.18 vs. 52.53 ± 1.61; p < 0.001) and a higher ECW% (54.50 ± 5.18 vs. 47.47 ± 1.61; p < 0.001). In addition, hand grip strength was highly significant lower in PWS group (9.42 ± 5.07 vs. 20.35 ± 4.21 kg; p < 0.001). CONCLUSION: To the best of our knowledge, this is the first study reporting smaller phase angles, as an indirect marker of inflammation and muscle strength, in a group of obese adults with PWS compared to obese adults without PWS. This evidence suggests a role of phase angle as adjunctive marker of the clinical severity of obese adults with PWS, which may go unrecognized using simple measures of body composition, and supports the relevance of a skilled nutritionist in the multidisciplinary management of these patients.},
address = {L. Barrea, Dipartimento di Medicina Clinica e Chirurgia, Unit of Endocrinology, Federico II University Medical School of Naples, Naples, Italy},
author = {Barrea, L and Muscogiuri, G and Pugliese, G and Laudisio, D and Aprano, S and Colao, A and Savastano, S},
doi = {10.1111/obr.13115},
issn = {1467-789X},
journal = {Obesity Reviews},
keywords = {Italy,Prader Willi syndrome,adult,body composition,cell body,clinical article,conference abstract,controlled study,cross-sectional study,dietitian,dynamometer,fat mass,female,grip strength,human,impedance,lean body weight,male,muscle mass,myositis,obese patient,sarcopenia,waist circumference,water},
language = {English},
number = {SUPPL 1},
title = {{Adult subjects with Prader-Willi syndrome show a lower phase angle than matched obese subjects}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L633902103&from=export http://dx.doi.org/10.1111/obr.13115},
volume = {21},
year = {2020}
}
@article{Cancello2022,
abstract = {Phase angle (PhA) is a recently proposed marker of nutritional status in many clinical conditions. Its use in patients with obesity presents different critical concerns due to the higher variability of the two measured parameters (resistance, R, and reactance, Xc) that contribute to the determination of PhA. Controversial is the relation between PhA and BMI that might vary with graded levels of obesity due to the variation in fat and free fat mass. Obesity is frequently associated with metabolic, hepatic, cardiovascular and kidney diseases that introduce variations in PhA values, in relation to multimorbidity and severity degree of these diseases. It is reported that the improvement of clinical condition is associated with a positive change in PhA. Also, the treatment of obesity with weight loss might confirm this effect, but with different responses in relation to the type and duration of the intervention applied. In fact, the effect appears not only related to the percentage of weight loss but also the possible loss of free fat mass and the nutritional, metabolic and structural modifications that might follow each therapeutic approach to decrease body weight. We can conclude that the PhA could be used as marker of health status in patients with obesity supporting an appropriate weight loss intervention to monitor efficacy and fat free mass preservation.},
address = {A. Brunani, Laboratory of Biomechanics, Rehabilitation and Ergonomics, IRCCS, Istituto Auxologico Italiano, Piancavallo Verbania, Italy},
author = {Cancello, R and Brunani, A and Brenna, E and Soranna, D and Bertoli, S and Zambon, A and Lukaski, H C and Capodaglio, P},
doi = {10.1007/s11154-022-09774-1},
file = {:Users/gustavosplmoura/Library/Mobile Documents/com$\sim$apple$\sim$CloudDocs/Medicina/2024_Mendeley/Cancello et al.. 2022. Phase angle (PhA) in overweight and obesity evidence of applicability from diagnosis to weight changes in obesity.pdf:pdf},
issn = {1573-2606},
journal = {Reviews in Endocrine and Metabolic Disorders},
keywords = {adult,body mass,body weight,body weight change,body weight loss,cardiovascular disease,diagnosis,fat free mass,fat mass,health status,human,impedance,kidney disease,liver disease,metabolic disorder,multiple chronic conditions,nutritional status,obesity,review},
language = {English},
title = {{Phase angle (PhA) in overweight and obesity: evidence of applicability from diagnosis to weight changes in obesity treatment}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2020530158&from=export http://dx.doi.org/10.1007/s11154-022-09774-1},
year = {2022}
}
@article{Siddqui2016,
abstract = {Introduction: Phase Angle (PhA) is a ratio of whole body reactance and resistance obtained from Bioelectrical Impedance Analysis (BIA). It indicates cellular health and integrity and is considered as prognostic tool in medical disorders. In spite of prognostic potentials of PhA, it has limited usefulness in clinical practice and in population studies because of non-availability of normal population reference limits for comparison. Moreover, it is influenced by various factors like age, sex, race and body composition (i.e. body fat, muscle mass, visceral fat, body cell mass, total body water, etc). Aim: The aim of this study was to evaluate predictors of phase angle which will be useful in formulation of reference values for Indian population. Materials and Methods: BIA was performed by Tanita Body Composition Analyser on healthy adults aged 17-24 years. The inbuilt software measured the phase angle by the formula: Phase angle (PhA) = Reactance (xc)/Resistance (R)* (180/$\pi$). Phase angle values were compared across categories of age, sex, weight, height, Body Mass Index (BMI), total fat, visceral fat and muscle mass. Results: Mean value of phase angle was found to be 5.65. Phase angle was significantly (p< 0.001) higher in male than in female. Phase angle was significantly predicted from height (p< 0.001), weight (p< 0.002), muscle mass (p< 0.002) and visceral fat (p< 0.02) in multiple regression models. Conclusion: Phase angle differs across anthropometric and body composition categories. Thus height, weight and muscle mass should also be taken into consideration while deriving population specific reference limits of phase angle.},
author = {Siddqui, N I and Khan, S A and Shoeb, M and Bose, S},
doi = {10.7860/JCDR/2016/17229.7976},
issn = {0973-709X},
journal = {Journal of Clinical and Diagnostic Research},
keywords = {adolescent,adult,analytic method,anthropometric parameters,article,bioimpedance analysis,body composition,body weight,controlled study,electric resistance,electrode,female,height,human,human experiment,intra-abdominal fat,male,muscle mass,normal human,phase angle,physical phenomena,population research,sex difference,young adult},
language = {English},
number = {6},
pages = {CC01--CC04},
title = {{Anthropometric predictors of Bio-impedance analysis (BIA) phase angle in healthy adults}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L610600991&from=export http://dx.doi.org/10.7860/JCDR/2016/17229.7976},
volume = {10},
year = {2016}
}
@article{Sammarco2019,
abstract = {Rationale: Both phase angle (PA) and handgrip strenght (HGS) are considered as markers of nutritional status: phase angle decreases in different forms of malnutrition and it's also a predictor of survival, in several pathological conditions; HGS, being a useful functional capacity test for evaluating muscle strength in the general population, is a diagnostic tool for assessing malnutrition, overall nutritional risk and mortality. Aim of this study was to evaluate the association between PA and HGS in a sample of young women. Methods: 476 females, age range 18–40 years, with BMI between 10 and 25 kg/m2 consecutively undergoing a routine clinical nutritional councelling at the Clinical Nutrition Outpatient Unit, from 2015 to 2018 were included in this study. Grip strength for the right and left arms was measured with a handgrip dynamometer and bioimpedance analysis was performed at 50 kHz (DS Medica) in all subjects. The mean HGS was the average value of three handgrip measurements of the dominant hand. Patient group were split into: BMI subgroup 1 <16 kg/m2, n.114 (age 22.5 ± 5.3 y, weight 37.2 ± 4.5 kg, BMI 14.5 ± 1.2 kg/m2); BMI subgroup 2 = 16.0–18.5 kg/m2, n.171 (age 21.3 ± 5.5 y, weight 44.6 ± 3.7 kg, BMI 17.2 ± 0.7 kg/m2); BMI subgroup 3 = 18.5–25 kg/m2n.191 (age 22.8 ± 5.4 y, weight 54.8 ± 5.9 kg, BMI 21.0 ± 1.6 kg/m2). Results: both PA and mean HGS increased significantly with increasing BMI until group 3 (group1 :5.2 ± 0.9, group2 5.7 ± 0.7, group 3 6.3 ± 0.7 degrees; group1: 18.4 ± 4.6, group2 20.6 ± 4.8, group 3 23.3 ± 5.3 kg). When we performed the linear correlation between PA and HGS, we found a significantly decreasing correlation with increasing BMI (group 1: r 0,417, p = 0,00; group 2 r = 0.324, p = 0.000; group 3 r = 0,308 p = 0.000). Conclusions: Our preliminary findings shows that HGS and phase angle seems strongly associated with BMI groups in Italian young females in particular they are significantly correlated in underweight patients (Group 1 and 2). So they should be considered as good marker of nutritional status.},
author = {Sammarco, R and Marra, M and Morlino, D and {Di Vincenzo}, O and Carmela, D C and {De Filippo}, E and {De Rosa}, A and Truocchio, L and Speranza, E and Contaldo, F and Pasanisi, F},
doi = {10.1016/S0261-5614(19)32351-9},
issn = {1532-1983},
journal = {Clinical Nutrition},
