doi = {10.3923/jpt.2011.468.480},
issn = {1816-496X},
journal = {Journal of Pharmacology and Toxicology},
keywords = {1,1 diphenyl 2 picrylhydrazyl,Eclipta prostrata extract,IC50,acetic acid,acetic acid ethyl ester,analgesic activity,animal experiment,animal model,antiinflammatory activity,antioxidant activity,article,ascorbic acid,ataxia,behavior change,body weight loss,carrageenan,controlled study,diarrhea,diclofenac,dose response,drug dose increase,drug megadose,drug safety,female,flavonoid,hot plate test,immersion,indometacin,male,methanol,mouse,nitric oxide,nonhuman,pain,paw edema,penicillamine,peroxynitrite,phenol derivative,quercetin,rat,writhing test},
