keywords = {adiponectin,adipose tissue,endoplasmic reticulum stress,energy balance,fatty acid oxidation,fatty acid transport,human,insulin resistance,insulin sensitivity,leptin,lipid metabolism,lipogenesis,nonalcoholic fatty liver,nonhuman,obesity,pathogenesis,prevalence,priority journal,resistin,retinol binding protein 4,review,steatosis,very low density lipoprotein},
language = {English},
number = {2},
pages = {679--689},
title = {{Obesity and nonalcoholic fatty liver disease: Biochemical, metabolic, and clinical implications}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L358199487&from=export http://dx.doi.org/10.1002/hep.23280},
volume = {51},
year = {2010}
}
@article{Guimaraes2007,
abstract = {A identifica{\c{c}}{\~{a}}o da leptina, horm{\^{o}}nio secretado pelos adip{\'{o}}citos, cujo efeito sobre o sistema nervoso simp{\'{a}}tico e a fun{\c{c}}{\~{a}}o end{\'{o}}crina confere participa{\c{c}}{\~{a}}o ativa no controle do disp{\^{e}}ndio energ{\'{e}}tico, bem como do apetite, acrescentou {\`{a}}s fun{\c{c}}{\~{o}}es do tecido adiposo no organismo humano o papel de {\'{o}}rg{\~{a}}o multifuncional, produtor e secretor de in{\'{u}}meros pept{\'{i}}deos e prote{\'{i}}nas bioativas, denominadas adipocitocinas. Altera{\c{c}}{\~{o}}es na quantidade de tecido adiposo, como ocorrem na obesidade, afetam a produ{\c{c}}{\~{a}}o da maioria desses fatores secretados pelos adip{\'{o}}citos. Ainda que essas altera{\c{c}}{\~{o}}es estejam freq{\"{u}}entemente associadas {\`{a}}s in{\'{u}}meras disfun{\c{c}}{\~{o}}es metab{\'{o}}licas e ao aumento do risco de doen{\c{c}}as cardiovasculares, permanece sob investiga{\c{c}}{\~{a}}o o envolvimento do tecido adiposo no desenvolvimento dessas complica{\c{c}}{\~{o}}es, considerada a sua fun{\c{c}}{\~{a}}o end{\'{o}}crina. As concentra{\c{c}}{\~{o}}es de v{\'{a}}rias adipocitocinas elevam-se na obesidade e t{\^{e}}m sido relacionadas {\`{a}} hipertens{\~{a}}o (angiotensinog{\^{e}}nio), ao preju{\'{i}}zo da fibrin{\'{o}}lise (inibidor do ativador de plasminog{\^{e}}nio-1) e {\`{a}} resist{\^{e}}ncia {\`{a}} insulina (prote{\'{i}}na estimuladora de acila{\c{c}}{\~{a}}o, fator de necrose tumoral-alfa, interleucina-6 e resistina). De outro modo, leptina e adiponectina t{\^{e}}m efeitos sobre a sensibilidade {\`{a}} insulina. Na obesidade, a resist{\^{e}}ncia insul{\'{i}}nica tamb{\'{e}}m est{\'{a}} relacionada {\`{a}} resist{\^{e}}ncia {\`{a}} leptina e aos teores plasm{\'{a}}ticos reduzidos de adiponectina. Leptina e adiponectina ainda exercem efeitos org{\^{a}}nicos adicionais distintos: frente {\`{a}} participa{\c{c}}{\~{a}}o da leptina no controle da ingest{\~{a}}o alimentar, a adiponectina apresenta potente a{\c{c}}{\~{a}}o anti-aterog{\^{e}}nica. Algumas drogas utilizadas no controle do diabetes elevam a produ{\c{c}}{\~{a}}o end{\'{o}}gena de adiponectina, em roedores e humanos, indicando que o desenvolvimento de novos medicamentos com alvo nas adipocitocinas pode representar uma alternativa terap{\^{e}}utica de preven{\c{c}}{\~{a}}o da resist{\^{e}}ncia insul{\'{i}}nica e da aterosclerose em indiv{\'{i}}duos obesos.Leptin is a hormone secreted by adipocytes whose effect on the sympathetic nervous system and endocrine function confers active participation in the control of energy expenditure and appetite. Its identification added to the fat tissues in the human body the role of a multifunctional organ that produces and secretes a number of bioactive peptides and proteins, called adipocytokines. Changes in the amount of fat tissue, such as the ones that occur in obesity, affect the production of most of these factors secreted by adipocytes. Even if these changes are frequently associated with many metabolic disorders and increased risk for cardiovascular diseases, the role of fat tissue in the development of these complications, considered its endocrine function, continue to be investigated. The concentration of various adipocytokines increase in obesity and have been associated with hypertension (angiotensinogen), fibrinolysis impairment (plasminogen activator inhibitor-1) and insulin resistance (protein that stimulates acylation, tumor necrosis factor-alpha, interleukine-6 and resistin). On the other hand, leptin and adiponectin affect insulin sensitivity. In obesity, insulin resistance is also associated with leptin resistance and reduced plasma levels of adiponectin. Leptin and adiponectin still have complementary and distinct organic functions: adiponectin has potent antiatherogenic activity while leptin participates in the control of food intake. Some medications used to control diabetes increase adiponectin production in rodents and humans, suggesting that the development of new medications that target the adipocytokines can represent a new therapeutic alternative to prevent insulin resistance and atherosclerosis in obese individuals.},
author = {Guimar{\~{a}}es, Daniella Esteves Duque and Sardinha, F{\'{a}}tima L{\'{u}}cia de Carvalho and Mizurini, Daniella de Moraes and do Carmo, Maria das Gra{\c{c}}as Tavares},
doi = {10.1590/s1415-52732007000500010},
file = {:Users/gustavosplmoura/Library/Mobile Documents/com$\sim$apple$\sim$CloudDocs/Medicina/2024_Mendeley/Guimar{\~{a}}es et al.. 2007. Adipocitocinas uma nova vis{\~{a}}o do tecido adiposo. Revista de Nutri{\c{c}}{\~{a}}o.pdf:pdf},
journal = {Revista de Nutri{\c{c}}{\~{a}}o},
number = {5},
pages = {549--559},
title = {{Adipocitocinas: uma nova vis{\~{a}}o do tecido adiposo}},
volume = {20},
year = {2007}
