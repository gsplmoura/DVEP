keywords = {Aspergillus,Pseudomonas,adult,age,antibiotic therapy,article,clinical evaluation,clinical feature,cohort analysis,comorbidity,controlled study,cystic fibrosis,cystic fibrosis transmembrane conductance regulato,disease association,disease exacerbation,disease severity,eosinophil count,eosinophilia,experience,exposure,female,gender,health care cost,hospitalization,human,immunoglobulin E,immunoglobulin blood level,inflammation,lung function,major clinical study,male,medical record review,mepolizumab,methicillin resistant Staphylococcus aureus,microbial colonization,prevalence,retrospective study,type 2 inflammation},
