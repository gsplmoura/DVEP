doi = {10.3389/fimmu.2020.586111},
issn = {1664-3224},
journal = {Frontiers in Immunology},
keywords = {G protein coupled receptor,Notch receptor,Notch signaling,RANTES,adaptive immunity,adult respiratory distress syndrome,article,autoimmune disease,blood group ABO system,cardiovascular disease,chronic fatigue syndrome,controlled study,coronavirus disease 2019,cytokine release,dipeptidyl carboxypeptidase inhibitor,disease severity,gene frequency,genetic association,genetic susceptibility,genome-wide association study,host factor,human,immunoglobulin enhancer binding protein,inflammation,insulin dependent diabetes mellitus,interleukin 12,interleukin 13,interleukin 17,interleukin 23,interleukin 5,interleukin 7,juvenile rheumatoid arthritis,liraglutide,lixisenatide,long QT syndrome,lung alveolus epithelium cell,lymphocytopenia,mucocutaneous lymph node syndrome,pathophysiology,preeclampsia,programmed death 1 ligand 1,protein expression,protein tyrosine kinase,psoriasis,regulatory T lymphocyte,rheumatoid arthritis,risk factor,sarcoidosis,saxagliptin,schizoaffective psychosis,septic shock,sex difference,signal transduction,single nucleotide polymorphism,systemic lupus erythematosus,tocilizumab,toll like receptor 3,toll like receptor 7,transcriptome,upregulation,uveitis,virus load,virus replication},
