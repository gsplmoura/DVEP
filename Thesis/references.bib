keywords = {adolescent,age,anthropometric parameters,arm circumference,article,bioelectrical impedance analysis,body mass,child,clinical article,controlled study,cross-sectional study,cystic fibrosis,female,forced expiratory volume,height,human,hydration,logistic regression analysis,lung disease,lung function,male,nutritional status,skinfold thickness,triceps brachii muscle},
language = {English},
