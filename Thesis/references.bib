journal = {Revista Latino Americana de Enfermagem},
keywords = {ansiedade,avalia{\c{c}}{\~{a}}o,depress{\~{a}}o,escalas,estresse},
number = {6},
pages = {1--9},
title = {{Adapta{\c{c}}{\~{a}}o para a L{\'{i}}ngua Portuguesa da Depression , Anxiety And Stress Scale (DASS)}},
