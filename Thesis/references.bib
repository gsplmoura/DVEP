journal = {Critical Reviews in Food Science and Nutrition},
keywords = {#(DROZD et al. 2022),Matrix effects,bioavailability,fermentation,healthy foods,microstructure,nutrition},
mendeley-tags = {#(DROZD et al. 2022)},
number = {22},
pages = {3612--3629},
pmid = {30040431},
publisher = {Taylor & Francis},
title = {{The food matrix: implications in processing, nutrition and health}},
url = {http://dx.doi.org/10.1080/10408398.2018.1502743},
volume = {59},
year = {2019}
}
