journal = {Hepatic Medicine: Evidence and Research},
keywords = {Model For End Stage Liver Disease Score,anthropometry,autoimmune hepatitis,bile duct atresia,bioelectrical impedance analysis,body mass,body weight,branched chain amino acid,cholangitis,chronic liver disease,computer assisted tomography,cystic fibrosis,dual energy X ray absorptiometry,dyspepsia,end stage liver disease,glucose transport,glutathione,hospitalization,human,hyperammonemia,inflammatory bowel disease,insulin resistance,intensive care unit,interleukin 6,intrahepatic cholestasis,intubation,length of stay,liver cirrhosis,liver disease,liver transplantation,malnutrition,mammalian target of rapamycin,mortality,muscle mass,myostatin,nonalcoholic fatty liver,nuclear magnetic resonance imaging,operation duration,operative blood loss,outcome assessment,pediatric End-Stage Liver Disease score,pediatric patient,protein kinase B,psoas muscle,quality of life,review,risk factor,sarcopenia,scoring system,skeletal muscle,somatomedin C,surface area,testosterone,triacylglycerol,tumor necrosis factor,vitamin D},
