@article{Saglam2019a,
abstract = {Aim: The study aimed to determine lung function, functional capacity, energy cost, and nutritional status in cystic fibrosis (CF) patients with and without inspiratory muscle weakness (IMW). It was also aimed to investigate the relationship between energy cost and lung function, functional capacity, and nutritional status. Methods: Forty-four CF patients (13.70±3.97 yrs, 30M, 24F) participated in this study. Lung function tests and the six-minute walk test (6MWT) were performed. The physiological cost index (PCI) was calculated by the difference between resting heart rate and heart rate at the end of the 6MWT, divided by average velocity (m/min). Nutritional status was evaluated using bioelectrical impedance analysis. Inspiratory and expiratory muscle strength (MIP and MEP, respectively) was measured. Based on MIP, patients were divided into two groups as with and without IMW (<80%, n=19 and >80%, n=25, respectively). Results: There were no significant differences in age, gender, height, weight between the groups (p>0.05). FEV1, PEF, FEF25-75%, 6MWT distance, and fat-free mass index were significantly lower in CF patients with IMW than those without IMW (p<0.05). The PCI was significantly higher in the IMW group (p<0.05). The PCI was related with FFM (r=-0.387, p=0.035), FVC (r=-0.306, p=0.043), and fatigue (r=0.315, p=0.045) in patients with CF. Discussion: The CF patients with IMW show worse lung function, functional capacity, energy cost, and nutritional status. Higher energy cost during submaximal test correlated with nutritional status, fatigue, and forced vital capacity. Preserved inspiratory muscle strength is important in CF.},
address = {M. Saglam, Hacettepe University, Department of Physical Therapy and Rehabilitation, Ankara, Turkey},
author = {Saglam, M and Uge, B and Vardar-Yagli, N and Inal-Ince, D and Sonbahar-Ulu, H and Calik-Kutukcu, E and Bayrakci-Tunay, V and Ozcelik, U},
doi = {10.1183/13993003.congress-2019.PA1160},
issn = {1399-3003},
journal = {European Respiratory Journal},
keywords = {adult,clinical article,conference abstract,controlled study,cystic fibrosis,energy cost,fat free mass,female,forced expiratory volume,forced vital capacity,functional status,gender,human,impedance,lung function test,male,muscle weakness,nutritional status,resting heart rate,six minute walk test},
