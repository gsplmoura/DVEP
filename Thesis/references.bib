title = {{Crosstalk between dietary patterns, obesity and nonalcoholic fatty liver disease}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2019351015&from=export http://dx.doi.org/10.3748/wjg.v28.i27.3314},
volume = {28},
