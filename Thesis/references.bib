journal = {Nutrition},
keywords = {adolescent,adult,article,ascites,body composition,body fat,child,controlled study,cystic fibrosis,edema,female,health status,human,impedance,infant,liver failure,major clinical study,male,potassium,priority journal,skinfold thickness,total body potassium},
language = {English},
