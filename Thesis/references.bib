journal = {Journal of Cystic Fibrosis},
keywords = {Netherlands,adolescent,adult,age,anthropometry,bioenergy,body height,body mass,body weight,child,comparative study,controlled study,correlation analysis,cystic fibrosis,diagnostic agent,female,hospitalization,human,impedance,longitudinal study,major clinical study,male,medical assessment,methodology,muscle mass,non invasive measurement,nutritional status,pathophysiology,physiology,preschool child,prevalence,reference value,reliability,review,scoring system,skinfold thickness},
