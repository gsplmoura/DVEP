journal = {Biomedical and Biopharmaceutical Research},
keywords = {Brazil,Kolmogorov Smirnov test,adult,biological marker,cell membrane permeability,clinical article,conference abstract,controlled study,endogenous compound,enzyme activity,female,glutathione,human,human tissue,inflammatory bowel disease,linear regression analysis,male,malonaldehyde,myeloperoxidase,nitrite,nutritional status,oxidative stress,professional standard,superoxide dismutase},
