@article{Bausewein2010,
abstract = {Breathlessness is a distressing symptom in advanced disease. Little is known about the trajectories of this symptom over time and towards death. This study aimed to describe and compare the summary and individual trajectories of breathlessness and overall symptom burden over time and towards the end of life following patients with advanced cancer or severe chronic obstructive pulmonary disease (COPD) in inpatient and outpatient health care settings in Germany. The modified Borg Scale, Memorial Symptom Assessment Scale Short Form and Palliative Outcome Scale were used as outcome measures. Data were collected at baseline and then monthly over six months or until death. Forty-nine cancer and 60 COPD patients were included. Both groups had similar demographics. Thirty out of the 49 cancer and 6/60 COPD patients died, 7/49 cancer and 20/60 COPD patients dropped out due to physical deterioration or questionnaire fatigue. In cancer patients, breathlessness increased towards death. In COPD patients, breathlessness increased over time. Twenty-one cancer and 43 COPD patients provided data for individual breathlessness trajectories. These revealed wide individual variations with four different patterns: fluctuation, increasing, stable and decreasing breathlessness. Symptom trajectories on the population level reflecting the whole group mask individual variation, which is reflected in distinct symptom trajectories with different patterns. {\textcopyright} 2010 The Author(s).},
address = {C. Bausewein, Department of Palliative Care, Policy and Rehabilitation, Kings' College London, Cicely Saunders Institute, London SE5 9PJ, United Kingdom},
author = {Bausewein, C and Booth, S and Gysels, M and K{\"{u}}hnbach, R and Haberland, B and Higginson, I J},
doi = {10.1177/0269216310378785},
issn = {0269-2163},
journal = {Palliative Medicine},
keywords = {Germany,adult,article,cancer patient,chronic obstructive lung disease,clinical assessment tool,disease severity,dyspnea,fatigue,female,human,major clinical study,male,neoplasm,outcome assessment,outpatient care,peer-reviewed,population research,questionnaire,terminal care},
language = {English},
mendeley-tags = {peer-reviewed},
number = {8},
pages = {777--786},
title = {{Individual breathlessness trajectories do not match summary trajectories in advanced cancer and chronic obstructive pulmonary disease: Results from a longitudinal study}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L360175814&from=export http://dx.doi.org/10.1177/0269216310378785},
volume = {24},
year = {2010}
}
@article{Selman2013,
abstract = {Background: Patients with incurable, progressive disease receiving palliative care in sub-Saharan Africa experience high levels of spiritual distress with a detrimental impact on their quality of life. Locally validated measurement tools are needed to identify patients' spiritual needs and evaluate and improve spiritual care, but up to now such tools have been lacking in Africa. The African Palliative Care Association (APCA) African Palliative Outcome Scale (POS) contains two items relating to peace and life worthwhile. We aimed to determine the content and construct validity of these items as measures of spiritual wellbeing in African palliative care populations.Methods: The study was conducted at five palliative care services, four in South Africa and one in Uganda. The mixed-methods study design involved: (1) cognitive interviews with 72 patients, analysed thematically to explore the items' content validity, and (2) quantitative data collection (n = 285 patients) using the POS and the Spirit 8 to assess construct validity.Results: (1) Peace was interpreted according to the themes 'perception of self and world', 'relationship to others', 'spiritual beliefs' and 'health and healthcare'. Life worthwhile was interpreted in relation to 'perception of self and world', 'relationship to others' and 'identity'. (2) Conceptual convergence and divergence were also evident in the quantitative data: there was moderate correlation between peace and Spirit 8 spiritual well-being (r = 0.46), but little correlation between life worthwhile and Spirit 8 spiritual well-being (r = 0.18) (both p < 0.001). Correlations with Spirit 8 items were weak to moderate. Conclusions: Findings demonstrate the utility of POS items peace and life worthwhile as distinct but related measures of spiritual well-being in African palliative care. Peace and life worthwhile are brief and simple enough to be integrated into routine practice and can be used to measure this important but neglected outcome in this population. {\textcopyright} 2013 Selman et al.; licensee BioMed Central Ltd.},
address = {L. Selman, King's College London, Department Palliative Care, Policy and Rehabilitation, Cicely Saunders Institute, Bessemer Road, Denmark Hill, London SE5 9PJ, United Kingdom},
author = {Selman, L and Speck, P and Gysels, M and Agupio, G and Dinat, N and Downing, J and Gwyther, L and Mashao, T and Mmoledi, K and Moll, T and Sebuyira, L M and Ikin, B and Higginson, I J and Harding, R},
doi = {10.1186/1477-7525-11-94},
issn = {1477-7525},
journal = {Health and Quality of Life Outcomes},
keywords = {African Palliative Outcome Scale,African medicine,Uganda,adult,article,assessment of humans,construct validity,content validity,controlled study,correlation analysis,female,health,health care,health care concepts,health service,human,information processing,interview,life satisfaction,major clinical study,male,palliative therapy,peace,peer-reviewed,perception,religion,self concept,wellbeing},
language = {English},
mendeley-tags = {peer-reviewed},
