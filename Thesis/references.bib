@article{Hollander-Kraaijeveld2020a,
abstract = {Objectives: In healthy adults, protein intake is positively associated with fat-free mass (FFM). We investigated the association between protein intake and FFM or its height-corrected index (FFMI) in adult cystic fibrosis (CF) patients. Additionally, the association between muscle strength and FFM(I) was investigated. Methods: FFM(I) (kg, kg/m2) was determined by whole body single frequency bioelectrical impedance analysis (Bodystat 500) in 105 adult CF patients. Protein intake was estimated from a dietary history and expressed as % of energy intake (EN%). Muscle strength expressed as isometric knee extension strength (%pred) (n = 71) was determined by a Cybex or MicroFET II dynamometer. Associations were tested by multiple linear regression analysis. Results: Protein intake ranged from 10.9 to 31.0 EN%. Mean FFM across tertiles of protein intake (EN%) was 50.7 kg, 48.4 kg and 52.9 kg (P = 0.401) whereas mean FFMI was 16.5 kg/m2, 16.1 kg/m2 and 17.2 kg/m2, respectively (P = 0.132). A low FFM and FFMI was observed in 7 and 21 patients, respectively. Adjusted for age and gender, protein intake was significantly associated with FFM (B = 0.507 kg per 2EN%, P = 0.047) and with FFMI (B = 0.165 kg/m2 per 2EN%, P = 0.012). Muscle strength was significantly associated with FFM (B = 0.893 kg per 5%, P = 0.000) and FFMI (B = 0.167 kg/m2 per 5%, P = 0.000). Conclusion: A higher protein intake and higher muscle strength were significantly associated with a higher FFM(I). Further research should focus on protein intake and muscle strength improvement in CF patients with low FFM(I).},
author = {Hollander-Kraaijeveld, F and Winter, I and Burghard, M and de Roos, N and Heijerman, H},
doi = {10.1016/S1569-1993(20)30218-6},
issn = {1873-5010},
journal = {Journal of Cystic Fibrosis},
keywords = {adult,caloric intake,conference abstract,controlled study,cystic fibrosis,dynamometer,fat free mass,female,gender,human,impedance,knee,major clinical study,male,protein intake},
language = {English},
pages = {S16--S17},
title = {{Associations of protein intake and muscle strength with fat-free mass (index) in adult cystic fibrosis patients}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2006056942&from=export http://dx.doi.org/10.1016/S1569-1993(20)30218-6},
volume = {19},
year = {2020}
}
@article{DeMarchis2022,
abstract = {Cystic fibrosis (CF) is the most common genetic disease in Caucasian people. Nutritional status represents an important key in the progression of the pulmonary disease in CF. People with better nutritional status, generally, maintain good levels of physical activity. Generally Bioelectrical impedance (BIA) analysis is frequently used as a method of body composition assessment, due to easy of use, safety and low cost of this procedure. The aim of this study was to investigate nutritional parameters in cystic fibrosis patient. We performed a single group cohort study. The study examined change in nutritional values in people with CF who practice sport or not, measured by bio-impedance analysis (BIA). Inclusion criteria were people with CF diagnosis confirmed. Primary outcome was evaluate body composition and the correlation with the rate of physical activity. A total of 32 patients were included in the analysis. The most important data was a correlation between Phangle and Body cellular mass index (BCMI) Pvalue<0.01, expecially in patients who had a good levels of aerobic and anaerobic session-training. Patients who did strong physical activity training had a statistically significant values of correlation with nutritional status. Further study were necessary to find association between exercise capa city and body mass index.},
