@article{Winer2018,
abstract = {INTRODUCTION: Our preliminary studies in a murine model of persistent inflammation, immunosuppression, and catabolism syndrome (PICS) suggested that the spleen and lungs may become alternative reservoirs of active neutrophils. We hypothesized that spleen, bone marrow (BM), and lung neutrophils develop altered and distinct phenotypes that contribute to immunosuppression during PICS. METHODS: Mice underwent 33% cecal ligation with a single 25-gauge needle puncture. Spleen, BM, and lung homogenates were harvested 8 days after cecal ligation, when mice display all characteristics of PICS. Neutrophils were identified and enumerated with flow cytometry. Neutrophil reactive oxygen species (ROS) were detected with a dihydrorhodamine (DHR) assay, and phagocytosis was measured with pHrodo succinimidyl ester staining. Student's t-test and ANOVA were used for analysis; p < 0.05 was considered statistically significant. RESULTS: During PICS, BM neutrophil counts remained stable, while splenic and alveolar neutrophil counts increased by 4-and 2.5-fold, respectively (p<0.05). Neutrophil ROS production increases by 1.5-to 2-fold in spleen, BM, and lungs, with the greatest oxidative burst noted in the spleen (p<0.05). Finally, neutrophil phagocytosis diminished by 30% in BM and increased by 50% in lung (p<0.05); this function remained unchanged in spleen. CONCLUSIONS: During PICS, there is a heterogeneous pool of dysfunctional neutrophils derived from BM, spleen, and lungs. We speculate that these alterations in neutrophil phenotype may contribute to the persistent inflammation and immunosuppression observed during PICS, and may provide potential therapeutic targets for patients who progress to this state.},
address = {L.K. Winer, University of Cincinnati, Cincinnati, OH, United States},
author = {Winer, L K and Pugh, A M and Caldwell, C C and Nomellini, V},
