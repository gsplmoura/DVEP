@article{Gonzalez2015,
abstract = {Rationale: To compare Phase Angle reference values (PhARV) from four population samples and to understand how population of origin, type of bioimpedance analysis (BIA) instrument, body mass index (BMI), sex and age can explain PhA variability. Methods: A sample was obtained by assembling four national databases containing PhARV from Brazil, Germany, Switzerland and the United States. Different BIA systems were used and the evaluations were performed under standardized procedures. Multivariate regression analysis was performed to identify the significant PhA determinants. Results: The pooled sample has 282,807 subjects, 69% female, mean (SD) age of 43.5 (12.6) years. Although several BIA devices were used, the PhARV have a similar distribution among the four countries: males have higher values than females for the same age group until 7th decade (p < 0.05) and values are lower with greater age from 4th decade (p < 0.05). Different PhARV were obtained from each population group, according to sex and age decade (p < 0.05). Sex and age explain most of the PhA variability (AdjR2 = 0.34) in the regression analysis. Age has a negative effect in PhA: each decade decreases from 0.21° to 0.88° from 5th decade. BMI <18.5 and >40 kg/m2 are associated with a decrease of 0.36° and 0.02°, respectively, in PhA values. African Americans have PhA values 0.84° higher than the other race groups. The BIA device is still a significant factor in determining PhA variability even after controlling for all of these biological variables. Compared to the RJL device, BIA 2000-S results in PhA values 0.38° smaller. Conclusion: PhARV has a similar distribution across sex and age among different populations, but reference values are quite different and it is not possible to use a “global” PhARV. The selected BIA device has a significant impact on measured PhA and is a likely cause of the between-population differences.},
author = {Gonzalez, M C and Pichard, C and Thibault, R and Bosy-Westphal, A and Larsson, E and Heymsfield, S B},
issn = {0261-5614},
journal = {Clinical Nutrition},
keywords = {African American,Brazil,European,Germany,Switzerland,United States,body mass,data base,devices,female,groups by age,human,male,metabolism,nutrition,population,population group,procedures,reference value,regression analysis,society},
language = {English},
pages = {S189},
title = {{Similarities and differences in phase angle reference values from different populations}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L72147278&from=export},
volume = {34},
year = {2015}
}
@misc{PAHO2022,
abstract = {A Organiza{\c{c}}{\~{a}}o Pan-Americana da Sa{\'{u}}de/Organiza{\c{c}}{\~{a}}o Mundial da Sa{\'{u}}de (OPAS/OMS) e o governo da Rep{\'{u}}blica Federativa do Brasil apresentam a Estrat{\'{e}}gia de Coopera{\c{c}}{\~{a}}o com o Pa{\'{i}}s (ECP) 2022–2027. {\'{E}} um documento que orienta a coopera{\c{c}}{\~{a}}o t{\'{e}}cnica, al{\'{e}}m de corroborar com a longa hist{\'{o}}ria de parceria da OPAS/OMS com o governo brasileiro e ratificar o compromisso de trabalho para alcance da sa{\'{u}}de como um direito de todos e dever do Estado. A ECP 2022-2027 configura-se como oportunidade de refor{\c{c}}ar compromissos e alian{\c{c}}as para o enfrentamento dos desafios ainda existentes no campo da sa{\'{u}}de p{\'{u}}blica com o apoio da coopera{\c{c}}{\~{a}}o t{\'{e}}cnica da OPAS/OMS com o Brasil. H{\'{a}} mais de 30 anos, o Brasil conta com um Sistema {\'{U}}nico de Sa{\'{u}}de, refer{\^{e}}ncia mundial, fonte de conhecimento para a Regi{\~{a}}o das Am{\'{e}}ricas e outras regi{\~{o}}es do mundo, baseado nos princ{\'{i}}pios de “universalidade, integralidade e equidade”, e que traz a participa{\c{c}}{\~{a}}o popular como elemento fundamental para o exerc{\'{i}}cio do direito {\`{a}} sa{\'{u}}de. O p{\'{o}}s-pandemia da Covid-19 deve ser uma oportunidade para retomada dos avan{\c{c}}os e da colabora{\c{c}}{\~{a}}o para recupera{\c{c}}{\~{a}}o oportuna, estrat{\'{e}}gica e sustent{\'{a}}vel rumo {\`{a}} sa{\'{u}}de universal no Pa{\'{i}}s. Continuaremos trabalhando juntos nos pr{\'{o}}ximos anos para garantir a sa{\'{u}}de universal, uma vida saud{\'{a}}vel e bem-estar para todas as pessoas sem deixar ningu{\'{e}}m para tr{\'{a}}s no Brasil.},
author = {PAHO and OPAS and OMS},
file = {:Users/gustavosplmoura/Library/Mobile Documents/com$\sim$apple$\sim$CloudDocs/Medicina/2024_Mendeley/PAHO, OPAS, OMS. 2022. Estrat{\'{e}}gia de Coopera{\c{c}}{\~{a}}o do Pa{\'{i}}s 2022-2027. Brasil. Unknown.pdf:pdf},
title = {{Estrat{\'{e}}gia de Coopera{\c{c}}{\~{a}}o do Pa{\'{i}}s 2022-2027. Brasil}},
