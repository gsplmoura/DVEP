issn = {16779487},
journal = {Arquivos Brasileiros de Endocrinologia e Metabologia},
keywords = {Endothelial dysfunction,Metabolic syndrome,Microcirculation,Obesity,Tissue perfusion},
number = {2},
pages = {238--245},
publisher = {Sociedade Brasileira de Endocrinologia e Metabologia},
title = {{Altera{\c{c}}{\~{o}}es metab{\'{o}}licas associadas {\`{a}} obesidade: O papel da disfun{\c{c}}{\~{a}}o da microcircula{\c{c}}{\~{a}}o tecidual}},
volume = {53},
