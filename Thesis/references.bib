@article{Francque2021,
abstract = {This patient guideline is intended for all patients at risk of or living with non-alcoholic fatty liver disease (NAFLD). NAFLD is the most frequent chronic liver disease worldwide and comes with a high disease burden. Yet, there is a lot of unawareness. Furthermore, many aspects of the disease are still to be unravelled, which has an important impact on the information that is given (or not) to patients. Its management requires a close interaction between patients and their many healthcare providers. It is important for patients to develop a full understanding of NAFLD in order to enable them to take an active role in their disease management. This guide summarises the current knowledge relevant to NAFLD and its management. It has been developed by patients, patient representatives, clinicians and scientists and is based on current scientific recommendations, intended to support patients in making informed decisions.},
author = {Francque, Sven M. and Marchesini, Giulio and Kautz, Achim and Walmsley, Martine and Dorner, Rebecca and Lazarus, Jeffrey V. and Zelber-Sagi, Shira and Hallsworth, Kate and Busetto, Luca and Fr{\"{u}}hbeck, Gema and Dicker, Dror and Woodward, Euan and Korenjak, Marko and Willemse, Jos{\'{e}} and Koek, Gerardus H. and Vinker, Shlomo and Ungan, Mehmet and Mendive, Juan M. and Lionis, Christos},
