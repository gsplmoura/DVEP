issn = {2163-0755},
journal = {Journal of Trauma and Acute Care Surgery},
keywords = {CD4+ T lymphocyte,CD8+ T lymphocyte,HLA DR antigen,RANTES,adaptive immunity,antigen expression,clinical feature,compensatory antiinflammatory response syndrome,cytokine production,cytokine release,dendritic cell,gene expression,human,immune deficiency,immunoglobulin enhancer binding protein,inflammation,innate immunity,intensive care unit,interleukin 1,interleukin 10,interleukin 17,interleukin 6,length of stay,macrophage,metabolic syndrome X,monocyte,monocyte chemotactic protein 1,multiple organ failure,nutritional support,pathology,persistent inflammation, immunosuppression and cat,physical disease by etiology and pathogenesis,priority journal,programmed death 1 ligand 1,review,systemic inflammatory response syndrome,tumor necrosis factor,upregulation,wellbeing},
