author = {Tsilingiris, D and Schimpfle, L and Kender, Z and Sulaj, A and {Von Rauchhaupt}, E and Bartl, H and Szendr{\"{o}}di, J and Kopf, S},
doi = {10.1055/s-0042-1746297},
issn = {1861-9010},
journal = {Diabetologie und Stoffwechsel},
keywords = {adult,ankle brachial index,antidiabetic agent,arterial wall thickness,atherosclerosis,biological marker,body mass,brain natriuretic peptide,cardiovascular risk,cardiovascular risk factor,carotid-femoral pulse wave velocity,cell membrane,cholesterol,conference abstract,controlled study,diabetes mellitus,endogenous compound,estimated glomerular filtration rate,female,gender,glucose,glucose tolerance,heart,hemoglobin A1c,human,hydroxymethylglutaryl coenzyme A reductase inhibit,hypertension,impaired glucose tolerance,insulin dependent diabetes mellitus,low density lipoprotein,major clinical study,male,middle aged,non insulin dependent diabetes mellitus,risk assessment,smoking,troponin,waist circumference},
