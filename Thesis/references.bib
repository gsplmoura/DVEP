journal = {Archives of Disease in Childhood},
keywords = {adult,article,body composition,bone mineral,child,clinical article,controlled study,cystic fibrosis,dual energy X ray absorptiometry,female,human,lumbar spine,male,nutrition,priority journal},
language = {English},
number = {4},
pages = {314--318},
title = {{Bone mineral content in cystic fibrosis patients: Correlation with fat-free mass}},
