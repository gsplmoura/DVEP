issn = {0944-7113},
journal = {Phytomedicine},
keywords = {Lippia alba extract,NCT01259947,adult,antimigraine agent,article,clinical article,disease severity,drug efficacy,drug response,female,human,migraine,pain assessment,phase 2 clinical trial,plant leaf,priority journal,prospective study,symptom assessment},
language = {English},
number = {10},
