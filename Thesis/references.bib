@techreport{Banji2010,
abstract = {Parturient females display impulsive behavior represented in the form of aggressive bouts when exposed to conspecifics. Prolonged aggression during the postpartum period could affect maternal care. Eclipta alba is traditionally known to induce neuropsychiatric alterations, however its ability to circumvent maternal aggression has not been elucidated. The present study was aimed to investigate the ability of the aqueous extract of Eclipta alba to suppress maternal aggression. In the single dose study, 100, 200 and 500mg/kg body weight of the aqueous extract of Eclipta alba was administered to parturient females 30 minutes prior to maternal aggression testing against intruder males. In the multiple dose study, 100, 200 and 500mg/kg of the extract were administered for 15 and 30 days and maternal aggression was quantified. Administration of the extract for 15 and 30 days in dose schedules of 200 and 500mg/kg body weight significantly suppressed agonistic encounters by the dams and therefore had beneficial anti-aggressive activity.},
author = {Banji, David and Banji, J F and Annamalai, A R and And, Shanthmurthy M},
booktitle = {Pak. J. Pharm. Sci},
file = {:Users/gustavosplmoura/Library/Mobile Documents/com$\sim$apple$\sim$CloudDocs/Medicina/2024_Mendeley/Banji et al.. 2010. IMPACT OF THE AQUEOUS EXTRACT OF ECLIPTA ALBA ON MATERNAL AGGRESSION IN RATS. Pak. J. Pharm. Sci.pdf:pdf},
keywords = {Eclipta alba,Maternal aggression},
number = {2},
pages = {138--142},
title = {{IMPACT OF THE AQUEOUS EXTRACT OF ECLIPTA ALBA ON MATERNAL AGGRESSION IN RATS}},
volume = {23},
year = {2010}
}
@article{Mattiello2020,
abstract = {Background & aims: The bioimpedence phase angle has been considered as a predictor for morbidity and mortality in different clinical situations, although reference values from a large healthy population are lacking. The aim of this meta-analysis is to estimate mean phase-angle values in healthy individuals. Methods: This meta-analysis systematically searched MEDLINE, EMBASE, The Cochrane Controlled Trials Register, SCIELO, LILACS, CINAHL, Web of Science and gray literature for studies estimating mean phase angles. Quality of evidence was assessed for all studies and subgroup (males and females) meta-analysis stratified by age group according to literature (up to 2; 3–5; 6–12; 13–15; 16–18; 19–28; 29–38; 39–48; 49–58; 59–69; 70–80 and >80 years of age) were conducted using random-effects models. Results: A total of 46 studies including 249,844 subjects were selected for the present analysis. Males show a pooled estimate of the mean phase angle of 3.6 (95% CI: 3.0–4.1) for infants (0–2 y), increasing progressively to 7.3 (95% CI: 7.0–7.5) at the teenage phase (16–18 y), stabilizing during adult ages (18–38) and decreasing progressively with ongoing years with an estimate of 5.3 (95% CI: 4.5–6.0) for elderly above 80 years old. Similarly, females start from 3.7 (95% CI: 3.2–4.3) for infants (0–2 y), increasing progressively to 6.4 (95% CI: 6.1–6.8) at the teenage phase (16–18 y), stabilizing during adult ages (18–48) and decreasing progressively with ongoing years with an estimate of 5.4 (95% CI: 5.3–5.6) for elderly above 80 years old. Also, males have higher estimates than females for all age groups except for infants (0–2) and subjects older than 80 years old. Heterogeneity was high for all age groups. Conclusions: In both sexes, phase-angle values have a similar pattern that start from infants, increase progressively up to the teenage phase, stabilize during adult ages, and then decrease progressively in older subjects and the elderly.},
author = {Mattiello, Rita and Amaral, Mariana Azambuja and Mundstock, Eduardo and Ziegelmann, Patr{\'{i}}cia Klarmann},
doi = {10.1016/j.clnu.2019.07.004},
file = {:Users/gustavosplmoura/Library/Mobile Documents/com$\sim$apple$\sim$CloudDocs/Medicina/2024_Mendeley//Mattiello et al.. 2020. Reference values for the phase angle of the electrical bioimpedance Systematic review and meta-analysis involvin.pdf:pdf;:Users/gustavosplmoura/Library/Mobile Documents/com$\sim$apple$\sim$CloudDocs/Medicina/2024_Mendeley//Mattiello et al.. 2020. Reference values for the phase angle of the electrical bioimpedance Systematic review and meta-analysis involvin.pdf:pdf;:Users/gustavosplmoura/Library/Mobile Documents/com$\sim$apple$\sim$CloudDocs/Medicina/2024_Mendeley/Mattiello et al.. 2020. Reference values for the phase angle of the electrical bioimpedance Systematic review and meta-analysis invol(3).pdf:pdf},
issn = {15321983},
journal = {Clinical Nutrition},
keywords = {Bioimpedance,Phase angle,Reference values},
number = {5},
pages = {1411--1417},
pmid = {31400996},
publisher = {Elsevier Ltd},
title = {{Reference values for the phase angle of the electrical bioimpedance: Systematic review and meta-analysis involving more than 250,000 subjects}},
url = {https://doi.org/10.1016/j.clnu.2019.07.004},
volume = {39},
year = {2020}
}
@article{DaSilva2022,
abstract = {Phase angle is a composite measure that combines two raw bioelectrical impedance analysis measures: resistance and reactance. Phase angle has been considered an indicator of cellular health, integrity, and hydration. As inflammation and oxidative stress can damage cellular structures, phase angle has potential utility in early detecting inflammatory and oxidative status. Herein, we aimed to critically review the current understanding on the determinants of phase angle and its relationship with markers of inflammation and oxidative stress. We also discussed the potential role of phase angle in detecting chronic inflammation and related adverse outcomes. Several factors have been identified as predictors of phase angle, including age, sex, extracellular to intracellular water ratio, and fat-free mass. In addition to these factors, body mass index (BMI) also seems to influence phase angle. Available data also show that lower phase angle values are correlated (negligible to high correlation coefficients) with higher c-reactive protein, tumour necrosis factor-$\alpha$, interleukin-6, and interleukin-10 in studies involving the general and aging populations, as well as patients with chronic conditions. Although fewer studies have evaluated the relationship between phase angle and markers of oxidative stress, available data also suggest that phase angle has potential to be used as an indicator (for screening) of oxidative damage. Future studies including diverse populations and bioelectrical impedance devices are required to confirm the validity and accuracy of phase angle as a marker of inflammation and oxidative stress for clinical use.},
annote = {From Duplicate 2 (Phase angle and cellular health: inflammation and oxidative damage - da Silva¹, Bruna Ramos; Orsso¹, Camila E; Gonzalez², Maria Cristina; Sicchieri³, Juliana Maria Faccioli; Mialich³, Mirele Savegnago; Jordao³, Alceu A; Prado, Carla M)

