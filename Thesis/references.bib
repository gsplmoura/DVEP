language = {English},
number = {1},
pages = {49--52},
title = {{Bioelectrical impedance analysis: Population reference values for phase angle by age and sex}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L41716376&from=export http://dx.doi.org/10.1093/ajcn.82.1.49},
volume = {82},
