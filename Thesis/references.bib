language = {English},
number = {8},
title = {{Inflammation as a Regulator of the Airway Surface Liquid pH in Cystic Fibrosis}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2023005342&from=export http://dx.doi.org/10.3390/cells12081104},
volume = {12},
