@article{Phan2023,
abstract = {Eclipta prostrata L. (EPL), a medicinal plant, is widely utilized in the central highlands of Vietnam. This study aims to assess the chemical profile and potential medical effects of an EPL extract rich in flavonoids. A total of 36 secondary metabolites were identified from the EPL extract through GC-MS and UHPLC-UV analysis. Among them, 15 volatile compounds and several phenolic and flavonoid chemicals, including salicylic acid, epicatechin gallate, isovitexin, and apigetrin, were reported in EPL extract for the first time. This herbal extract demonstrated moderate inhibition against $\alpha$-amylase and $\alpha$-glucosidase, and high anti-oxidant and anti-acetylcholinesterase activities (IC50 = 76.8 ± 0.8 $\mu$g/mL). These promising attributes can be likely attributed to the high levels of major compounds, including wedelolactone (1), chlorogenic acid (3), epicatechin gallate (6), salicylic acid (8), isovitexin (9), apigetrin (11), and myricetin (12). These findings align with the traditional use of EPL for enhancing memory and cognitive function, as well as its potential benefits in diabetes management. The results of the molecular docking study reveal that the major identified compounds (1, 6, 9, and 11) showed a more effective acetylcholinesterase inhibitory effect than berberine chloride, with good binding energy (DS values, −12.3 to −14.3 kcal/mol) and acceptable values of RMSD (1.02–1.67 {\AA}). Additionally, almost all the identified major compounds exhibited good ADMET properties within the required limits.},
author = {Phan, Tu Quy Thi Kim Phung and Wang, San-Lang Lang and Nguyen, Quang Vinh and Phan, Tu Quy Thi Kim Phung and Nguyen, Tan Thanh and Tran, Thanh Tam Toan and Nguyen, Anh Dzung and Nguyen, Van Bon and Doan, Manh Dung},
doi = {10.3390/ph16101476},
file = {:Users/gustavosplmoura/Library/Mobile Documents/com$\sim$apple$\sim$CloudDocs/Medicina/2024_Mendeley/Phan et al.. 2023. Assessment of the Chemical Profile and Potential Medical Effects of a Flavonoid-Rich Extract of Eclipta prostrata L..pdf:pdf;:Users/gustavosplmoura/Library/Mobile Documents/com$\sim$apple$\sim$CloudDocs/Medicina/2024_Mendeley/Phan et al.. 2023. Assessment of the Chemical Profile and Potential Medical Effects of a Flavonoid-Rich Extract of Eclipta prostrata (2).pdf:pdf},
issn = {1424-8247},
journal = {Pharmaceuticals},
keywords = {Eclipta prostrataL,UHPLC,anti-acetylcholinesterase,anti-oxidant,flavonoids},
month = {oct},
number = {10},
pages = {1476},
pmid = {37895947},
publisher = {Multidisciplinary Digital Publishing Institute  (MDPI)},
title = {{Assessment of the Chemical Profile and Potential Medical Effects of a Flavonoid-Rich Extract of Eclipta prostrata L. Collected in the Central Highlands of Vietnam}},
