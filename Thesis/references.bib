year = {2018}
}
@article{Genton2018,
abstract = {Purpose: A high phase angle measured by the Nutriguard{\textregistered} bioelectrical impedance analysis device is associated with a reduced mortality risk in older people. This retrospective study aims to analyze whether this association persists with the other devices that have been used in our hospital. Methods: This study encompasses all people 65 yrs and older who underwent a phase angle measurement between 1990 and 2011 at the Geneva University Hospitals, with the RJL-101{\textregistered} (RJL Systems), Xitron 4000B{\textregistered} (Xitron Technologies), Eugedia{\textregistered} (Eug{\'{e}}dia-Spengler) and Bio-Z{\textregistered} (Spengler). Diseases at the time of phase angle measurement were reported in the form of the Cumulative Illness Rating Scale. Date of death was retrieved until December 2012. Phase angle values were categorized into sex- and device-specific quartiles, where quartile 1 represents the lowest quartile and reference value. Cox regressions were performed to evaluate the association between phase angle quartiles and mortality. Results: We considered 1878 people (969 women), of whom 1151 had died. In univariate sex-specific Cox regressions, the death risk decreased progressively as the phase angle quartile measured by the Bio-Z{\textregistered} or RJL-101{\textregistered} increased. The HR (95% CI) in quartile 4 was 0.36 (0.26, 0.50) and 0.38 (0.29, 052) in women and men measured with the Bio-Z{\textregistered} (both p < 0.001), and 0.23 (0.14, 0.39) and 0.19 (0.10, 0.36) in women and men measured with the RJL-101{\textregistered} (both p < 0.001). The association between phase angle and mortality persisted when adjusted for age, body mass index or co-morbidities. The small number of deaths in people who underwent a measurement by Eugedia{\textregistered} (n = 93) or Xitron 4000B{\textregistered} (n = 56) did not allow performing multivariate Cox regressions. Conclusions: Phase angle quartiles are associated with mortality in people aged ≥65 years when using the RJL-101{\textregistered} or Bio-Z device{\textregistered}.},
address = {L. Genton, Clinical Nutrition, Geneva University Hospitals, Rue Gabrielle Perret-Gentil 4, Geneva, Switzerland},
