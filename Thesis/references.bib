language = {English},
number = {3},
pages = {211--216},
title = {{Phase angle is a better indicator of nutritional status than body mass index in patients on hemodialysis}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2013565574&from=export http://dx.doi.org/10.24875/GMM.M19000263},
