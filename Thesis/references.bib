journal = {Nutrition},
keywords = {adolescent,anthropometry,article,bioenergy,child,clinical article,cystic fibrosis,female,human,impedance,male,malnutrition,nutritional health,nutritional status,potassium,priority journal,statistical analysis,total body potassium},
language = {English},
number = {5},
pages = {412--416},
title = {{Evaluation of bioelectrical impedance for prospective nutritional assessment in cystic fibrosis}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L27282133&from=export http://dx.doi.org/10.1016/S0899-9007(97)91278-8},
volume = {13},
year = {1997}
}
@article{Ballinger2020,
abstract = {Objectives: Current clinical practice relies heavily on BMI and weight, but these alone will fail to identify adults with CF (AWCF) with excess or depleted fat mass (FM) or fat free mass (FFM). This service evaluation aims to describe the body composition of AWCF at a UK CF centre using bioelectrical impedance analysis (BIA), to examine the prevalence of previously undetectable anthropometric states including ‘hidden obesity' (called normal weight obesity - NWO), and to examine the relationship with clinical parameters. Methods: BIA data collected as part of dietetic clinic assessment from 2017 to 2019 using TANITA 780 model. BIA measurements included FFM, body fat (BF)%, BF kg, phase angle (PA). Clinical data was also collected. NWO has been defined as BMI < 25 kg/m2 and body fat >30% women, >23% men. Results: Data for 174 AWCF (53% male) aged 17–72 years were analysed. Based on BMI alone, 26/174 were classified as overweight (BMI>25 kg/m2) and 5/174 were classified as obese (BMI>30 kg/m2). 25/174(14%) had NWO. Statistical analysis was performed to compare those with NWO and AWCF who had normal weight and normal FFM; mean BMI 20.3 v 22.75 (p = 0.7). FEV1% 74 v 63% ± 0.24 v 0.24 (p = 0.04); Phase angle (PA) 6.25 v 5.82 ± 0.91 v 0.67 (p = 0.03). Conclusion: This data suggests that current standard anthropometric measures are missing a substantial number of AWCF who are obese, and that this is associated with worse clinical outcomes. Results show NWO is associated with both lower lung function and phase angle (a measure of overall body health). Currently a third of our CF population have excess body fat (combination of overweight, obese and NWO). With the advent of CFTR modulator therapies, nutrition in PWCF will improve and obesity and its complications are likely to increase in prevalence. Dietetic practice needs to adapt to the evolving CF population, however reversing long-term dietary habits may prove challenging.},
author = {Ballinger, K and Daniels, T and Pearson, C},
doi = {10.1016/S1569-1993(20)30216-2},
issn = {1873-5010},
journal = {Journal of Cystic Fibrosis},
keywords = {adolescent,adult,aged,body fat,body mass,clinical outcome,complication,conference abstract,controlled study,cystic fibrosis,fat free mass,female,forced expiratory volume,habit,human,impedance,lung function,major clinical study,male,nutrition,obesity,outcome assessment,prevalence},
language = {English},
pages = {S16},
title = {{Prevalence of hidden obesity in adults with cystic fibrosis}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2006056738&from=export http://dx.doi.org/10.1016/S1569-1993(20)30216-2},
volume = {19},
year = {2020}
}
@article{Lands1993,
abstract = {Body composition analysis is an important component of nutritional assessment in cystic fibrosis (CF). No gold standard of measurement exists, and techniques applicable to healthy populations may be unsuitable for CF patients. We assessed lean body mass (LBM) in 12 children with CF by skinfold (SK) measurements, bioelectrical impedance analysis (BIA), and dual-photon absorptiometry (DPA) and repeated these measures in 10 subjects 6 mo later. SK and DPA measures in eight older CF patients and eight healthy controls were compared to evaluate any effect of disease on estimates of LBM by use of DPA. Good agreement between the measures was seen at baseline and 6 mo by use of concordance plots. However, the limits of agreement between measures ranged up to 19% of SK-derived LBM measures (baseline: SK and DPA, 2.63 to - 3.93 kg; SK and BIA, 2.36 to -1.24 kg; BIA and DPA, 1.88 to -4.28 kg; 6 mo: SK and DPA, 2.10 to -3.58 kg; SK and BIA, 6.28 to -5.49 kg; BIA and DPA, 5.53 to -7.79 kg). The change in LBM over 6 mo did not correlate among the three measures. Only BIA change in LBM correlated with weight change (r = 0.716, P < 0.02), probably due to the inclusion of weight in the regression equations for determining LBM from impedance. The relationship between SK and DPA measures did not differ between the CF and control groups, suggesting that there was no effect of disease on the DPA measure. The results suggest that none of these methods is precise enough to follow short-term changes in the nutritional status of CF patients longitudinally.},
address = {L.C. Lands, HSC 3U25, McMaster University, Box 2000, Hamilton, Ont. L8N 3Z5, Canada},
author = {Lands, L C and Gordon, C and Bar-Or, O and Blimkie, C J and Hanning, R M and Jones, N L and Moss, L A and Webber, C E and Wilson, W M and Heigenhauser, G J F},
doi = {10.1152/jappl.1993.75.1.162},
issn = {8750-7587},
journal = {Journal of Applied Physiology},
keywords = {adolescent,article,body composition,child,clinical article,controlled study,cystic fibrosis,female,human,intermethod comparison,lean body weight,male,nutritional status,photon absorptiometry,priority journal,skin conductance,skinfold thickness},
language = {English},
number = {1},
pages = {162--166},
title = {{Comparison of three techniques for body composition analysis in cystic fibrosis}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L23221970&from=export http://dx.doi.org/10.1152/jappl.1993.75.1.162},
volume = {75},
year = {1993}
}
@article{Mathur2017,
abstract = {Rationale: Muscle cross-sectional area (CSA) from thoracic computed tomography (CT) has been utilized as a marker of low muscle mass, however its relationship to overall fat free mass has not been established. Furthermore, the measurement of body fat from thoracic CT has not been explored. The study objectives were: 1) to determine the reliability of measuring body fat compartments from thoracic CT and 2) to evaluate the criterion validity of muscle and fat CSA obtained from thoracic CT as measures of body composition compared to a reference standard, bioelectrical impedance analysis (BIA). Methods: Thoracic CT scans were obtained from medical records of people listed for lung transplantation. BIA was done at least 4 weeks after transplant listing. Total thoracic muscle CSA, pectoral muscle CSA (-29 to 150 HU), subcutaneous fat anterior to the pectoral muscles (-190 to -30 HU), and intrathoracic fat in the mediastinal region (-150 to -50 HU) were manually segmented at the level of the carina using Slice-O-Matic. Reliability testing was done by two independent raters and analysed using intra-class correlation coefficient (ICC). Muscle CSAs were correlated to fat free mass index (FFMI) and fat CSAs to body fat mass index (BFMI) and percent body fat (%BF) obtained from BIA. Regression analysis was used to determine the relationship between body composition and fat CSA, accounting for age and sex. Results: Fifty subjects were included (29 males, age=59±9 yrs), with diagnoses of interstitial lung disease (n=32), chronic obstructive pulmonary disease (n=12), pulmonary hypertension (n=4) and cystic fibrosis (n=2). Reliability analysis showed excellent intra-rater (ICC=0.99) and inter-rater reliability (ICC=0.99) for both subcutaneous and intrathoracic fat. Body composition is shown in the Table. Total thoracic muscle CSA had a stronger correlation to FFMI (r =0.46, p< 0.001) than pectoral muscle CSA (r=0.28, p=0.05). Muscle CSAs did not correlate to BMI (r= -0.1-0.01, p>0.50). Subcutaneous fat CSA showed moderate correlations with BFMI (r=0.80, p<0.001), %BF (r=0.66, p<0.001) and BMI (r=0.76, p <0.001). Intrathoracic fat only correlated with BMI (r = 0.58, p <0.001). Multivariate analysis revealed that for every 1 kg/m2 increase in BFMI, there was a 6.2cm2 increase in subcutaneous fat (adjusted for age and sex). Conclusions: This study supports the reliability and validity of using thoracic CT to measure muscle and fat. Future studies are needed to investigate the relationship between body composition using this method and clinical outcomes in individuals with advanced lung disease. (Table Presnted).},
address = {S. Mathur, Department of Physical Therapy, University of Toronto, Toronto, ON, Canada},
author = {Mathur, S and Verweel, L and Rozenberg, D and Singer, L G},
