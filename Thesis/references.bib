file = {:Users/gustavosplmoura/Library/Mobile Documents/com$\sim$apple$\sim$CloudDocs/Medicina/2024_Mendeley/Mizuta et al.. 2008. Leptin gene and Leptin receptor gene polymorphisms are associated with sweet preference and Obesity. Hypertension R.pdf:pdf},
issn = {09169636},
journal = {Hypertension Research},
keywords = {Genetic polymorphism,Leptin,Obesity,Taste},
number = {6},
pages = {1069--1077},
pmid = {18716353},
title = {{Leptin gene and Leptin receptor gene polymorphisms are associated with sweet preference and Obesity}},
volume = {31},
year = {2008}
}
