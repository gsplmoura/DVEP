journal = {Shock},
keywords = {C2C12 cell line,Janus kinase 2,STAT3 protein,adult,advanced glycation end product receptor,animal cell,animal experiment,animal model,animal tissue,article,bioinformatics,body weight change,bone injury,controlled study,critical illness myopathy,differential gene expression,fracture healing,grip strength,hematological parameters,histochemistry,human,human tissue,immune response,immunofluorescence,immunohistochemistry,in vitro study,intensive care unit,interleukin 6,interleukin 6 receptor,major clinical study,male,messenger RNA,mouse,muscle mass,muscle weakness,myoblast,myotube,nonhuman,persistent inflammation, immunosuppression and cat,protein expression level,purinergic P2X7 receptor,serum amyloid A,signal transduction,splenomegaly,tibia fracture,toll like receptor 2,toll like receptor 4,transcription factor FOXO,tumor necrosis factor,upregulation},
