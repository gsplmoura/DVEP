@article{Confortin2021,
abstract = {This paper describes the history, objectives and methods used by the nine Brazilian cohorts of the RPS Brazilian Birth Cohorts Consortium (Ribeir{\~{a}}o Preto, Pelotas and S{\~{a}}o Lu{\'{i}}s). Common thematic axes are identified and the objectives, baseline periods, follow-up stages and representativity of the population studied are presented. The Consortium includes three birth cohorts from Ribeir{\~{a}}o Preto, S{\~{a}}o Paulo State (1978/1979, 1994 and 2010), four from Pelotas, Rio Grande do Sul State (1982, 1993, 2004 and 2015), and two from S{\~{a}}o Lu{\'{i}}s, Maranh{\~{a}}o State (1997 and 2010). The cohorts cover three regions of Brazil, from three distinct states, with marked socioeconomic, cultural and infrastructure differences. The cohorts were started at birth, except for the most recent one in each municipality, where mothers were recruited during pregnancy. The instruments for data collection have been refined in order to approach different exposures during the early phases of life and their long-term influence on the health-disease process. The investigators of the nine cohorts carried out perinatal studies and later studied human capital, mental health, nutrition and precursor signs of noncommunicable diseases. A total of 17,636 liveborns were recruited in Ribeir{\~{a}}o Preto, 19,669 in Pelotas, and 7,659 in S{\~{a}}o Lu{\'{i}}s. In the studies starting during pregnancy, 1,400 pregnant women were interviewed in Ribeir{\~{a}}o Preto, 3,199 in Pelotas, and 1,447 in S{\~{a}}o Lu{\'{i}}s. Different strategies were employed to reduce losses to follow-up. This research network allows the analysis of the incidence of diseases and the establishment of possible causal relations that might explain the health outcomes of these populations in order to contribute to the development of governmental actions and health policies more consistent with reality.},
author = {Confortin, Susana Cararo and Ribeiro, Mariz{\'{e}}lia Rodrigues Costa and Barros, Alu{\'{i}}sio J.D. and Menezes, Ana Maria Baptista and Horta, Bernardo L. and Victora, Cesar Gomes and Barros, Fernando C. and Gon{\c{c}}alves, Helen and Bettiol, Heloisa and {Dos Santos}, In{\'{a}} Silva and Barbieri, Marco Antonio and {Da Concei{\c{c}}{\~{a}}o Pereira Saraiva}, Maria and {De Britto e Alves}, Maria Teresa Seabra Soares and {Da Silveira}, Mari{\^{a}}ngela Freitas and Domingues, Marlos Rodrigues and Lima, Nat{\'{a}}lia Peixoto and Rocha, Paulo Ricardo Higassiaraguti and Cavalli, Ricardo Carvalho and Batista, Ros{\^{a}}ngela Fernandes Lucena and Cardoso, Viviane Cunha and Sim{\~{o}}es, Vanda Maria Ferreira and {Da Silva}, Ant{\^{o}}nio Augusto Moura},
doi = {10.1590/0102-311X00093320},
file = {:Users/gustavosplmoura/Library/Mobile Documents/com$\sim$apple$\sim$CloudDocs/Medicina/2024_Mendeley/Confortin et al.. 2021. RPS Brazilian Birth Cohort Consortium (Ribeir{\~{a}}o Preto, Pelotas and S{\~{a}}o Lu{\'{i}}s) History, objectives and methods..pdf:pdf},
issn = {16784464},
journal = {Cadernos de Saude Publica},
keywords = {Cohort Studies,Indicators of Morbidity and Mortality,Noncommunicable Diseases,Risk Factors},
number = {4},
pmid = {33950086},
title = {{RPS Brazilian Birth Cohort Consortium (Ribeir{\~{a}}o Preto, Pelotas and S{\~{a}}o Lu{\'{i}}s): History, objectives and methods}},
volume = {37},
year = {2021}
}
@article{Muniz2023,
abstract = {Background: Few studies have assessed dietary patterns (DPs) and the changes in these patterns over time in adults. The present study aimed to investigate whether possible changes in DPs in two assessments are associated with obesity and excess body fat. Methods: A prospective study was conducted in which data were collected from 1082 adults of a Brazilian birth cohort during two periods 15 years apart (T1: 2002–2004; T2: 2016–2017). Food consumption was assessed in both periods using validated food frequency questionnaires. Three similar DPs were found in the two assessments, and adherence to these patterns was classified as prudent, risk or mixed. Nine DPs changes were defined. At T2, subjects with a body mass index ≥ 30.0 kg m–² were classified as obese, and men and women with a body fat (BF) percentage ≥ 25.0 and ≥ 35.0, respectively, were classified as having excess BF. A directed acyclic graph was built to adjust the association for confounding variables. Results: At T2, 34.4% of the subjects were obese and 61.4% had excess BF. In the adjusted analysis, the changes associated with obesity and excess BF were prudent–mixed (prevalence ratio [PR] 1.55; 95% confidence interval [CI] = 1.04–2.29 and PR = 1.35; 95% CI = 1.10–1.65), risk–risk (PR = 1.49; 95% CI = 1.03–2.13 and PR = 1.27; 95% CI = 1.04–1.53), risk–mixed (PR = 1.56; 95% CI = 1.05–2.31 and PR = 1.33; 95% CI = 1.07–1.63) and mixed–risk (PR = 1.61; 95% CI = 1.10–2.35 and PR = 1.29; 95% CI = 1.04–1.58). Conclusions: A decline in food quality over time or stagnation in an unhealthy DP can lead to obesity and excess BF.},
author = {Muniz, Stephanie C.R.S. and Barbieri, Marco A. and Sanches, Gabrielle F. and Arruda, Soraia P.M. and da Silva, Ant{\^{o}}nio A.M. and Sartorelli, Daniela S. and Bettiol, Helo{\'{i}}sa},
doi = {10.1111/jhn.13049},
file = {:Users/gustavosplmoura/Library/Mobile Documents/com$\sim$apple$\sim$CloudDocs/Medicina/2024_Mendeley/Muniz et al.. 2023. Dietary pattern changes, obesity and excess body fat in adults of a Brazilian birth cohort. Journal of Human Nutriti.pdf:pdf},
issn = {1365277X},
journal = {Journal of Human Nutrition and Dietetics},
keywords = {body composition,cohort study,dietary assessment,dietary change,eating patterns,food intake,obesity,study design and analysis},
number = {1},
pages = {191--202},
pmid = {35722666},
title = {{Dietary pattern changes, obesity and excess body fat in adults of a Brazilian birth cohort}},
volume = {36},
year = {2023}
}
@article{Oliveira2023,
abstract = {Obesity is a recognized risk factor for the development of cardiometabolic outcomes. Therefore, it is essential to evaluate anthropometric and body composition indicators used for its diagnosis. This study aimed to assess the diagnostic performance of body fat percentage (BF%), fat mass index (FMI) and body mass index (BMI) for detecting cardiometabolic outcomes in adults. A cross-sectional study was conducted involving adults at 30 years of age from Pelotas, RS (n = 3517) and at 37–39 years from Ribeir{\~{a}}o Preto, SP (n = 1696). Receiver operating characteristic (ROC) curves were used to determine the cut-off points for predicting cardiometabolic risk factors, including altered blood pressure, blood glucose, triglycerides, total cholesterol, low-density lipoprotein cholesterol (LDL-c), high-density lipoprotein cholesterol (HDDL-c), C-reactive protein and glycated hemoglobin. The cut-off points of BF% ranged from 25.2 to 27.8 in men and from 37.4 to 39.7 in women at 30 years, and from 26.1 to 27.8 in men and from 38.5 to 42.2 in women at 37–39 years. For FMI (kg/m2), the cut-off points ranged from 6.3 to 7.5 in men and from 9.5 to 10.8 in women at 30 years, and from 7.3 to 7.8 in men and from 10.2 to 12.2 in women at 37–39 years. The BMI cut-off points (kg/m2) ranged from 26.3 to 27.3 in men and from 25.4 to 27.2 in women at 30 years, and from 28.3 to 29.0 in men and from 27.2 to 29.6 in women at 37–39 years. The areas under the curve were similar for the three indicators, ranging from 0.523 to 0.746. BMI showed a performance similar to that of the body fat-based indicators in identifying cardiometabolic outcomes. The cut-off points of the three indicators showed acceptable discriminatory power in subjects with cardiometabolic risk factors.},
author = {de Oliveira, Bianca Rodrigues and Magalh{\~{a}}es, Elma Izze da Silva and Bragan{\c{c}}a, Maylla Luanna Barbosa Martins and Coelho, Carla Cristine Nascimento da Silva and Lima, Nat{\'{a}}lia Peixoto and Bettiol, Heloisa and Barbieri, Marco Ant{\^{o}}nio and Cardoso, Viviane Cunha and dos Santos, Alcione Miranda and Horta, Bernardo Lessa and da Silva, Ant{\^{o}}nio Augusto Moura},
doi = {10.3390/nu15132974},
file = {:Users/gustavosplmoura/Library/Mobile Documents/com$\sim$apple$\sim$CloudDocs/Medicina/2024_Mendeley/Oliveira et al.. 2023. Performance of Body Fat Percentage, Fat Mass Index and Body Mass Index for Detecting Cardiometabolic Outcomes in.pdf:pdf},
issn = {20726643},
journal = {Nutrients},
keywords = {ROC curve,body fat percentage,body mass index,cardiometabolic risk factors,cut-off,fat mass index},
number = {13},
pmid = {37447300},
title = {{Performance of Body Fat Percentage, Fat Mass Index and Body Mass Index for Detecting Cardiometabolic Outcomes in Brazilian Adults}},
volume = {15},
year = {2023}
}
@misc{BRISA2024,
abstract = {Coorte BRISA O projeto “Fatores etiol{\'{o}}gicos do nascimento pr{\'{e}}-termo e fatores perinatais na sa{\'{u}}de da crian{\c{c}}a: coortes de nascimentos em duas cidades brasileiras”, financiado pela FAPESP (Processo No 2008/53593-0) em suas fases iniciais, est{\'{a}} sendo realizado nas cidades de Ribeir{\~{a}}o Preto (RP) em S{\~{a}}o Paulo e S{\~{a}}o Lu{\'{i}}s (SL) no Maranh{\~{a}}o. O estudo BRISA Ribeir{\~{a}}o Preto {\'{e}} conduzido pela equipe do N{\'{u}}cleo de Estudos da Sa{\'{u}}de da Crian{\c{c}}a e do Adolescente (NESCA), um centro de estudos da Faculdade de Medicina de Ribeir{\~{a}}o Preto/USP. Atualmente, o projeto {\'{e}} coordenado pela Profa. Dra. Viviane Cunha Cardoso, Profa. Dra. Heloisa Bettiol, Prof. Dr. Marco Antonio Barbieri, Profa. Dra. Maria da Concei{\c{c}}{\~{a}}o Saraiva e pelo Prof. Dr. Ricardo Cavalli. O BRISA se caracteriza por ser um estudo de coorte, isto {\'{e}}, um estudo observacional onde os participantes s{\~{a}}o acompanhados ao longo do tempo para que seja poss{\'{i}}vel avaliar desfechos relacionados {\`{a}} sa{\'{u}}de. A coorte BRISA foi iniciada em 2010, com o objetivo de acompanhar, ao longo de v{\'{a}}rias etapas da vida, crian{\c{c}}as nascidas nesse ano nas cidades de Ribeir{\~{a}}o Preto/SP e S{\~{a}}o Lu{\'{i}}s/MA. O BRISA se subdivide em dois grandes projetos: o BRISA pr{\'{e}}-natal e o BRISA nascimento. Vamos apresentar a seguir a coorte BRISA Ribeir{\~{a}}o Preto. Objetivos do BRISA O principal objetivo do BRISA {\'{e}} estudar fatores de risco para o nascimento pr{\'{e}}-termo (antes de 37 semanas de gesta{\c{c}}{\~{a}}o) e suas consequ{\^{e}}ncias ao longo da vida. Para isso, hip{\'{o}}teses que relacionam altera{\c{c}}{\~{o}}es em alguns sistemas do organismo materno que funcionam de modo altamente interligado (como o sistema neurol{\'{o}}gico e o endocrinol{\'{o}}gico) e que podem repercutir na sa{\'{u}}de do feto levando ao nascimento pr{\'{e}}-termo foram testadas. Essas altera{\c{c}}{\~{o}}es foram tamb{\'{e}}m avaliadas em associa{\c{c}}{\~{a}}o com uma s{\'{e}}rie de condi{\c{c}}{\~{o}}es durante a gesta{\c{c}}{\~{a}}o, como viol{\^{e}}ncia dom{\'{e}}stica e familiar, experi{\^{e}}ncia de discrimina{\c{c}}{\~{a}}o racial, presen{\c{c}}a de estresse na gesta{\c{c}}{\~{a}}o, infec{\c{c}}{\~{o}}es maternas, infec{\c{c}}{\~{a}}o bucal materna, n{\'{i}}veis de algumas prote{\'{i}}nas liberadas no sangue da m{\~{a}}e e varia{\c{c}}{\~{o}}es em genes da m{\~{a}}e que podem desencadear parto pr{\'{e}}-termo. Isso tudo foi avaliado na coorte do pr{\'{e}}-natal. Na coorte do nascimento, foram estimadas as taxas de baixo peso ao nascer, nascimento pr{\'{e}}-termo e restri{\c{c}}{\~{a}}o de crescimento intrauterino e da mortalidade infantil, com o intuito de realizar compara{\c{c}}{\~{o}}es com as coortes anteriores estudadas em Ribeir{\~{a}}o Preto (1978/79 e 1994) e avaliar os fatores associados a essas condi{\c{c}}{\~{o}}es. Al{\'{e}}m disso, estimou-se a preval{\^{e}}ncia de infec{\c{c}}{\~{a}}o cong{\^{e}}nita por citomegalov{\'{i}}rus (CMV), um v{\'{i}}rus muito comum na popula{\c{c}}{\~{a}}o e que pode causar algumas manifesta{\c{c}}{\~{o}}es em rec{\'{e}}m-nascidos, como dificuldade de audi{\c{c}}{\~{a}}o. No seguimento dessas coortes, realizado ap{\'{o}}s um ano de vida, os objetivos foram avaliar as repercuss{\~{o}}es das condi{\c{c}}{\~{o}}es de nascimento sobre as caracter{\'{i}}sticas da sa{\'{u}}de geral dessas crian{\c{c}}as, tais como asma, alergia, obesidade, interna{\c{c}}{\~{o}}es hospitalares, c{\'{a}}ries dent{\'{a}}rias, neurodesenvolvimento, entre outros. – BRISA Pr{\'{e}}-Natal O “BRISA Pr{\'{e}}-Natal” em Ribeir{\~{a}}o Preto foi iniciado em fevereiro de 2010 e contou com a participa{\c{c}}{\~{a}}o de 1400 gestantes, entre a 22{\textordfeminine} e 25{\textordfeminine} semana de gesta{\c{c}}{\~{a}}o. Nesta fase foram realizadas as seguintes avalia{\c{c}}{\~{o}}es: aplica{\c{c}}{\~{a}}o de question{\'{a}}rios sociodemogr{\'{a}}ficos, question{\'{a}}rios sobre aspectos psicol{\'{o}}gicos, sociais e relativos a sa{\'{u}}de em geral, coleta de amostra de sangue e material vaginal, ultrassonografia, medida de peso e altura e exame odontol{\'{o}}gico. Com os dados obtidos foi poss{\'{i}}vel caracterizar a popula{\c{c}}{\~{a}}o estudada e realizar avalia{\c{c}}{\~{a}}o de alguns aspectos da sa{\'{u}}de da gestante e fatores de risco para o nascimento pr{\'{e}}-termo. As crian{\c{c}}as do BRISA Pr{\'{e}}-Natal de Ribeir{\~{a}}o Preto nasceram entre abril de 2010 e junho de 2011. Nessa ocasi{\~{a}}o, foram coletadas informa{\c{c}}{\~{o}}es do nascimento (por meio de consulta aos prontu{\'{a}}rios) de 1370 crian{\c{c}}as ainda na maternidade. Ap{\'{o}}s as crian{\c{c}}as completarem 13 meses de idade, de 2011 a 2013, foi realizado o primeiro seguimento das crian{\c{c}}as, tendo participado desta etapa 1007 crian{\c{c}}as. Nesta ocasi{\~{a}}o foram aplicados question{\'{a}}rios com a m{\~{a}}e/respons{\'{a}}vel relativos {\`{a}} sa{\'{u}}de mental, h{\'{a}}bitos de vida e m{\'{e}}todos de contracep{\c{c}}{\~{a}}o, question{\'{a}}rios sobre a alimenta{\c{c}}{\~{a}}o e sa{\'{u}}de geral das crian{\c{c}}as, coletado sangue da crian{\c{c}}a, realizado exame odontol{\'{o}}gico e avalia{\c{c}}{\~{o}}es sobre o desenvolvimento infantil (Escalas Bayley), al{\'{e}}m de medidas de peso e altura e coleta de informa{\c{c}}{\~{o}}es socioecon{\^{o}}micas e demogr{\'{a}}ficas. – BRISA Nascimento Em 2010, iniciou-se o “BRISA Nascimento”: entre 1{\textordmasculine} de janeiro e 31 de dezembro de 2010, durante todos os dias, todas as maternidades de Ribeir{\~{a}}o Preto foram visitadas e todas as m{\~{a}}es convidadas a participarem do estudo. Nesta fase, foi registrada a participa{\c{c}}{\~{a}}o de 7752 crian{\c{c}}as. A coorte de nascimento identificou os nascimentos hospitalares que ocorreram em todos os servi{\c{c}}os de maternidade, p{\'{u}}blicos ou privados. Ap{\'{o}}s as crian{\c{c}}as completarem 13 meses, parte das m{\~{a}}es foram convidadas a participar de uma nova etapa do BRISA, de reavalia{\c{c}}{\~{a}}o das crian{\c{c}}as (etapa do seguimento). Em Ribeir{\~{a}}o Preto foram avaliadas nesta ocasi{\~{a}}o 4182 crian{\c{c}}as da coorte de nascimento, tendo sido feitas as mesmas avalia{\c{c}}{\~{o}}es realizadas no pr{\'{e}}-natal. Atualiza{\c{c}}{\~{a}}o de Contatos No per{\'{i}}odo de mar{\c{c}}o a julho de 2020 foi realizada a atualiza{\c{c}}{\~{a}}o e confirma{\c{c}}{\~{a}}o de contatos das m{\~{a}}es e familiares. Nesse per{\'{i}}odo, foi realizada uma busca ativa de todas as m{\~{a}}es participantes de alguma etapa anterior do BRISA. Dessa forma, foi poss{\'{i}}vel garantir maior agilidade no agendamento das avalia{\c{c}}{\~{o}}es das crian{\c{c}}as do BRISA para a etapa seguinte, a Fase Escolar. Para atualiza{\c{c}}{\~{a}}o, a equipe entrou em contato com milhares de m{\~{a}}es pelo WhatsApp, telefone fixo e redes sociais. O in{\'{i}}cio das avalia{\c{c}}{\~{o}}es da etapa Fase escolar estava previsto para fevereiro de 2020, no entanto, devido {\`{a}} pandemia da COVID-19, o contato presencial com as crian{\c{c}}as e respons{\'{a}}veis foi temporariamente suspenso. Enquanto as avalia{\c{c}}{\~{o}}es presenciais n{\~{a}}o foram poss{\'{i}}veis, o Projeto realizou duas avalia{\c{c}}{\~{o}}es online, BRISA WhatsApp e BRISA WEB. O BRISA WhatsApp avaliou por meio de question{\'{a}}rios aplicados via WhatsApp entre os meses de agosto e novembro de 2020. Os question{\'{a}}rios avaliaram o bem-estar de m{\~{a}}es e crian{\c{c}}as durante o per{\'{i}}odo de maior isolamento social. No total, 461 fam{\'{i}}lias participaram desta avalia{\c{c}}{\~{a}}o. O BRISA Web avaliou como a pandemia afetou a vida das crian{\c{c}}as por meio de entrevistas de aproximadamente 30 minutos realizadas por liga{\c{c}}{\~{a}}o telef{\^{o}}nica ou Google Meet. No total, 1033 fam{\'{i}}lias foram entrevistadas. {\'{U}}LTIMA ETAPA DE AVALIA{\c{C}}{\~{O}}ES: BRISA FASE ESCOLAR Neste ano, a coorte BRISA finalizou suas avalia{\c{c}}{\~{o}}es da etapa de acompanhamento chamada fase escolar. As avalia{\c{c}}{\~{o}}es iniciaram efetivamente em mar{\c{c}}o de 2022 e foram finalizadas em julho de 2023. Nesta etapa, aos 11-13 anos, os adolescentes que nasceram entre janeiro de 2010 e junho de 2011 em Ribeir{\~{a}}o Preto realizaram exames e avalia{\c{c}}{\~{o}}es de sa{\'{u}}de geral e bucal (TOTALMENTE GRATUITOS!), realizados no pr{\'{e}}dio da Unidade de Pesquisa Cl{\'{i}}nica do Hospital das Cl{\'{i}}nicas da FMRP/USP (localizado na Rua Tenente Cat{\~{a}}o Roxo, 2701 – pr{\'{o}}ximo ao HC Campus). Os participantes tiveram direito a realiza{\c{c}}{\~{a}}o de avalia{\c{c}}{\~{o}}es com equipes de nutricionistas, dentistas, fisioterapeutas e psic{\'{o}}logos. Foram realizadas an{\'{a}}lises de composi{\c{c}}{\~{a}}o corporal (percentual de gordura e massa {\'{o}}ssea), antropometria (medidas de peso, altura, circunfer{\^{e}}ncias), question{\'{a}}rios de sa{\'{u}}de mental e consumo alimentar, al{\'{e}}m de investiga{\c{c}}{\~{o}}es sobre a sa{\'{u}}de respirat{\'{o}}ria, for{\c{c}}a muscular, qualidade do sono, press{\~{a}}o arterial e sa{\'{u}}de dos vasos sangu{\'{i}}neos. No total, 3263 adolescentes participaram presencialmente da coorte BRISA na fase escolar! Import{\^{a}}ncia do BRISA Todas as fases da coorte BRISA contribu{\'{i}}ram e continuam contribuindo muito com o avan{\c{c}}o do conhecimento sobre nossa popula{\c{c}}{\~{a}}o e condi{\c{c}}{\~{o}}es de sa{\'{u}}de. V{\'{a}}rios resultados j{\'{a}} foram publicados, outros est{\~{a}}o em an{\'{a}}lise e mais dados futuramente ser{\~{a}}o utilizados para compara{\c{c}}{\~{o}}es com os resultados da atual etapa “BRISA Fase Escolar”. Al{\'{e}}m da publica{\c{c}}{\~{a}}o de trabalhos em revistas cient{\'{i}}ficas com resultados encontrados no BRISA, estamos divulgando informa{\c{c}}{\~{o}}es muito importantes deste estudo para a popula{\c{c}}{\~{a}}o, por meio de nossas m{\'{i}}dias sociais (Facebook e Instagram). Sigam nossas p{\'{a}}ginas e saibam como sua participa{\c{c}}{\~{a}}o tem sido fundamental para o estudo da sa{\'{u}}de de nossas crian{\c{c}}as. O estudo se prop{\~{o}}e a investigar poss{\'{i}}veis fatores associados ao crescimento e desenvolvimento das crian{\c{c}}as da coorte BRISA, explorando e utilizando instrumentos e medidas de indicadores de desenvolvimento de doen{\c{c}}as cr{\^{o}}nicas ainda pouco utilizados e investigados em crian{\c{c}}as na idade escolar. O BRISA pretende ainda investigar aspectos comportamentais e de transtornos do neurodesenvolvimento associados aos h{\'{a}}bitos de vida da gestante no per{\'{i}}odo pr{\'{e}}-natal, da crian{\c{c}}a no in{\'{i}}cio da vida e no presente. Ningu{\'{e}}m esperava por isso, mas enquanto est{\'{a}}vamos preparando o Projeto “BRISA Fase Escolar” para ir a campo nesta nova etapa, aconteceu a pandemia causada pelo novo coronav{\'{i}}rus, chamado SARS-Cov-2, que causa a doen{\c{c}}a do coronav{\'{i}}rus, chamada COVID-19 . Essa pandemia causou e tem causado um grande impacto na vida de todos n{\'{o}}s, interferindo nas atividades di{\'{a}}rias, no emprego, na sa{\'{u}}de e na perda de entes queridos em algumas fam{\'{i}}lias. As crian{\c{c}}as tiveram que se afastar da escola e muitas n{\~{a}}o puderam ter contato com seus amigos e com parentes queridos, como tios e av{\'{o}}s. Devido a isso, naquele per{\'{i}}odo, prorrogamos o in{\'{i}}cio da fase escolar. Quando tudo se acalmou, retomamos as atividades do projeto e tamb{\'{e}}m procuramos avaliar como essa pandemia influenciou o bem estar das crian{\c{c}}as e das fam{\'{i}}lias do BRISA. Assim, ser{\'{a}} poss{\'{i}}vel formular propostas para lidar com esses problemas. Mais um motivo pelo qual a participa{\c{c}}{\~{a}}o de voc{\^{e}}s continua sendo muito importante!},
author = {BRISA},
booktitle = {NESCA – N{\'{u}}cleo de Estudos da Sa{\'{u}}de da Crian{\c{c}}a e do Adolescente Faculdade de Medicina de Ribeir{\~{a}}o Preto},
title = {{COORTE BRISA}},
urldate = {2024-07-25},
year = {2024}
}
