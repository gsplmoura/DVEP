language = {English},
title = {{Of mice and men: Laboratory murine models for recapitulating the immunosuppression of human sepsis}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2018703375&from=export http://dx.doi.org/10.3389/fimmu.2022.956448},
