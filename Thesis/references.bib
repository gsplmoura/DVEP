volume = {31},
year = {2023}
}
@article{Braulke2023,
abstract = {Purpose: Guidelines recommend a structured symptom screening (SC) for especially advanced cancer patients (CPs). The aim of this multicenter German prospective quality assurance project KeSBa (Kennzahl Symptom- und Belastungserfassung) was to gain knowledge on SC procedures in Oncology Centers (OCs) for advanced cancer patients and a first impression on the consequences of SC. Methods: The KeSBa project consisted of three phases: pilot, 3 months screening and feedback phase. Participating OCs decided to use either the Minimal Documentation System (MIDOS) or the Integrated Palliative care Outcome Scale (IPOS) and defined the cutoff values for positive screening results. Results: Out of 172 certified German OCs, 40 (23%) participated in the KeSBa pilot phase, 29 (16.8%) in the 3 months screening phase using MIDOS (n = 18, 58.6%) or IPOS (n = 11, 41.3%) and in the feedback round. 25/29 performed paper-based screening (86.2%). 2.963 CPs were screened. Results were documented for 1255 (42.2%, SC +) positive and 874 (29.5%, SC–) negative screenings depending on the center´s schedules: 452 SC + CPs (28.4%) and 42 SC– CPs (2.6%) had contact to specialized palliative care or other supportive specialist teams afterwards, 458 SC + CPs (28.8%) and 605 SC– CPs (38.1%) remained in standard oncology care. In the feedback round missing resources (personal and IT) and improved communication were mentioned most often. Conclusion: Routine SC is feasible in advanced CPs treated in OCs but associated with considerable workload. In 42.2% of CPs SC was classified as positive, indicating the need of further diagnostics or professional judgment. SC requires staff and IT resources.},
author = {Braulke, Friederike and Para, Servet and Alt-Epping, Bernd and Tewes, Mitra and B{\"{a}}umer, Markus and Haberland, Birgit and Mayer-Steinacker, Regine and Hopprich, Anne and de Wit, Maike and Grabe, Michaela and Bender-S{\"{a}}belkampf, Sophia and We{\ss}ling, Caroline and Aulmann, Christoph and Gerlach, Christina and Regincos, Pascale and Fischer, Ferdinand and Haarmann, Soraya and Huys, Tatjana and Drygas, Sabine and Rambau, Anett and Kiani, Alexander and Schnabel, Astrid and Buhl, Christoph and Seipke, Stefanie and Hiemer, Sonja and Polata, Silke and Me{\ss}mann, Maximilian and Hansmeier, Anna and Anastasiadou, Louiza and Letsch, Anne and Wecht, Daniel and Hellberg-Naegele, Matthias and Krug, Utz and Wedding, Ulrich and van Oorschot, Birgitt},
