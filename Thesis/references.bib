author = {K{\'{a}}rp{\'{a}}thi, K and Holndonner-Kirst, E and Keszty{\"{u}}s, V and K{\'{o}}ph{\'{a}}zi, L K and Tak{\'{a}}cs, L and Biro, E and Kocsis, F and Havasi, A and Guly{\'{a}}s, E and G{\'{a}}l, J and Hermann, C},
