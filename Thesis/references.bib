volume = {91-92},
year = {2021}
}
@article{Gincaite2016,
abstract = {Rationale: Even though bioelectrical impedance analysis (BIA) derived phase angle (PA) is used to define nutritional state of the patients, there are no definite PA cut-off value of malnutrition. Therefore aim of this study is to determine the malnutrition specific cut-off value of the BIA derived PA. Methods: We conducted an observational study in a tertiary reference hospital. Nutritional state of 99 ESRD patients was examined post HD procedure using Subjective Global Assessment scale (SGA) and BIA derived PA. Values of PA were standardized and stratified by age and gender using population reference values. Patients were split into groups with PA values below 5th through 50th percentile of the mean reference value. The relationship between these groups and SGA defined malnutrition were evaluated using ROC AUC and the most accurate percentile was detected. Results: Mean age of the patients was 58.7 ± 14.4 years, half of them were men 51 (51.4%) with mean time on HD for 6.1 ± 5.6 years. The SGA rates of nutrition were as follows: well-nourished 56 (56.6%), malnourished 28 (28.3%) and severely malnourished 14 (12.2%). Mean PA was 4.87 ± 1.08 SD, and rates of patients in different percentile groups were as follows: <5th = 39 (39.4%), <10th = 53 (53.5%), <15th = 59 (59.6%), <20th = 62 (62.6%), <25th = 70 (70.7%), <30th = 74 (74.7%), <35th = 75 (75.8%), <40th = 79 (79.8%), <45th = 83 (83.8%), <50th = 87 (87.9%). The cut-off of 10th percentile was revealed as the most accurate with ROC AUC of 0.73 CI95% 0.61-0.85 p < 0.001 for SGA severe malnutrition and ROC AUC of 0.68 CI95% 0.59-0.8 p = 0.001 for SGA moderate and severe malnutrition. Conclusion: Malnutrition is present in almost half of the HD patients. BIA provided and standardized PA is an accurate and potent marker of malnutrition. The malnutrition cut-off percentile we report in our study must be further evaluated in upcoming studies before implementation into clinical practice.},
address = {A. Gincaite, Faculty of Medicine, Vilnius University, Vilnius, Lithuania},
author = {Gincaite, A and Vicka, V and Rimsevicius, L and Sukackiene, D and Miglinas, M},
issn = {1532-1983},
journal = {Clinical Nutrition},
keywords = {adult,clinical practice,end stage renal disease,female,gender,hemodialysis patient,hospital,human,impedance,major clinical study,male,malnutrition,middle aged,nutritional status,observational study,reference value},
language = {English},
pages = {S90},
title = {{Bioelectrical impedance analysis phase angle: Marker of malnutrition in haemodialysis patients}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L614798277&from=export},
volume = {35},
year = {2016}
}
@article{Karagiannis2020,
abstract = {Rationale: Nutritional status can been evaluated by various objective measures, including anthropometric and laboratory parameters. Bioelectrical impedance analysis (BIA) is an easy-to-use, non-invasive and reproducible method for evaluating changes in body composition that reflect changes in nutritional status. BIA-derived phase angle (PhA) is considered a biological marker of cellular health and has been proven to be a predictor of morbidity and mortality in a variety of patient populations including cancer patients. The purpose of this study was to evaluate the association between BIA-derived phase angle (PhA) and overall 30 day post operative complications in patients with pancreatic cancer undergoing pancreaticoduodenectomy. Methods: A prospective study was conducted in a single center in Athens, Greece. A series of 89 patients (aged 40-81 year, 40.2% women) with pancreatic cancer undergoing pancreaticoduodenectomy between November 2016 and February 2020 were included in the study. Data including patients' characteristics, laboratory results, and 30 day post operative complications were recorded during the perioperative period. BIA was performed using a multifrequency bioelectrical impedance analyser (InBody S10; Korea), with patients lying supine on a bed or examination table, with legs apart and arms not touching the torso. Potential associations between PhA values and postoperative complications were analyzed using generalized linear models adjusted for age, BMI, serum albumin levels, gender and total bilirubin. Results: Median PhA values were 5.9 (4.2 – 7.9), and their distribution had no association with demographic and clinical factors such as sex, disease stage; however, it was significantly associated with age. A total of 32 patients (35,9%) suffered from postoperative complications (19 cases of pancreatic fistulas grade A & B and 13 cases of delayed gastric emptying). There were significant differences in the postoperative complications incidence between younger and older individuals (>65 years) (p=0.016), those with short and longer hospital stays (>12 days) (p=0.007), and between individuals who survived and those who did not (p=0.002). After adjustments for age, sex, BMI, serum albumin levels and total caloric intake, it was found that individuals in the lowest tertile of PhA had a doubled risk of post operative complications compared with individuals in the upper tertile of PhA (RR=2.01; 95% CI 1.21-2.82, p=0.004). Conclusion: PhA values among patients with pancreatic cancer undergoing pancreaticoduodenectomy may serve as a significant predictor of 30 day postoperative complications incidence. Disclosure of Interest: None declared},
author = {Karagiannis, D and Katralis, P and Pafili, Z and Bouloubasi, Z and Evangelatou, I and Almperti, A and George, S},
doi = {10.1016/j.clnesp.2020.09.114},
issn = {2405-4577},
journal = {Clinical Nutrition ESPEN},
keywords = {Greece,Korea,adult,age,aged,analyzer,bilirubin,body mass,caloric intake,cancer patient,cancer staging,cancer surgery,cancer survival,complication,conference abstract,controlled study,demography,examination table,female,gender,hospitalization,human,impedance,incidence,leg,major clinical study,male,pancreas cancer,pancreas fistula,pancreaticoduodenectomy,perioperative period,postoperative complication,prospective study,protein expression,serum albumin,stomach paresis,surgery,trunk},
language = {English},
pages = {442},
title = {{Bioelectrical impedance analysis-derived phase angle as a predictor of 30 day postoperative complication incidence among patients with pancreatic cancer undergoing pancreaticoduodenectomy.}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2008505907&from=export http://dx.doi.org/10.1016/j.clnesp.2020.09.114},
volume = {40},
year = {2020}
}
@article{Huang2020,
abstract = {OBJECTIVE: Phase angle (PA) determined by bioelectrical impedance analysis has been suggested to be a predictor of death in a variety of disease conditions, but its associations with outcomes have not yet been assessed in a large continuous ambulatory peritoneal dialysis (CAPD) patient cohort. The aim of the present study was to examine the association of PA with risks for all-cause and cardiovascular death in patients treated with CAPD. METHODS: Incident CAPD patients were enrolled from January 1, 2010 to December 31, 2015 and were followed until December 31, 2017. Multifrequency bioelectrical impedance analysis was conducted in the morning with patients in a fasting state. Multivariable linear regression analyses were performed to study the relationships between PA and other variables. Cox proportional hazard models were used to evaluate the association between PA and mortality. RESULTS: A total of 760 incident CAPD patients were enrolled in this study. Patients have a median PA value of 4.59° ranging from 2.30° to 7.22°. Aging, presence of diabetes mellitus, and fluid overload were independently associated with lower PA, whereas male sex, higher body mass index, higher serum levels of albumin and creatinine, and better residual renal function were independently associated with higher PA in a multivariable linear regression model. A total of 125 (16.4%) patients died during a median follow-up of 42 months. In the Cox model with adjustment for confounders, PA was significantly associated with all-cause and cardiovascular mortality in incident CAPD patients (hazard ratio, 0.584; 95% confidence interval, 0.403 to 0.844, P = .004; hazard ratio, 0.597; 95% confidence interval, 0.359 to 0.993, P = .047, respectively). CONCLUSIONS: PA reflected a combined dimension of the illness including deranged hydration status and nutritional status. Lower PA was associated with both all-cause and cardiovascular death in patients with CAPD.},
author = {Huang, R and Wu, M and Wu, H and Ye, H and Peng, Y and Yi, C and Yu, X and Yang, X},
doi = {10.1053/j.jrn.2019.06.006},
issn = {1532-8503},
journal = {Journal of renal nutrition : the official journal of the Council on Renal Nutrition of the National Kidney Foundation},
keywords = {adult,aging,albumin,article,body mass,cardiovascular mortality,cohort analysis,continuous ambulatory peritoneal dialysis,controlled study,creatinine,death,diabetes mellitus,endogenous compound,fasting,follow up,gene expression,human,human tissue,hydration status,impedance,kidney function,linear regression analysis,major clinical study,male,nutritional status,protein blood level,protein expression},
language = {English},
number = {2},
pages = {119--125},
title = {{Lower Phase Angle Measured by Bioelectrical Impedance Analysis Is a Marker for Increased Mortality in Incident Continuous Ambulatory Peritoneal Dialysis Patients}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L629102811&from=export http://dx.doi.org/10.1053/j.jrn.2019.06.006},
volume = {30},
year = {2020}
}
@article{Yamada2022,
abstract = {The phase angle (PhA), measured via bioelectrical impedance analysis, is considered an indicator of cellular health, where higher values reflect higher cellularity, cell membrane integrity, and better cell function. This study aimed to examine the relationship between PhA and exercise habits or objectively measured physical activity. We included 115 people aged 32-69 years. The body composition and PhA were measured using a bioelectrical impedance device. Physical activity and sedentary behavior (SB) were assessed using a triaxial accelerometer. Exercise habits were also obtained through structured interviews, and participants were categorized into the no exercise habit (No-Ex), resistance training exercise habit (RT), or aerobic training exercise habit (AT) groups. Objectively measured moderate-to-vigorous physical activity or step count significantly correlated with PhA, independent of age, sex, height, percent body fat, body cell mass, and leg muscle power. In contrast, SB was not significant determinants of PhA. People who exercised regularly (RT or AT) had significantly higher PhA values than did those in the No-Ex group. Furthermore, the PhA was not significantly different between the RT and AT groups. Regularly engaging in exercise with moderate-to-vigorous intensity may improve or maintain muscle cellular health and muscle quality.},
author = {Yamada, Y and Yoshida, T and Murakami, H and Kawakami, R and Gando, Y and Ohno, H and Tanisawa, K and Konishi, K and Julien, T and Kondo, E and Nakagata, T and Nanri, H and Miyachi, M},
