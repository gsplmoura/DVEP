@article{RibeiroFilho2006,
abstract = {A s{\'{i}}ndrome metab{\'{o}}lica (SM) {\'{e}} vista atualmente como uma epidemia mundial, com n{\'{u}}meros alarmantes, associada a alta morbi-mortalidade cardiovascular e elevado custo s{\'{o}}cio-econ{\^{o}}mico. O ganho ponderal {\'{e}} preditor independente para o desenvolvimento da SM, embora nem todos os indiv{\'{i}}duos obesos a apresentem. Por outro lado, certas popula{\c{c}}{\~{o}}es com baixa preval{\^{e}}ncia de obesidade apresentam elevada preval{\^{e}}ncia da SM e mortalidade cardiovascular. A distribui{\c{c}}{\~{a}}o da gordura corporal {\'{e}} relevante, e especificamente a gordura visceral (GV) parece ser o elo entre o tecido adiposo e a resist{\^{e}}ncia {\`{a}} insulina (RI), caracter{\'{i}}stica da SM. Na {\'{u}}ltima d{\'{e}}cada, o tecido adiposo deixou de ser um simples reservat{\'{o}}rio de energia para se transformar num complexo {\'{o}}rg{\~{a}}o com m{\'{u}}ltiplas fun{\c{c}}{\~{o}}es. A GV apresenta caracter{\'{i}}sticas metab{\'{o}}licas diferentes da gordura subcut{\^{a}}nea gl{\'{u}}teo-femoral, as quais favorecem a instala{\c{c}}{\~{a}}o do quadro de RI. Diversos estudos revelam a estreita rela{\c{c}}{\~{a}}o da adiposidade abdominal com a toler{\^{a}}ncia {\`{a}} glicose, hiperinsulinemia, hipertrigliceridemia e hipertens{\~{a}}o arterial. Mais que uma simples associa{\c{c}}{\~{a}}o, recentemente, acredita-se que a GV desempenha um papel central na fisiopatologia da SM. Assim, a quantifica{\c{c}}{\~{a}}o da GV se torna importante para identificar indiv{\'{i}}duos com maior risco para o desenvolvimento da SM, eleitos para sofrer interven{\c{c}}{\~{o}}es precoces na tentativa de reduzir o impacto das anormalidades metab{\'{o}}licas sobre a mortalidade cardiovascular. Este artigo discute particularidades da distribui{\c{c}}{\~{a}}o central de gordura, no contexto da SM, poss{\'{i}}veis mecanismos fisiopatog{\^{e}}nicos relacionados {\`{a}} GV e os m{\'{e}}todos dispon{\'{i}}veis para a avalia{\c{c}}{\~{a}}o da adiposidade abdominal.},
author = {{Ribeiro Filho}, Fernando F. and Mariosa, Lydia S. and Ferreira, Sandra R. G. and Zanella, Maria Teresa},
doi = {10.1590/S0004-27302006000200009},
issn = {0004-2730},
journal = {Arquivos Brasileiros de Endocrinologia & Metabologia},
month = {apr},
number = {2},
pages = {230--238},
title = {{Gordura visceral e s{\'{i}}ndrome metab{\'{o}}lica: mais que uma simples associa{\c{c}}{\~{a}}o}},
url = {http://www.scielo.br/scielo.php?script=sci_arttext&pid=S0004-27302006000200009&lng=pt&tlng=pt},
volume = {50},
year = {2006}
}
@article{Silva2006,
abstract = {O objetivo do presente estudo foi investigar associa{\c{c}}{\~{o}}es entre a distribui{\c{c}}{\~{a}}o centr{\'{i}}peta da gordura corporal e lip{\'{i}}dios-lipoprote{\'{i}}nas s{\'{e}}ricos, press{\~{a}}o arterial e {\'{i}}ndice Homa-IR de resist{\^{e}}ncia insul{\'{i}}nica, mediante ajuste estat{\'{i}}stico de indicadores quanto ao sobrepeso e {\`{a}} aptid{\~{a}}o cardiorrespirat{\'{o}}ria. Foram analisados 89 volunt{\'{a}}rios (44 homens e 45 mulheres). A distribui{\c{c}}{\~{a}}o centr{\'{i}}peta da gordura foi analisada atrav{\'{e}}s de circunfer{\^{e}}ncia da cintura (CC) e o sobrepeso pelo {\'{i}}ndice de massa corporal (IMC). A aptid{\~{a}}o cardiorrespirat{\'{o}}ria foi acompanhada pelo VO2m{\'{a}}x estimado por teste de caminhada. Ap{\'{o}}s ajuste pelos valores de IMC verificamos coeficientes de correla{\c{c}}{\~{a}}o significativos entre CC e n{\'{i}}veis de press{\~{a}}o arterial e ApoB em homens, e entre CC e {\'{i}}ndice Homa-IR e triglicer{\'{i}}dios em mulheres. Ap{\'{o}}s ajuste pelos valores de VO2m{\'{a}}x observamos correla{\c{c}}{\~{o}}es significativas entre CC e ApoB e {\'{i}}ndice Homa-IR em homens, e entre CC e {\'{i}}ndice Homa-IR em mulheres. Conclui-se que, dependendo do sexo, a quantidade e a distribui{\c{c}}{\~{a}}o da gordura corporal podem apresentar a{\c{c}}{\~{o}}es distintas na resist{\^{e}}ncia insul{\'{i}}nica e nas disfun{\c{c}}{\~{o}}es associadas. A aptid{\~{a}}o cardiorrespirat{\'{o}}ria per se parece n{\~{a}}o contribuir na minimiza{\c{c}}{\~{a}}o da associa{\c{c}}{\~{a}}o entre a distribui{\c{c}}{\~{a}}o centr{\'{i}}peta da gordura e o {\'{i}}ndice Homa-IR; por{\'{e}}m, apresenta consider{\'{a}}vel impacto na associa{\c{c}}{\~{a}}o entre a distribui{\c{c}}{\~{a}}o centr{\'{i}}peta da gordura e o metabolismo lip{\'{i}}dico e os n{\'{i}}veis de press{\~{a}}o arterial, sobretudo nos homens.},
author = {da Silva, Jos{\'{e}} Luciano T. and Barbosa, D{\'{e}}cio Sabbatini and de Oliveira, Jair Aparecido and Guedes, Dartagnan Pinto},
doi = {10.1590/S0004-27302006000600009},
issn = {0004-2730},
journal = {Arquivos Brasileiros de Endocrinologia & Metabologia},
month = {dec},
number = {6},
pages = {1034--1040},
title = {{Distribui{\c{c}}{\~{a}}o centr{\'{i}}peta da gordura corporal, sobrepeso e aptid{\~{a}}o cardiorrespirat{\'{o}}ria: associa{\c{c}}{\~{a}}o com sensibilidade insul{\'{i}}nica e altera{\c{c}}{\~{o}}es metab{\'{o}}licas}},
url = {http://www.scielo.br/scielo.php?script=sci_arttext&pid=S0004-27302006000600009&lng=pt&tlng=pt},
volume = {50},
year = {2006}
}
