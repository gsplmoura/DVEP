}
@article{Schonenberger2019,
abstract = {Objectives: Cystic fibrosis (CF) patients are at nutritional risk. In addition to the lung pathology, one of the main pathologic issues is viscous mucus blocking pancreatic ducts, leading to reduced production of pancreatic enzymes. Therefore, maldigestion and consequently malabsorption occurs, particularly of fats and liposoluble vitamins, resulting in steatorrhea, vitamin deficiencies and subsequently malnutrition. We aimed to determine the prevalence of malnutrition and investigate the nutritional status of an adult Swiss CF cohort. Methods: We contrasted CF patients with healthy controls regarding nutritional status and dietary habits in an observational cohort study. Assessment was based on nutritional risk screening (NRS-2002), dietary habits (7-day diet record), body composition (bioelectric impedance analysis, anthropometrics), resting energy expenditure (REE; indirect calorimetry), and physical/mental function (hand grip strength, SF-36v2{\textregistered}). Results: Nineteen patients (15 men, median age 3y) and 15 controls (8 men, median age 52y) were included. Eight patients (42%) were at nutritional risk (NRS-2002 ≥ 3). According to ESPEN guidelines, 8 (42%) patients were malnourished. Energy intake per kg body weight was significantly higher in patients (p =.021). No significant differences arose in REE (p =.451) and estimated energy requirements (p =.202). Energy balance is + 648 kcal/day in patients and + 57 kcal/day in controls (p =.121). Patients had significantly lower BMI (p =.030), body fat percentage (p <.001), and physical/mental health scores (p <.001) than controls. Conclusion: Malnutrition is highly prevalent in this CF cohort. Energy intake and body weight are highly discrepant in this population. In clinical practice, energy requirements of CF patients add up to approximately twice the Harris-Benedict estimation and 1.7 times indirect calorimetry REE. An adequate intake of pancreatic enzyme substitution is crucial indeed.},
author = {Sch{\"{o}}nenberger, K A and Reber, E and Bally, L and Geiser, T and Lin, D and Stanga, Z},
doi = {10.1016/S1569-1993(19)30608-3},
issn = {1873-5010},
journal = {Journal of Cystic Fibrosis},
keywords = {adult,anthropometry,body fat,body mass,body weight,caloric intake,child,clinical article,cohort analysis,conference abstract,controlled study,cystic fibrosis,diet,energy balance,female,grip strength,habit,hand function,human,impedance,indirect calorimetry,lung disease,male,mental function,mental health,middle aged,nutritional assessment,nutritional status,pancreas enzyme,practice guideline,preschool child,prevalence,resting energy expenditure,risk assessment,steatorrhea,vitamin deficiency},
language = {English},
pages = {S146},
title = {{Nutritional Assessment in Adults with Cystic Fibrosis (NACYFI study)}},
