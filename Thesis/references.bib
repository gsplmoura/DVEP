journal = {ABCD. Arquivos Brasileiros de Cirurgia Digestiva (S{\~{a}}o Paulo)},
number = {2},
pages = {124--126},
title = {{S{\'{i}}ndrome de Ogilvie (pseudo-obstru{\c{c}}{\~{a}}o intestinal aguda): relato de caso}},
volume = {22},
year = {2009}
}
