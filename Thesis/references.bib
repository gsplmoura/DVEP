}
@article{Generoso2014,
abstract = {Rationale: The Phase Angle (PA) consists in a tool that detects changes in body composition and cell membrane function, and can be used as an indicator of prognosis in varied populations. So the aim of this research was to evaluate the correlation between PA and outcomes in surgical cancer patients. Methods: Observational study developed from December 2012 to February 2014 in a Public Hospital of Brazil. Patients, of both genders, admitted to elective surgical treatment of cancer of the head and neck and gastrointestinal tract were enrolled in the study. The patients, after signing the informed consent, underwent nutritional assessment in the immediate preoperative period. Nutritional status was evaluated by Subjective Global Assessment (SGA), weight loss percentage, body mass index and body composition by Bioelectrical Impedance, which provided the PA. The outcome variables evaluated were: length stay after operation and in intensive care unit (ICU), hospitalization time as well as mortality. Results: 77 patients, mean age 61.1±11.7 years, 53.2% male were evaluated. According to SGA 43.2% of patients were malnourished. The mean postoperative length stay was 9.1±3.2 days. Around 22.1% of the sample was transferred to the ICU after the operation, with persistence of 3.4±1.8 days. The death occurred in 2.6% of patients. There was significant indirect correlation between PA and postoperative length stay (r= 0.390; p<0.001) and permanence in ICU (r= 0.354, p=0.002). Patients who died had lower PA (4.3±2.5 vs. 5.2±1.8; p=0.001). Conclusion: The PA in surgical cancer patients was associated with worse clinical outcome, suggesting that PA can be used as a prognosis indicator for this population.},
author = {Generoso, S V and Rodrigues, A M S and Maia, F and Armani, B and Costa, A C and Jansen, A K and Correia, M.I.T.D.},
issn = {0261-5614},
journal = {Clinical Nutrition},
keywords = {Brazil,European,body composition,body mass,body weight loss,cancer patient,cell membrane,death,gastrointestinal tract,gender,hospitalization,human,impedance,informed consent,intensive care unit,male,metabolism,mortality,neck,neoplasm,nutrition,nutritional assessment,nutritional status,observational study,outcome variable,patient,population,preoperative period,prognosis,public hospital,society,surgery},
language = {English},
pages = {S53},
title = {{Phase angle as prognostic indicator in surgical cancer patients}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L71801361&from=export},
volume = {33},
year = {2014}
}
@article{Bittencourt2020,
abstract = {Objective: This study verified if the phase angle (PA) formed by the resistance (R) and reactance (Xc) obtained from bioimpedance (BIA) might be a useful tool to identify functionality. Design and Participants: It was conducted a cross-sectional study with 152 women ≥ 65 years old from the community. Measurements: Weight, height, body mass index (BMI), calf circumference (CC), hand grip strength (HGS), gait speed (GS), fat free mass (FFM), body fat (BF), PA with R and Xc from the BIA were measured. Spearman's and Pearson's correlations and the odds ratio (OR) were performed using the IBM SPSS software version 22.0. Results: Sixty-four percent (n=98) women are with PA below the reference. Negative moderate significant correlation was found between PA and age (r =−0.440*; p<0.001). Moderate significant correlation was observed between PA and GS (r = 0.484**; p<0.001). Weak significant correlation was found between PA and HGS (r = 0.177*; p = 0.029). Odds ratio (OR) demonstrated that individuals with PA above the mean value have 4.77 times more chances of having increased GS (confidence interval 2.40–9.48; 95%). Women aged below the mean value have 4.02 times more chances of having higher PA (confidence interval 2.02–7.99; 95%). Younger aged women showed 4.02 times more chances of having higher PA (confidence interval 2.02–7.99; 95%). Conclusions: Phase angle can be associated with functional tests, such as gait speed and hand grip strength, in older women.},
author = {Bittencourt, D C D and Schieferdecker, M E M and Macedo, D S and Biesek, S and {Silveira Gomes}, A R and Rabito, E I},
doi = {10.1007/s12603-020-1324-5},
issn = {1760-4788},
journal = {Journal of Nutrition, Health and Aging},
keywords = {adult,article,body fat,body mass,calf (mammal),controlled study,cross-sectional study,data analysis software,fat free mass,female,grip strength,human,human experiment,major clinical study,nonhuman,walking speed},
language = {English},
number = {3},
pages = {251--254},
title = {{Phase Angle Reflects Loss of Functionality in Older Women}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2004116808&from=export http://dx.doi.org/10.1007/s12603-020-1324-5},
volume = {24},
year = {2020}
}
@article{Miranda2020,
abstract = {Rationale: Abnormal body composition and a decreased phase angle (PA) are indicators of adverse outcomes in several surgical populations, including poor quality of life, perioperative mortality, longer hospital stay, higher incidence of infection, and higher overall health care cost. In the present study, we investigate the association between PA assessed by preoperative bioimpedance (BIA) with postoperative complications in patients with colorectal neoplasia undergoing abdominal surgery with curative intent. Methods: Transversal observational study carried out in patients with colorectal cancer undergoing curative surgery. The body composition analysis was determined by BIA. PA was result of the relation between reactance (Xc) and resistance. The diagnosis of sarcopenia was established based on the criteria of The European Working Group on Sarcopenia in Older People (EWGSOP2). Univariate and multivariate survival analyses were conducted using the Cox proportional hazards model. All P-values were 2-sided, and those < 0.05 were considered statistically significant. Results: The study was conducted in 50 patients hospitalized with colorectal neoplasia undergoing abdominal surgery with curative intent. The mean age was 68.32 years (SD 15.98) and 56% (N = 28) were males. 52% of the patients presented sarcopenia, with a predominance in women. (81,1 % (N= 18)). The mean of length of stay was 6,38 days (SD 2,97). There was a negative correlation between PA and length of stay (p = 0,0001). Conclusion: Sarcopenia is prevalent condition in patients with colorectal cancer. Phase angle is a predictor of longer hospital stay. Disclosure of Interest: None declared},
author = {Miranda, C M P and Toro, J J O and Mart{\'{i}}n, B S and Gimeno, S R and Borque, R U and Bret{\'{o}}n, M J O},
doi = {10.1016/j.clnesp.2020.09.427},
issn = {2405-4577},
journal = {Clinical Nutrition ESPEN},
