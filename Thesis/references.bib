journal = {Journal of Comprehensive Pediatrics},
keywords = {Staphylococcus aureus,adult,air pollution,allergy,alternative medicine,antibiotic agent,anxiety,case report,ciliary motility,clinical article,cold air,cold stress,conference abstract,cystic fibrosis,elasticity,endogenous compound,exercise,female,gas,gene expression,health care personnel,human,human cell,immune response,immunoglobulin E,infectious inflammation,inflammation,lifestyle,lung disease,lung function,male,methicillin resistance,mucociliary clearance,mucus secretion,nonhuman,physical activity,quality of life,respiratory tract infection,respiratory tract inflammation,sympathetic tone,viscosity},
language = {English},
pages = {9--10},
title = {{Inappropriate Lifestyle as an Effective Factor on Inflammation in Cystic Fibrosis}},
