@article{Sills2020,
abstract = {Objectives: To evaluate body composition measures using bioelectrical impedance analysis in adults with CF. Methods: In 2018 bioelectrical impedance analysis was introduced as part of standard practice during annual assessment for adults with at a UK adult CF specialist Centre. We performed bioelectrical impedance analysis using multi frequency a whole-body bioelectrical impedance analyser, Tanita MC 780 MA, (Tanita Corporation, Tokyo-Japan). At annual assessment absolute FEV1, weight, height, phase angle, body fat percentage and fat free mass were measured. Fat free mass and total body weight were divided by the square of height for each participant to determine BMI and fat free mass index. Results: One hundred and twenty-four sets of data were available from 103 patients. There was a positive weak correlation between absolute FEV1 and BMI (r = 0.19, p = 0.032). Absolute FEV1 was positively correlated with fat free mass index (r = 0.34, p < 0.001) and phase angle (r = 0.34, p < 0.001). Phase angle strongly and positively correlated with fat free mass (r = 0.69, p < 0.001) and fat free mass index (r = 0.69, p = <0.001), whilst it was negatively correlated with body fat percentage (r = −0.33, p = <0.001). Phase angle was also positively correlated with absolute FEV1 (r = 0.33, p = <0.001). Conclusion: BMI as a marker of nutritional status is associated with pulmonary function in people with CF, yet in this data set the correlation for absolute FEV1 and BMI was weaker than for absolute FEV1 and fat free mass index and phase angle. Moreover, a strong correlation for phase angle is of interest especially as a prognostic marker as it provides information on cell mass and integrity. These findings may benefit from larger prospective studies to confirm these findings and determine the long-term clinical consequences in CF and phase angle may be better suited due to the assumptions made by bioelectrical impedance analysis equations.},
author = {Sills, D and Mitchell-Whyte, M and Dewar, J and Barr, H},
doi = {10.1016/S1569-1993(20)30609-3},
issn = {1873-5010},
journal = {Journal of Cystic Fibrosis},
keywords = {Japan,adult,analyzer,body fat percentage,body mass,body weight,conference abstract,controlled study,cystic fibrosis,fat free mass,female,forced expiratory volume,human,impedance,lung function,major clinical study,male,nutritional status,prospective study},
language = {English},
pages = {S134},
title = {{Evaluation of body composition measures in cystic fibrosis: moving beyond body mass index for assessing nutritional status}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2006057037&from=export http://dx.doi.org/10.1016/S1569-1993(20)30609-3},
volume = {19},
year = {2020}
}
@article{Sills2020a,
abstract = {Objectives: To evaluate and compare body composition measures using bioelectrical impedance analysis in people with people with CF. Methods: In 2018 bioelectrical impedance analysis was introduced as part of standard practice during annual assessment for adults with at a UK adult CF specialist Centre. We performed bioelectrical impedance analysis using multi frequency a whole-body bioelectrical impedance analyser, Tanita MC 780 MA, (Tanita Corporation, Tokyo-Japan). At annual assessment absolute FEV1, weight, height, phase angle, body fat percentage and fat free mass were measured and diabetes status were recorded. Normal weight obesity was defined as a BMI < 25 kg/m2 and body fat >30% in women or >23% in men. Results: One hundred and twenty-four sets of data were available from 103 patients. We found normal weight obesity in nine (8.7%) of the 103 patients. In those with normal weight obesity 89% of those had glucose tolerance irregularities (impaired glucose tolerance or cystic fibrosis diabetes). Absolute FEV1 was negatively associated with body fat percentage (r = 0.2, p = 0.03). Conclusion: Excess adiposity, particularly in the form of normal weight obesity (normal weight BMI with elevated body fat percentage) has been shown to have a negative association with lung function. In this small study we noted higher frequency of normal weight obesity in people with glucose tolerance irregularities. This finding may benefit from larger prospective studies to confirm these findings and determine the long-term clinical consequences in CF.},
