@article{Tierney2018,
abstract = {Background: Ivacaftor improves lung function and increases body weight in patients with CF and the G551D mutation. The effect of Ivacaftor on body weight seems to occur within weeks of commencing therapy. Long term effects on weight are emerging, however, body composition changes are undocumented. Objectives: To evaluate 5 and 24 month treatment effects of Ivacaftor on weight, BMI and body composition in CF adults with the G551D-CFTR mutation. Methods:We undertook a prospective study of Ivacaftor in 20 adults with CF.1 Data were available at 24 months of treatment for 11 of these adults (mean ± SD age 33.5 ± 13.1 years, BMI 23.4 ± 3.4 kg/m2, FEV1 61.8 ± 22.9%; 7 male). Using Multifrequency Bioelectrical Impedance Analysis (SECA, Germany), we measured weight, fat mass (FM) and fat free mass (FFM) at baseline, five and 24 months. Change data were determined and repeated measures ANOVA undertaken. Results: Mean weight, BMI and FM significantly increased following five months of Ivacaftor treatment ($\Delta$Weight 3.2 ± 2.2 kg, p = 0.002; $\Delta$BMI 1.22 ± 0.8 kg/m2, p = 0.002; $\Delta$FM 2.37 ± 1.1 kg, p = 0.0005). No significant changes were observed for FFM ($\Delta$FFM 0.8 ± 1.3 kg, p = 0.21). No further increases were observed forweight, BMI and FM between 5 and 24 months of treatment exposure ($\Delta$Weight 0.04 ± 3.5 kg, p = 1.0; $\Delta$BMI -0.06 ± 1.2 kg/m2, p = 1.0; $\Delta$FM -0.15 ± 2.35 kg, p = 1.0). Whilst most patients (45.5%) were within normal BMI range at baseline, 36.4% were overweight. This increased to 54.4% at 24 months with two patients moving from the normal to the overweight category. In this cohort $\Delta$FEV1%was not associated with a $\Delta$weight, $\Delta$FM or $\Delta$FFM at 5 or 24 months. Conclusion: Weight, BMI and FM gains are observed within the first 5 months of Ivacaftor treatment, followed by a plateauing thereafter by 2 years. The metabolic and clinical consequences of weight and fat mass gains from normal to overweight BMI categories are yet to be determined.},
address = {A. Tierney, Alfred Health, Cystic Fibrosis Service, Melbourne, Australia},
author = {Tierney, A and King, S J and Edgeworth, D and Williams, E and Finlayson, F and Keating, D and Clark, D and Button, B M and Kotsimbos, T and Wilson, J W},
issn = {1873-5010},
journal = {Journal of Cystic Fibrosis},
keywords = {Germany,adult,analysis of variance,body composition,body mass,clinical article,conference abstract,controlled study,cystic fibrosis,endogenous compound,fat free mass,female,forced expiratory volume,human,impedance,ivacaftor,male,mutation,obesity,prospective study},
language = {English},
pages = {S117},
title = {{An increase in weight and fat mass observed following five months of ivacaftor treatment plateaus at 24 months in adults with G551D-related cystic fibrosis}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L622930989&from=export},
volume = {17},
year = {2018}
}
@article{Ziai2014,
abstract = {Malnutrition in cystic fibrosis (CF) is associated with increased mortality and can lead to fat-free (FFM) and fat mass (FM) loss. Dual-energy X-ray absorptiometry (DXA) is used and validated to measure FFM and FM. DXA's high cost has led to the utilization of less costly techniques such as bioelectrical impedance analysis (BIA). The aim of this study was to determine the agreement of FFM, FM and %FM measurements taken with DXA and BIA in adults with CF. We measured FFM, FM and %FM in 34 adults with CF with a leg-to-leg BIA and an iDXA and determined agreement using Bland-Altman analysis. While DXA and BIA measurements were well correlated (r > 0.8), mean biases between both methods were between 8 and 11%. BIA underestimated FM and %FM and overestimated FFM. In a clinical research setting where these measurements are used to phenotype patients, BIA cannot replace DXA.},
