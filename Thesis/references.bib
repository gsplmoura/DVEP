journal = {Clinical Nutrition},
keywords = {European,arm circumference,body weight loss,cell function,digestive system injury,female,hospital,human,impedance,length of stay,male,metabolism,neck,neoplasm,nutrition,nutritional status,patient,skinfold,society,surgery,total body water,university hospital},
