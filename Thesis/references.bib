author = {Tierney, A C and Edgeworth, D and Williams, E and Finlayson, F and Keating, D and Clark, D and Button, B M and Kotsimbos, T and Wilson, J W},
issn = {1569-1993},
journal = {Journal of Cystic Fibrosis},
keywords = {European,adult,body composition,crossover procedure,cystic fibrosis,fat free mass,fat mass,human,impedance,ivacaftor,muscle mass,mutation,nutrition,patient,placebo,skeletal muscle,therapy,total body water,weight},
language = {English},
pages = {S50},
title = {{Ivacaftor and its effects on body composition in adults with G551D related cystic fibrosis}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L71951636&from=export},
volume = {14},
year = {2015}
}
@article{Robbins2012,
abstract = {Rationale While lung transplantation is a life-saving procedure for many adults with advanced lung disease, the risk of death in the first year after transplantation remains unacceptably high. There is an unmet need for novel, precisely measured extrapulmonary measures of peri-operative risk that might aid in the prediction of post-operative complications. We hypothesize that sarcopenia (low skeletal muscle mass) and skeletal muscle function may be unrecognized extrapulmonary predictors of poor outcomes following lung transplantation. Methods Adult lung transplant candidates at Columbia University Medical Center were enrolled in a prospective cohort study that included body composition assessment by bioelectrical impedance analysis and grip strength assessment. Skeletal muscle mass was estimated from age, gender, height, and impedance and normalized to height-squared (skeletal muscle mass index, SMI). Sarcopenia was defined as an SMI below the lower limit of normal (9.12 kg/m2 for men and 6.53 kg/m2 for women) using NHANES III reference values. Grip strength was measured using a hand-held Jamar dynamometer. Non-parametric statistics were used. Results 64 adult lung transplant candidates completed assessments. The mean age was 56 yrs (range 25-71 yrs), 20% were over age 65, 48% were men, 73% were non-Hispanic white, 56% had interstitial lung disease (ILD), 22% had emphysema, 11% had cystic fibrosis, and 15 (23%) were sarcopenic. Sarcopenic candidates tended to be older (63 vs 54 yrs, p = 0.001), non-Hispanic white (47% vs 24%, p = 0.12), have ILD (80% vs 49%, p = 0.03), and have a lower body mass index (24 vs 26 kg/m2, p = 0.12). To date, 16 have undergone lung transplantation, of whom 6 (38%) were sarcopenic. Sarcopenic transplant recipients had higher oxygen requirements (8 vs 5 L/min, p = 0.11), higher LAS (59 vs 47, p = 0.14), and lower mean pulmonary artery pressure (16 vs 28, p = 0.03) than non-sarcopenic recipients. SMI was positively correlated with FVC% (0.50, p = 0.049). Lower grip strength was associated with a higher LAS (r = -61, p = 0.01), lower six-minute walk distance (r= 0.57, p = 0.02), fewer ICU-free days (r = 0.58, p = 0.02), and fewer ventilator-free days (r = 0.43, p = 0.09). Conclusions Sarcopenia is prevalent among lung transplant candidates. Reduced grip strength, a measure of skeletal muscle function, may be associated with poor outcomes after lung transplantation.},
address = {H. Robbins, Columbia University, New York, NY, United States},
author = {Robbins, H and Arcasoy, S M and Wilt, J S and Shah, L and Sanyal, S and Peterson, E and Philip, N and Sonett, J R and Lederer, D J},
issn = {1073-449X},
journal = {American Journal of Respiratory and Critical Care Medicine},
keywords = {American,Colombia,Hispanic,adult,body composition,body mass,cohort analysis,cystic fibrosis,death,dynamometer,emphysema,female,gender,graft recipient,grip strength,height,human,impedance,interstitial lung disease,lung,lung artery pressure,lung disease,lung transplantation,male,muscle function,muscle mass,oxygen consumption,parametric test,postoperative complication,prediction,procedures,recipient,reference value,risk,sarcopenia,skeletal muscle,society,surgical risk,transplantation,university hospital,ventilator},
language = {English},
title = {{Skeletal muscle mass and function in lung transplant candidates}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L71990006&from=export},
volume = {185},
year = {2012}
}
@article{Epifanio2017,
abstract = {Introduction: Structural abnormalities in the Cystic fbrosis transmembrane conductance regulator protein causes clinical symptoms in several organs and organ systems in patients with cystic fbrosis (CF), which mainly include respiratory and gastrointestinal changes. As a consequence of these changes, patients with CF usually display nutrient malabsorption, low-caloric intake, and increased energy expenditures. This results in a signifcant nutritional impairment. Respiratory exacerbations frequently occur in these patients, and many require hospital admission, which risks nutritional deterioration.Several treatment strategies have been offered to improve pulmonary and nutritional status. With respect to nutritional therapy during hospitalization, there is a consensus that patients should receive a hypercaloric diet with adequate replacement of pancreatic enzymes. Energy supplements should also be used to facilitate weight gainIntervention studies suggest that the accumulation of lean mass is associated with the preservation of lung function in CF patients. Meanwhile, the loss of lean mass has been associated with increased severity of the disease, decreased lung function, respiratory muscle weakness, and increased systemic infammation. Therefore, aggressive nutritional support aims to achieve normal growth patterns and should lead to the maintenance of lung function. However, there is a paucity of evidence evaluating the effectiveness of the nutritional therapy recommended to inpatients. The few studies that evaluated weight gain included a limited number of participants a large age group, and it did not assess weight gain with an emphasis on body mass change as the main outcome. Therefore, the objective of this study is to evaluate the variations in body weight and composition by means of electric bioimpedance in children and adolescents diagnosed with cystic fbrosis. These patients received a high-calorie diet during hospitalization for pulmonary exacerbation. Materials/Methods: This was a longitudinal study involving children and adolescents with cystic fbrosis, who were being followed up by the Multidisciplinary Cystic Fibrosis Team at S{\~{a}}o Lucas Hospital of PUCRS in Porto Alegre, Brazil. We included patients with a confrmed diagnosis of CF, who underwent hospital admission because of pulmonary exacerbation. The nutritional evaluation of the patients occurred at two timepoints: once during the frst 24 hours of hospitalization and again 24 hours before discharge. In each case, weight and height were measured after an electrical bioimpedance analysis. The caloric intake was evaluated by describing the foods consumed using a food registry for 3 consecutive days from day 7 of hospitalizationPulmonary function tests were performed at the time of the initial hospitalization by individuals trained according with the American Thoracic Society standards. The spirometry parameters were evaluated also. Results: The sample consisted of 23 patients. Of these 10/23 (43.4%) were male. The mean age was 11.9 ± 3.8 years, and 9/23 (39.0%) had undergone gastrostomy. The mean length of hospital stay was 12.9 ± 3.9 days. The patients had in average moderate pulmonary compromise according to the ATS ERS classifcation. The results of dietary intake showed that food consumption was categorized as a hypercaloric diet. Tables shows variations in the anthropometric parameters and body composition variables. These fndings showed that there was a signifcant change in the means of the following variables in the patients: weight, BMI (z-score), percentage of lean mass, and percentage of fat mass. Discussion: We found that during the hospital stay, a hypercaloric diet correlated with a signifcant increase in weight, BMI, and the percentage of fat mass. Meanwhile, this diet correlated with a decrease in the percentage of lean mass.It is possible that the signifcant gain in BMI weight and z-scores in the patients resulted from a greater caloric intake and the restriction in physical activity during hospitalization. The observed decrease in lean mass and the increase in fat mass illustrate the importance of reviewing nutritional interventions in patients during hospitalization. The increase in fat mass and the loss of lean mass in patients with adequate nutritional status could cause harm to the health of these individuals.The loss of lean mass may have been associated with the extended time restricted to bed-rest. This emphasizes the importance of maintaining physical activity, even during hospitalization.The results of the present study show the importance of evaluating body composition and tailoring nutritional interventions to CF patients over the course of treatment. Conclusions: In conclusion, a hypercaloric and hyperlipidic diet during hospitalization resulted in signifcant weight gain, a decrease in lean mass, and an increase in the percentage of fat mass in CF patients.},
address = {M. Epifanio, Pediatric Gastroenterologist, Hospital Santo Antonio, Porto Alegre, Rio Grande do Sul, Brazil},
author = {Epifanio, M and {Dos Santos Sampaio}, A and Bosa, V and Marostica, P and Preto, L and Fernandes, S and Costa, C A D and Mattiello, R},
doi = {10.1097/MPG.0000000000001805},
issn = {1536-4801},
journal = {Journal of Pediatric Gastroenterology and Nutrition},
keywords = {Brazil,bed rest,body composition,body mass,body weight gain,breathing muscle,caloric intake,child,clinical article,clinical evaluation,comparative effectiveness,consensus,cystic fibrosis,deterioration,diagnosis,diet,disease exacerbation,fat mass,female,food intake,gastrostomy,height,hospital admission,hospital patient,hospitalization,human,human tissue,longitudinal study,lung disease,lung function,male,nutritional assessment,nutritional status,nutritional support,outcome assessment,pancreas enzyme,physical activity,register,respiration depression,school child,spirometry,weakness},
language = {English},
pages = {S318--S319},
title = {{Electric bioimpedance reveals variations in body composition in children and adolescents with cystic fibrosis during hospital admission}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L619609042&from=export http://dx.doi.org/10.1097/MPG.0000000000001805},
volume = {65},
year = {2017}
}
@article{Emsley2020,
abstract = {Purpose: Progressive or severe malnutrition is a relative contraindication to lung transplant (LTx). The Global Leadership Initiative on Malnutrition (GLIM) developed new criteria for malnutrition incorporating low muscle mass, relevant due to its high prevalence in severe lung disease. Objective is to compare the prevalence of malnutrition in adults assessed for LTx using International Classification of Diseases, 10th Revision (ICD-10-AM) criteria against GLIM criteria1. Methods: Retrospective medical record analysis of adults assessed for LTx Sep 2018-Aug 2019. Weight history from dietitian assessment. Six-Minute Walk Test (6MWT) from physiotherapy assessment. Body Mass Index (BMI) and Fat-Free Mass Index (FFMI) from Bioelectrical Impedance Analysis. BMI classification: underweight <18.5kg/m2, normal 18.5-24.9kg/m2, overweight 25.0-29.9kg/m2 and obese >30kg/m2. Criteria for malnutrition using ICD-10-AM: BMI <18.5kg/m2 or unintentional weight loss (moderate 5-9%, severe ≥10%) with reduced food intake. Criteria for malnutrition using GLIM: low BMI or low FFMI (males <17kg/m2, females <15kg/m2) or unintentional weight loss; plus reduced food intake and/or acute or chronic disease1. Results: Of 130 patients assessed, 112 had all data for malnutrition classification [54% male; mean±SD age 54.5±13.8yrs; BMI 24.6±4.0kg/m2; 6MWT 334±119m]. Primary lung disease: ILD 38%, COPD 34%, Cystic Fibrosis 11%, re-transplant 9%, other 8%. BMI: underweight 7%, normal 44%, overweight 43%, obese 6%. FFMI in males was greater than females (17.4±1.8kg/m2 vs 14.3±1.8kg/m2, P <0.0001). Using ICD-10-AM, 26% were malnourished, of whom 28% were severe. Using GLIM, 59% were malnourished, 15% of these severe. Greater diagnosis using GLIM due to high prevalence of low FFMI (females 61% vs males 36%, p=0.009). Of those malnourished using GLIM, 58% would be missed using ICD-10-AM. A third (32%) of those malnourished using GLIM had a BMI >25kg/m2. Malnutrition was most common in COPD (79%) and re-transplant (70%), but there was no association with age or 6MWT. Conclusion: Use of GLIM improves detection of malnutrition, permitting more targeted nutrition interventions. Malnutrition in LTx candidates is seen across all BMI categories. Future studies are needed to investigate GLIM and LTx outcomes.1Jensen GA et al; JPEN 2019; 43:32-40.},
address = {C. Emsley, Nutrition, Alfred Hospital, Melbourne, Australia},
author = {Emsley, C and King, S and Snell, G},
doi = {10.1016/j.healun.2020.01.456},
issn = {1557-3117},
journal = {Journal of Heart and Lung Transplantation},
keywords = {ICD-10,adult,age,body composition,body mass,body weight loss,chronic obstructive lung disease,clinical assessment,conference abstract,controlled study,cystic fibrosis,dietitian,fat free mass,female,food intake,human,impedance,leadership,major clinical study,male,malnutrition,medical record,middle aged,obesity,physiotherapy,prevalence,retrospective study,six minute walk test,underweight},
language = {English},
number = {4},
pages = {S369},
title = {{Incorporating Body Composition into Lung Transplant Assessment Enhances the Detection of Malnutrition}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2005250669&from=export http://dx.doi.org/10.1016/j.healun.2020.01.456},
volume = {39},
year = {2020}
}
@article{Richards2003,
abstract = {The aim of this study was to compare the measurement of total body water (TBW) by deuterium (2H2O) dilution and bioelectrical impedance analysis (BIA) in patients with cystic fibrosis (CF) and healthy controls. Thirty-six clinically stable patients with CF (age 25.4 ± 5.6 yrs) and 42 healthy controls (age 25.4 ± 4.8) were recruited into this study. TBW was measured by 2H2O dilution and predicted by BIA in patients and controls. The TBW predicted from BIA was significantly different from TBW as measured using 2H2O in patients (P < 0.05) but not in controls. Mean (±SD) values for predicted and measured TBW differed by 5.6 (±9.1) L in patients and 0.4 (±3.6)L in controls. This bias was consistent for all controls but not for patients. In CF, BIA over predicted TBW determined by 2H2O dilution to an increasing extent at larger TBW volumes. There was a strong correlation between height2/impedance and TBW in patients with CF (r = 0.90; y = 0.67x + 2.50) and in controls (r = 0.81; y = 0.57x + 9.60). The slope of the regression lines was similar for both groups, however the y intercepts were significantly different (P < 0.05). BIA overestimates TBW in patients with CF, possibly due to invalid factory installed regression equations within BIA instrumentation. Future studies employing BIA as a measure of TBW or FFM in CF should use alternative predictive equations to those that have been developed for healthy individuals. A large scale study to develop specific regression equations for use in CF is warranted.},
address = {P.S.W. Davies, Dept. of Paediatr. and Child Health, University of Queensland, Royal Children's Hospital, Brisbane, QLD 4029, Australia},
author = {Richards, M L and Bell, S C and Edmiston, K A and Davies, P S W},
issn = {0964-7058},
journal = {Asia Pacific Journal of Clinical Nutrition},
keywords = {adult,article,bioenergy,body height,body water,clinical article,controlled study,correlation analysis,cystic fibrosis,deuterium,dilution,female,human,impedance,male,prediction,regression analysis},
language = {English},
number = {2},
pages = {161--165},
title = {{Assessment of bioelectrical impedance analysis for the prediction of total body water in cystic fibrosis}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L38886746&from=export},
volume = {12},
year = {2003}
}
@article{Godin2018,
abstract = {Objective: Nutritional status, evaluated by BMI, is a major issue in cystic fibrosis. The aim of this study was to evaluate the effects of a physical retraining (PR) program on body composition assessed by bioimpedancemetry. Methods: A PR 8 week program consisted of 3 weekly sessions including aerobic training (≥ 30 min), muscle strengthening with a circuit training and relaxation individually adapted each week. The evaluations were done before and after PR: a 6-minute walk test (6MWT) and quadriceps isometric strength. Bioelectrical impedance analysis (Biacorpus RX4000,) assessed fat-free mass (FFM), fat mass (FM) and body cell mass (BCM). Brachial circumference and triceps skinfold were also measured. The same evaluation were performed at the same interval in a control group of 24 patients. Results: 28 patients aged 28 ± 5 years (FEV1 48.8 ± 19%) were included. There was no significant change in FEV1. After PR, patients improved exercise tolerance and muscle strength. Whereas BMI did not improve, we observed a significant increase of FFM and a trend for BCM. There was no significant change of FM, brachial circumference and triceps fold. There was no significant change in all measurements in controls. There was no correlation between improvement of FFM and improvement of 6MWTdistance or muscle strength. (Table Presented) Conclusion: While BMI was not modified, there was a significant improvement in FFM, illustrating the need to use bioimpedancemetry, making it more accurate than with simple BMI for changes in body composition. In this context PR is of importance not only to improve exercise tolerance but also the nutritional status.},
address = {J. Godin, CHRU Lille, Adult Cystic Fibrosis Center, Lille, France},
author = {Godin, J and Bernard, H and Perez, T and {Le Rouzic}, O and Wallaert, B and Prevotat, A},
issn = {1873-5010},
journal = {Journal of Cystic Fibrosis},
keywords = {adult,body composition,body mass,cell body,circuit training,clinical article,conference abstract,controlled study,cystic fibrosis,exercise tolerance,fat free mass,female,forced expiratory volume,human,impedance,leisure,male,muscle strength,muscle training,nutritional status,quadriceps femoris muscle,six minute walk test,skinfold,triceps brachii muscle},
language = {English},
pages = {S49--S50},
title = {{Effects of physical retraining on the body composition of adult patients with cystic fibrosis}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L622930755&from=export},
volume = {17},
year = {2018}
}
@article{VanBiervliet2018,
abstract = {Background: Body mass index is a clinical outcome parameter for the evaluation of the nutritional status in CF. However, recent studies indicate that lean body mass (LBM) might be a stronger indicator. In the nutritional management of CF, tube feeding (TF) is often used to improve the nutritional status. The influence of TF on body composition was not yet evaluated. Method: Bio-electrical impedance analysis (BIA) measurements were performed in CF patients with TF (2F, 7M) and without TF (matched controls, Co). Percent fat, body fat mass index (BFMI) and fat free mass index (FFMI) were calculated. Patients were matched for age, gender, pancreatic function. Results are given as median (quartiles). Results: At the start of TF patients were aged 12.8 years (8.7 15.6), had a height z-score of-2.7 (-2.8-1.7) and a BMI z-score of-2.1 (-2.9;-1.4). The median time they were on TF before the BIA measurement was 1.5 years (0.84 2.7). There was a significant improvement in BMI z-score (p= 0.038) and L z score (p=0.008). The age at BIA measurement was 14.4 years (12.5 16.5). TF patients were significantly shorter than Co (p=0.001) but the BMI z-score as well as pulmonary function (FEV1%, FVC%) were not significantly different between TF and Co. The body composition, however, was significantly different with a higher percent fat (p=0.024) (TF: 24.7 (19.6 33.1) and Co: 12.9 (10.8; 22.1)), a higher BFMI (p= 0.019) (TF: 4 (3.3 5.7) Co: 2.5 (1.95 3.85)), and a lower FFMI (p= 0.04) (TF: 11.8 (10.9;13.4) Co 15.1 (13.2;17.25) in TF patients. There was no relation of body composition with age at start of TF, duration of TF or pulmonary function. The BMI z-score at start of TF was positively correlated with percent fat (p=0.032) and BFMI (p=0.01). Conclusion: Patients receiving TF have less FFM. Although tube feeding is often the only remaining way to restore nutritional status in CF patients, it leads to an increase in body fat. Different TF types as well as interventions combining activity to TF should be evaluated in the future.},
address = {S. Van Biervliet, Pediatric Gastro-enterology and Nutrition, Gent University Hospital, Gent, Belgium},
author = {{Van Biervliet}, S and {Van Braeckel}, E and {De Baets}, F and {Van Daele}, S and {Van Meerhaeghe}, S and Declercq, D},
doi = {10.1002/ppul.24152},
issn = {1099-0496},
journal = {Pediatric Pulmonology},
