@article{Pereira2002,
abstract = {TEF 25-75% {\'{e}} o tempo expirat{\'{o}}rio for{\c{c}}ado m{\'{e}}dio entre 25 e 75% da CVF. Esta grandeza {\'{e}} expressa em segundos. Ventila{\c{c}}{\~{a}}o volunt{\'{a}}ria m{\'{a}}xima (VVM): representa o volume m{\'{a}}ximo de ar ventilado em um per{\'{i}}odo de tempo por repetidas manobras respirat{\'{o}}rias for{\c{c}}adas. Espir{\^{o}}metros de volume: s{\~{a}}o equipamentos desti-nados a medir volumes e fluxos a partir de um sinal pri-m{\'{a}}rio de volume. Espir{\^{o}}metros de fluxo: s{\~{a}}o equipamentos destina-dos a medir volumes e fluxos a partir de um sinal prim{\'{a}}rio de fluxo. 1. ABNT. Espir{\^{o}}metros – Requisitos, RJ, 1996;1-9. 2. Jardim JRB, Romaldini H, Ratto OR. Proposta para unifica{\c{c}}{\~{a}}o dos ter-mos e s{\'{i}}mbolos pneumol{\'{o}}gicos no Brasil. J Pneumol 1983;9:45-51. DEFINI{\c{C}}{\~{O}}ES As defini{\c{c}}{\~{o}}es e s{\'{i}}mbolos usados em espirometria est{\~{a}}o situados abaixo (1,2) . As grandezas funcionais s{\~{a}}o expressas em litros nas condi{\c{c}}{\~{o}}es de temperatura corporal (37 o C), press{\~{a}}o am-biente e saturado de vapor de {\'{a}}gua (BTPS). Volume residual (VR): representa o volume de ar que permanece no pulm{\~{a}}o ap{\'{o}}s uma expira{\c{c}}{\~{a}}o m{\'{a}}xima. Capacidade pulmonar total (CPT): o volume de g{\'{a}}s nos pulm{\~{o}}es ap{\'{o}}s a inspira{\c{c}}{\~{a}}o m{\'{a}}xima. Capacidade residual funcional (CRF): {\'{e}} o volume de ar que permanece nos pulm{\~{o}}es ao final de uma expi-ra{\c{c}}{\~{a}}o usual, em volume corrente. Capacidade vital (CV): representa o maior volume de ar mobilizado, podendo ser medido tanto na inspira-{\c{c}}{\~{a}}o quanto na expira{\c{c}}{\~{a}}o. Capacidade vital for{\c{c}}ada (CVF): representa o volu-me m{\'{a}}ximo de ar exalado com esfor{\c{c}}o m{\'{a}}ximo, a partir do ponto de m{\'{a}}xima inspira{\c{c}}{\~{a}}o. Volume expirat{\'{o}}rio for{\c{c}}ado no tempo (VEFt): re-presenta o volume de ar exalado num tempo especifica-do durante a manobra de CVF; por exemplo VEF 1 {\'{e}} o vo-lume de ar exalado no primeiro segundo da manobra de CVF.},
author = {Pereira, CAC and Jansen, JM and Barreto, SSM and Marinho, J and Sulmonett, N and RM, Dias},
journal = {J Bras Pneumolpneumol},
