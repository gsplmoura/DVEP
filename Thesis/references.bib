@article{Middleton2023,
abstract = {Introduction: Physical Activity (PA), particularly moderate-high intensity, benefits general health and prognosis in Cystic Fibrosis (CF). PA declines in the teenage years. The optimal training program to promote PA remains unknown. Home-based group exercise supervised over telehealth has not been investigated. Aim: A single-centre telehealth, pilot study to explore feasibility, acceptability and efficacy of a progressive home-based group high-intensity interval training (HIIT) intervention in young people with CF who don't meet Australian PA recommendations. Methods: 8 young people (10-17 years) were recruited during clinic visits. The physiotherapist supervised 30-60 min exercise sessions delivered via telehealth 3×/week for 2 school terms (10 weeks each). Participants were provided with basic gym equipment and performed core, resistance and aerobic exercises in a progressive HIIT format. Feasibility and acceptability were measured by the proportion of sessions attended and completed without technical interruption. Efficacy measures included: Spirometry (FEV1, % predicted); cardiopulmonary exercise testing using the Bruce Treadmill Protocol (peak VO 2); functional muscle tests (vertical jump height, medicine ball throw, hand grip strength, knee-extension dynamometry, sit-ups in 30 s); and muscle mass measured by Bioelectrical Impedance Analysis. Efficacy data was assessed using Wilcoxon matched-pairs signed rank tests. Results: 8 subjects, 3 male, median age 15 (12-17) years were recruited and all completed the study. Of the 55 sessions, participants attended a median of 70% (44-89) and only 4 sessions (<1%) experienced technical interruption. Baseline and efficacy data are presented in Table 1. Conclusions: Progressive supervised home-based group HIIT provided over telehealth was feasible and acceptable. Lung function, exercise capacity and muscle mass remained stable and there were significant improvements in strength measures. Larger randomised controlled trials are needed to explore the efficacy of telehealth-supervised group exercise interventions in people with CF.},
address = {A. Middleton, The Children's Hospital at Westmead, Sydney, Australia},
author = {Middleton, A},
doi = {10.1111/resp.14459},
issn = {1323-7799},
journal = {Respirology},
keywords = {adolescent,aerobic exercise,bioelectrical impedance analysis,child,clinical article,conference abstract,controlled study,cystic fibrosis,dynamometry,exercise,exercise test,feasibility study,female,forced expiratory volume,grip strength,high intensity interval training,human,knee,lung function,male,muscle mass,peak oxygen uptake,physical activity,physiotherapist,pilot study,randomized controlled trial,spirometry,telehealth,treadmill},
language = {English},
pages = {45--46},
title = {{Telehealth high-intensity interval training for young people with cystic fibrosis}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L641144917&from=export http://dx.doi.org/10.1111/resp.14459},
volume = {28},
