keywords = {anthropometry,biochemical marker,body composition,death,female,hazard ratio,hemodialysis,hemodialysis patient,human,interleukin 6,kidney disease,long term survival,marker,metabolism,mortality,nutrition,parameters,proportional hazards model,protein intake,risk,serum},
