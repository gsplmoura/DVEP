journal = {World Journal of Gastroenterology},
keywords = {Diagnosis,Epidemiology,Genetics,Insulin resistance,Management,Metabolic syndrome,Non-alcoholic fatty liver disease,Pathogenesis,Prognosis},
number = {36},
