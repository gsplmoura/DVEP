language = {English},
pages = {608--614},
title = {{A polysaccharide from Eclipta prostrata alleviates experimental autoimmune encephalomyelitis through inhibiting Th17 cells}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L627425457&from=export http://dx.doi.org/10.1016/j.carbpol.2018.08.113},
