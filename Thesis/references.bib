@article{Berge2016,
abstract = {BACKGROUND: To date there has been limited success with childhood obesity prevention interventions. This may be due in part, to the challenge of reaching and engaging parents in interventions. The current study used a community-based participatory research (CBPR) approach to engage parents in cocreating and pilot testing a childhood obesity prevention intervention. Because CBPR approaches to childhood obesity prevention are new, this study aims to detail the creation, including the formation of the citizen action group (CAG), and implementation of a childhood obesity prevention intervention using CBPR methods.
METHOD: A CBPR approach was used to recruit community members to partner with university researchers in the CAG (n = 12) to create and implement the Play It Forward! childhood obesity intervention. The intervention creation and implementation took 2 years. During Year 1 (2011-2012), the CAG carried out a community needs and resources assessment and designed a community-based and family focused childhood obesity prevention intervention. During Year 2 (2012-2013), the CAG implemented the intervention and conducted an evaluation. Families (n = 50; 25 experimental/25 control group) with children ages 6-12 years participated in Play It Forward!
RESULTS: Feasibility and process evaluation data suggested that the intervention was highly feasible and participants in both the CAG and intervention were highly satisfied. Specifically, over half of the families attended 75% of the Play It Forward! events and 33% of families attended all the events.
CONCLUSION: Equal collaboration between parents and academic researchers to address childhood obesity may be a promising approach that merits further testing.},
author = {Berge, J M and Jin, S W and Hanson, C and Doty, J and Jagaraj, K and Braaten, K and Doherty, W J},
doi = {10.1037/fsh0000116},
issn = {1939-0602},
journal = {Families, systems & health : the journal of collaborative family healthcare},
keywords = {Minnesota,adult,child,childhood obesity,community participation,feeding behavior,female,health promotion,human,male,middle aged,parent,participatory research,procedures,program development,psychology},
language = {English},
number = {1},
pages = {15--30},
title = {{Play it forward! A community-based participatory research approach to childhood obesity prevention}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L621618533&from=export http://dx.doi.org/10.1037/fsh0000116},
volume = {34},
year = {2016}
}
@article{Johnson-Shelton2015,
abstract = {BACKGROUND: Childhood obesity is a systemic and complex, multilevel public health problem. Research approaches are needed that effectively engage communities in reversing environmental determinants of child obesity.
OBJECTIVES: This article discusses the Communities and Schools Together (CAST) Project and lessons learned about the project's community-based participatory research (CBPR) model.
METHODS: A partnership of schools, community organizations, and researchers used multiple methods to examine environmental health risks for childhood obesity and conduct school-community health programs. Action work groups structured partner involvement for designing and implementing study phases.
LESSONS LEARNED: CBPR in child obesity prevention involves engaging multiple communities with overlapping yet divergent goals. Schools are naturally situated to participate in child obesity projects, but engagement of key personnel is essential for functional partnerships. Complex societal problems require CBPR approaches that can align diverse communities and necessitate significant coordination by researchers. CBPR can provide simultaneous health promotion across multiple communities in childhood obesity prevention initiatives. Support for emergent partner activities is an essential practice for maintaining community interest and involvement in multiyear CBPR projects.
CONCLUSION: Investigator-initiated CBPR partnerships can effectively organize and facilitate large, health-promoting partnerships involving multiple, diverse stakeholder communities. Lessons learned from CAST illustrate the synergy that can propel projects that are holistically linked to the agents of a community.},
author = {Johnson-Shelton, D and Moreno-Black, G and Evers, C and Zwink, N},
doi = {10.1353/cpr.2015.0056},
issn = {1557-0541},
journal = {Progress in community health partnerships : research, education, and action},
keywords = {child,childhood obesity,community care,health promotion,health service,human,participatory research,procedures,program evaluation,school health service},
language = {English},
number = {3},
pages = {351--361},
title = {{A Community-Based Participatory Research Approach for Preventing Childhood Obesity: The Communities and Schools Together Project}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L607706442&from=export http://dx.doi.org/10.1353/cpr.2015.0056},
volume = {9},
year = {2015}
}
@article{Loney2014,
abstract = {Employing a participatory research design permits members of the target population to play an active role within intervention development and implementation. The aim of this study was to collaborate with members of the general population to design a physical activity (PA) intervention for sedentary overweight and obese males. Based on data collected from a cross-sectional lifestyle survey in Bath and North East Somerset (UK), males aged 25-55 years were recruited according to their self-reported PA level (International Physical Activity Questionnaire) and anthropometric assessment (body mass index, waist circumference). Consenting participants were classified into either the physically active and healthy body size group (PAH; n = 6), or the sedentary and overweight/obese group (SOW; n = 6). Semi-structured interviews were undertaken and Atlas.ti was used to explore the transcripts according to the tenets of Interpretative Phenomenological Analysis. Males in the SOW group identified time, money, and motivation as perceived barriers preventing regular participation in PA. To overcome the perceived barriers of time, expense and accessibility, PAH participants suggested that walking may prove a useful vehicle to increase PA levels in SOW individuals and SOW participants also highlighted walking as a preferred activity. Study findings suggest that future PA interventions for SOW males may maximise participation and adherence rates by promoting activities that are affordable, appealing and can be easily incorporated into an individual's daily work-life schedule. Based on the study findings, a pilot randomised controlled trial was designed and implemented to assess the feasibility and efficacy of a six-week walking intervention (Walkabout to Workout Programme) on selected cardiovascular risk factors in SOW males.},
address = {T. Loney, Institute of Public Health, United Kingdom},
author = {Loney, T and Standage, M and Gillison, F and Thompson, D},
doi = {10.1111/obr.12153},
issn = {1467-7881},
journal = {Obesity Reviews},
keywords = {body mass,body size,cardiovascular risk,human,lifestyle,male,methodology,money,motivation,obesity,participatory research,physical activity,population,questionnaire,randomized controlled trial,semi structured interview,waist circumference,walking},
language = {English},
pages = {211--212},
title = {{Developing physical activity interventions for sedentary overweight and obese males: A community-based participatory research study}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L71516318&from=export http://dx.doi.org/10.1111/obr.12153},
volume = {15},
year = {2014}
}
@article{Davis2011,
abstract = {Childhood obesity is a major epidemic, with African American (AA) children aged 6 to 11 years experiencing increased burden. The AA faith community has numerous assets that point to the need for the intersection of faith and health to address obesity-related racial disparities. The purpose of the Our Bodies, God's Temples (OBGT) study was to examine diet, physical activity, and body image behaviors among AA children aged 6 to 11 years; receptivity to a faith-based obesity intervention among AA children, parents, and church leaders; and strengths and barriers of implementing a faith-based obesity curriculum in the Sunday school setting. A community-based participatory research approach was used to develop an obesity intervention to be integrated into the church Sunday school setting for AA children. A Community Advisory Network worked with researchers to develop a 12-week culturally appropriate faith-based obesity intervention. Future work will test the effectiveness of the newly created curriculum on obesity-related outcomes in AA children.},
