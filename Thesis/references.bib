issn = {2352-3018},
journal = {The Lancet HIV},
keywords = {CD4 lymphocyte count,Human immunodeficiency virus infection,Kenya,NCT01608802,adult,article,assessment of humans,controlled study,female,health care delivery,highly active antiretroviral therapy,human,major clinical study,male,mental health,pain,pain score,palliative nursing,peer-reviewed,priority journal,quality of life,randomized controlled trial,sensitivity and specificity,staff training,treatment outcome},
language = {English},
