journal = {Journal of Drug Delivery Science and Technology},
keywords = {Bacillus subtilis,Eclipta prostrata,Escherichia coli,Fourier transform infrared spectroscopy,MTT assay,Pseudomonas aeruginosa,RIN-5F cell line,Staphylococcus aureus,Western blotting,X ray diffraction,animal cell,antiapoptotic activity,antidiabetic activity,antidiabetic agent,apoptosis,article,atomic force microscopy,biosynthesis,cell viability,controlled study,down regulation,drug efficacy,drug mechanism,flow cytometry,gold nanoparticle,high resolution transmission electron microscopy,immunoglobulin enhancer binding protein,nanomedicine,nonhuman,pharmacological parameters,photon correlation spectroscopy,protein Bax,protein bcl 2,protein expression,rat,scanning electron microscopy,streptozotocin-induced diabetes mellitus,surface plasmon resonance,transmission electron microscopy,ultraviolet visible spectroscopy,upregulation,zeta potential},
