journal = {Deutsches Arzteblatt},
keywords = {Clinical research,Epidemiology,Literature search,Meta-analysis,Systematic review,checklist,data extraction,evaluation study,information retrieval,medical literature,medical research,oral contraception,oral contraceptive agent,publication,publishing,review,scientific literature,sensitivity analysis,systematic review,uterine cervix cancer},
