@article{Ye2020,
abstract = {Background: Although non-alcoholic fatty liver disease (NAFLD) is commonly associated with obesity, it is increasingly being identified in non-obese individuals. We aimed to characterise the prevalence, incidence, and long-term outcomes of non-obese or lean NAFLD at a global level. Methods: For this systematic review and meta-analysis, we searched PubMed, Embase, Scopus, and the Cochrane Library from inception to May 1, 2019, for relevant original research articles without any language restrictions. The literature search and data extraction were done independently by two investigators. Primary outcomes were the prevalence of non-obese or lean people within the NAFLD group and the prevalence of non-obese or lean NAFLD in the general, non-obese, and lean populations; the incidence of NAFLD among non-obese and lean populations; and long-term outcomes of non-obese people with NAFLD. We also aimed to characterise the demographic, clinical, and histological characteristics of individuals with non-obese NAFLD. Findings: We identified 93 studies (n=10 576 383) from 24 countries or areas: 84 studies (n=10 530 308) were used for the prevalence analysis, five (n=9121) were used for the incidence analysis, and eight (n=36 954) were used for the outcomes analysis. Within the NAFLD population, 19{\textperiodcentered}2% (95% CI 15{\textperiodcentered}9–23{\textperiodcentered}0) of people were lean and 40{\textperiodcentered}8% (36{\textperiodcentered}6–45{\textperiodcentered}1) were non-obese. The prevalence of non-obese NAFLD in the general population varied from 25% or lower in some countries (eg, Malaysia and Pakistan) to higher than 50% in others (eg, Austria, Mexico, and Sweden). In the general population (comprising individuals with and without NAFLD), 12{\textperiodcentered}1% (95% CI 9{\textperiodcentered}3–15{\textperiodcentered}6) of people had non-obese NAFLD and 5{\textperiodcentered}1% (3{\textperiodcentered}7–7{\textperiodcentered}0) had lean NAFLD. The incidence of NAFLD in the non-obese population (without NAFLD at baseline) was 24{\textperiodcentered}6 (95% CI 13{\textperiodcentered}4–39{\textperiodcentered}2) per 1000 person-years. Among people with non-obese or lean NALFD, 39{\textperiodcentered}0% (95% CI 24{\textperiodcentered}1–56{\textperiodcentered}3) had non-alcoholic steatohepatitis, 29{\textperiodcentered}2% (21{\textperiodcentered}9–37{\textperiodcentered}9) had significant fibrosis (stage ≥2), and 3{\textperiodcentered}2% (1{\textperiodcentered}5–5{\textperiodcentered}7) had cirrhosis. Among the non-obese or lean NAFLD population, the incidence of all-cause mortality was 12{\textperiodcentered}1 (95% CI 0{\textperiodcentered}5–38{\textperiodcentered}8) per 1000 person-years, that for liver-related mortality was 4{\textperiodcentered}1 (1{\textperiodcentered}9–7{\textperiodcentered}1) per 1000 person-years, cardiovascular-related mortality was 4{\textperiodcentered}0 (0{\textperiodcentered}1–14{\textperiodcentered}9) per 1000 person-years, new-onset diabetes was 12{\textperiodcentered}6 (8{\textperiodcentered}0–18{\textperiodcentered}3) per 1000 person-years, new-onset cardiovascular disease was 18{\textperiodcentered}7 (9{\textperiodcentered}2–31{\textperiodcentered}2) per 1000 person-years, and new-onset hypertension was 56{\textperiodcentered}1 (38{\textperiodcentered}5–77{\textperiodcentered}0) per 1000 person-years. Most analyses were characterised by high heterogeneity. Interpretation: Overall, around 40% of the global NAFLD population was classified as non-obese and almost a fifth was lean. Both non-obese and lean groups had substantial long-term liver and non-liver comorbidities. These findings suggest that obesity should not be the sole criterion for NAFLD screening. Moreover, clinical trials of treatments for NAFLD should include participants across all body-mass index ranges. Funding: None.},
author = {Ye, Qing and Zou, Biyao and Yeo, Yee Hui and Li, Jie and Huang, Daniel Q. and Wu, Yuankai and Yang, Hongli and Liu, Chuanli and Kam, Leslie Y. and Tan, Xiang Xuan Eunice and Chien, Nicholas and Trinh, Sam and Henry, Linda and Stave, Christopher Donald and Hosaka, Tetsuya and Cheung, Ramsey C. and Nguyen, Mindie H.},
doi = {10.1016/S2468-1253(20)30077-7},
file = {:Users/gustavosplmoura/Library/Mobile Documents/com$\sim$apple$\sim$CloudDocs/Medicina/2024_Mendeley/Ye et al.. 2020. Global prevalence, incidence, and outcomes of non-obese or lean non-alcoholic fatty liver disease a systematic review a.pdf:pdf},
issn = {24681253},
journal = {The Lancet Gastroenterology and Hepatology},
number = {8},
pages = {739--752},
pmid = {32413340},
publisher = {Elsevier Ltd},
title = {{Global prevalence, incidence, and outcomes of non-obese or lean non-alcoholic fatty liver disease: a systematic review and meta-analysis}},
url = {http://dx.doi.org/10.1016/S2468-1253(20)30077-7},
volume = {5},
year = {2020}
}
