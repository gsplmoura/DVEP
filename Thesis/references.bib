title = {{RELATIONSHIP BETWEEN PHASE ANGLE, HANDGRIP STRENGHT AND BMI IN ITALIAN WOMEN}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2002859689&from=export http://dx.doi.org/10.1016/S0261-5614(19)32351-9},
volume = {38},
