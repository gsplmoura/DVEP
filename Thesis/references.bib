journal = {Clinical Nutrition},
keywords = {Brazil,New York,X ray,aged,body mass,cardiac rhythm management device,case report,clinical article,conference abstract,congestive heart failure,female,general hospital,human,human tissue,impedance,male,muscle mass,normal value,nutritional status,prognosis,prosthesis,stadiometer,very elderly},
language = {English},
pages = {S141},
title = {{PHASE ANGLE IN PATIENTS WITH CONGESTIVE HEART FAILURE}},
