title = {{Patient and proxy ratings using the huntington's disease health-related quality of life questionnaire (HDQoL)}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L72199393&from=export http://dx.doi.org/10.1136/jnnp-2012-303524.142},
