journal = {Palliative Medicine},
keywords = {Delphi study,United Kingdom,adult,article,bereavement,controlled study,education,female,follow up,funding,grief,hospice,hospital patient,human,major clinical study,male,organization,palliative therapy,patient referral,semi structured interview},
