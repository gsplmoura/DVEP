title = {{Tools and Methods Used for the Assessment of Body Composition in Patients With Cystic Fibrosis: A Systematic Review}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L626296757&from=export http://dx.doi.org/10.1002/ncp.10247},
