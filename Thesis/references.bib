volume = {18},
year = {2016}
}
@article{Thibault2016,
abstract = {Introduction ICU patients prognosis is evaluated at admission by severity scores (APACHEII, SAPSII) that are time-consuming. 50 kHz phase angle (PhA) is measured easily and quickly by bioimpedance analysis (BIA). Lowered PhA is associated with worse prognosis of chronic diseases. The main aim was to determine whether PhA measured at ICU admission predicts 28-day survival. Patients and methods Patients were recruited in 10 centers from 9 countries. The inclusion criteria were: age >18 years, absence of implantable cardioverter defibrillator or pacemaker, expected ICU length of stay >48 h. PhA was measured at admission (day (d) 1) by the Nutriguard (Data-Input, Germany) BIA analyzer. APACHEII and SAPSII were calculated at d1. The following variables were collected: 28-day mortality, duration of mechanical ventilation, ICU length of stay, infections, and expressed as mean ± standard deviations or median ± interquartiles accordingly. Relations between PhA and 28-day mortality, duration of mechanical ventilation or ICU length of stay were analyzed by Kaplan- Meier actuarial curves and Spearman test. Sensitivity, specificity and PhA cutoffs associated with 28-day mortality were analyzed by ROC curves and the Youden method. Variables associated with 28-day mortality were determined by multivariate logistic regression. Results Thirty-three percentage of screened patients were analyzed: n = 918 (/1000 planned patients according to the power analysis sample calculation): Geneva, Switzerland (n = 178), Pelotas, Brazil (n = 103), Brussels, Belgium (n = 100), Vilnius, Lithuania (n = 100), Clermont-Ferrand, France (n = 99), Ljubljana, Slovenia (n = 96), Palma de Majorque, Spain (n = 94), Bydgoszcz, Poland (n = 52), Zagreb, Croatia (n = 49), Rouen, France (n = 47). The characteristics of analyzed patients were: 549 males, 61 ± 16 years, body mass index 26.4 ± 5.6, medical 63 %, shock all cause 29 %, APACHEII 19 ± 9, SAPSII 43 ± 19, PhA d1: 4.4 ± 1.7 degrees (°), 28-day mortality 21 %. Day 1 PhA was lower in non-survivors than in survivors (4.0 ± 2.0 vs 4.5 ± 1.7, P = 0.005). Sensitivity, specificity, positive predictive value (PPV) and negative predictive value (NPV) of d1 PhA < 3.49 for 28-day mortality were, respectively, 49, 74, 31 and 86 % (area under the curve (AUC) = 0.63 [0.58-0.68]). The Kaplan-Meier actuarial curve indicated a better 28-day survival in patients with d1 PhA ≥ 3.49° (P < 0.001). Survival was worse in the quartile of patients with d1 PhA < 3.23° vs. quartiles (3.23-4.24, 4.25-5.32, > 5.32) (P < 0.001). Variables associated with d28 mortality were: SAPSII ≥ 46 (hazard ratio (HR) 4.2 [2.1-8.3], P < 0.001), PhA < 3.49° (HR 2.2 [1.3- 3.8], P = 0.005) and age >64 years (HR 1.9 [1.0-3.5], P = 0.05). Sensitivity, specificity, PPV and NPV of a multivariate model associating d1 PhA < 3.49° and SAPSII ≥ 46 for 28-day mortality were 77, 66, 32 and 93 %, respectively (AUC = 0.78 [0.73-0.83]). There was a reverse correlation between d1 PhA and SAPSII (rho = -0.16), or APACHE II (rho = -0.19) (P < 0.001). The proportion of patients with mechanical ventilation was higher in patients with PhA ≤ 4.2° vs. PhA > 4.2°: 50 vs 31 %, P < 0.001. There was no relation between PhA and ICU length of stay or infections. Conclusion At ICU admission PhA is an indicator of 28-day mortality, even if SAPSII is the best predictor. A combined multivariate model associating PhA and SAPSII improves 28-day mortality predictability.},
address = {R. Thibault, Service d'Endocrinologie, Diab{\'{e}}tologie, Nutrition, Hospital Center University De Rennes, Rennes, France},
author = {Thibault, R and Makhlouf, A.-M. and Mulliez, A and Gonzalez, C and Dadet, S and Kekstas, G and Preiser, J.-C. and Rotovnik, K N and Rozalen, I C and Kupczyk, K and Krznaric, Z and Tamion, F and Cano, N and Pichard, C},
doi = {10.1186/s13613-016-0114-z},
issn = {2110-5820},
journal = {Annals of Intensive Care},
keywords = {APACHE,Belgium,Brazil,Croatia,France,Germany,Lithuania,Poland,Slovenia,Spain,Switzerland,analyzer,area under the curve,artificial ventilation,body mass,cardiac rhythm management device,chronic disease,hazard ratio,human,implantable cardioverter defibrillator,infection,intensive care,intensive care unit,length of stay,logistic regression analysis,male,model,mortality,observational study,patient,power analysis,predictive value,prognosis,receiver operating characteristic,resuscitation,society,survival,survivor},
language = {English},
title = {{Results of the international multicentric prospective observational study Phase Angle Project: Phase angle measured by bioimpedance analysis at intensive care unit (ICU) admission predicts 28-day survival}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L72343030&from=export http://dx.doi.org/10.1186/s13613-016-0114-z},
volume = {6},
year = {2016}
}
@article{Bering2016,
abstract = {Rationale: The choice of the best nutrition assessment method in chronic hepatitis C (CHC) remains a challenge. BIA is a potential instrument to evaluate muscular wasting and phase angle (PhA), which have been considered as important prognostic factors in liver diseases. Thus, we aimed to assess the muscle mass and PhA in patients with CHC. Methods: We included prospectively 148 consecutive CHC patients [77 (51.4%) males; mean age, 52.5 ± 11.6 yrs; 54 (36.5%) cirrhosis]. The diagnosis of liver diseases was based on standard clinical, biochemical, serological, radiological and histological parameters. Low PhA (<5th percentile) and muscle mass were measured by BIA. IPAQ was used to evaluate physical activity. Alcohol use and smoking were quantified. As a control group, we included 64 asymptomatic individuals who were matched by age and sex with CHC patients. Logistic regression analyses were used (SPSS 17.0). Results: Low muscle mass was found in 14 (21.9%) CHC patients [11(45.8%) with cirrhosis and 4 (10.0%) without cirrhosis] vs 6 (9.4%) asymptomatic individuals ( p < 0.0001). The low PhA was found in 21 (32.8%) CHC patients [13 (54.2%) with and 8 (20.0%) without cirrhosis] vs 7 (10.9%) asymptomatic individuals ( p < 0.0001). In multivariate analysis, adjusted by sex and age, cirrhosis (OR = 3.52; 95%CI = 1.15-10.80; p = 0.03) was independently associated with low muscle mass. Low PhA was independently associated with male sex (OR = 3.67; 95%CI = 1.32-10.19; p = 0.01) and cirrhosis (OR = 2.82; 95%CI = 1.20- 6.60; p = 0.02). Conclusion: In this study cirrhosis and male sex were associated with low muscle mass and PhA bellow the 5th percentile. These findings highlight the relevance of these BIA parameters as indicators of severity of liver disease stage in patients chronically infected by HCV.},
address = {T. Bering, Viral Hepatitis Ambulatory, Instituto Alfa De Gastroenterologia, Hospital Das Cl{\'{i}}nicas, Belo Horizonte, Brazil},
author = {Bering, T and Diniz, K G D and Coelho, M P P and {De Souza}, A C M and {De Melo}, L F and Soares, M M S and Kakehasi, A M and Teixeira, R and Rocha, G A and Vieira, D A and Silva, L D},
issn = {1532-1983},
journal = {Clinical Nutrition},
keywords = {adult,alcohol consumption,chronic hepatitis C,control group,controlled study,diagnosis,female,human,impedance,international physical activity questionnaire,liver cirrhosis,logistic regression analysis,major clinical study,male,middle aged,multivariate analysis,muscle mass,quantitative study,smoking},
language = {English},
