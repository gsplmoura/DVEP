journal = {Evidence-based Complementary and Alternative Medicine},
keywords = {1h indole 3 carboxaldehyde,2 propenyl 1 (1 propenylsulfinyl) propyl disulfide,20a dihydroprogesterone,ChiCTR1800014422,Chinese medicinal formula,Chinese medicine,Cistanche deserticola,Cuscuta chinensis,Eclipta prostrata,Ligustrum lucidum,Lotus (genus),Lycium barbarum,Rehmannia glutinosa,Rosa laevigata,adult,article,choline,chorionic gonadotropin,clinical trial,daphnemaline,decanoylcarnitine,dizziness,embryo transfer,fatigue,female,female infertility,glycerophospholipid,glycyrrhizae radix,gonal f,heel pain,human,humanos,icosatrienoic acid,in vitro fertilization,indoleacrylic acid,kidney,libido disorder,liquid chromatography-mass spectrometry,low back pain,major clinical study,metabolomics,mulberry,n acetyltryptophan,oocyte retrieval,ovary follicle fluid,ovary hyperstimulation,phosphorylcholine,qi deficiency,qi zi yu si decoction,raspberry,recombinant follitropin,steroidogenesis,testosterone acetate,tinnitus,transvaginal echography,triptorelin,tryptophan,unclassified drug},
