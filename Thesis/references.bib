file = {:Users/gustavosplmoura/Library/Mobile Documents/com$\sim$apple$\sim$CloudDocs/Medicina/2024_Mendeley/Singh, Bajpai, Mishra. 2021. Herbal folklore medication for liver disorders. Current Traditional Medicine.pdf:pdf},
