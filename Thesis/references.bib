doi = {10.1016/j.jep.2016.12.008},
file = {:Users/gustavosplmoura/Library/Mobile Documents/com$\sim$apple$\sim$CloudDocs/Medicina/2024_Mendeley/Morel et al.. 2017. A standardized methanol extract of Eclipta prostrata (L.) L. (Asteraceae) reduces bronchial hyperresponsiveness and.pdf:pdf},
