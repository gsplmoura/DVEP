title = {{Resting energy expenditure in elite athletes: development of new predictive equations based on anthropometric variables and bioelectrical impedance analysis derived phase angle}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L636625063&from=export http://dx.doi.org/10.1186/s12970-021-00465-x},
