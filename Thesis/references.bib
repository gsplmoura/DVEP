issn = {1573-2606},
journal = {Reviews in Endocrine and Metabolic Disorders},
keywords = {attention,bioelectrical impedance analysis,physical model,review,simulation,spectroscopy},
language = {English},
title = {{Bioimpedance basics and phase angle fundamentals}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2021362063&from=export http://dx.doi.org/10.1007/s11154-022-09780-3},
year = {2023}
}
@article{Ravasco2013,
abstract = {Rationale: Dietary pattern may influence body composition and is believed to influence cancer progression. This crosssectional study aimed to characterise nutritional intake of cancer pts & explore potential associations between diet, body composition & phase angle (PA). Methods: We evaluated 71(42M:29F) cancer pts referred for Radiotherapy. Parameters: PA assessed by tetrapolar multifrequency bioimpedance analysis (Biodynamics 450{\textregistered}, Seattle, USA) further compared with age/sex reference percentiles; muscle mass, assessed by Computed Tomography (n = 35); dietary intake, assessed by 24-hour recall. Food data were analysed by Food Processor SQL{\textregistered} software to obtain detailed daily nutrient intake, that were compared with estimated energy requirements (WHO formulas) and Dietary Reference Intakes (DRIs). Results: We found a deficit of energy and protein intake in ∼35% of pts. Fat intake was higher than the maximum recommended in 50% of pts, reaching 38% of their total energy intake (TEI); 40% of pts consumed excessive saturated fat (15±2% of TEI), concomitantly with deficient intake of polyunsaturated fats (5±1% of TEI). In most patients, intake of vitamins E & C was below DRIs. A PA< 5th percentile was associated with decreased energy & protein intakes (p < 0.02), and 55% of pts with low PA had excessive body fat (p < 0.05). Conclusion: More than 1/3 of patients had low energy/protein intake and inadequate diet pattern, excessive in total & saturated fats & poor in essential fatty acids & micronutrients. PA, an indicator of membrane integrity & metabolic efficiency, was associated with dietary deficits and inadequate body composition. These preliminary data support the continuation of this study, allowing further in-depth analyses in a larger cohort to explore the role of dietary components & nutrients in modulating cell membrane integrity & body composition. These results may be crucial to optimize the potential for favorable treatment response & efficacy.},
address = {P. Ravasco, Unit of Nutrition and Metabolism, Faculty of Medicine, University of Lisbon, Lisboa, Portugal},
author = {Ravasco, P and Almeida, A I and Ferreira, C and Johns, N and Fearon, K and Camilo, M and {Monteiro Grillo}, I},
issn = {0261-5614},
journal = {Clinical Nutrition},
keywords = {European,body composition,body fat,caloric intake,cancer growth,cell membrane,computer assisted tomography,diet,dietary intake,dietary reference intake,essential fatty acid,fat intake,food,human,membrane,metabolism,muscle mass,neoplasm,nutrient,nutrition,parameters,patient,polyunsaturated fatty acid,protein intake,radiotherapy,recall,sarcopenia,saturated fatty acid,society,software,trace element,treatment response,vitamin},
language = {English},
pages = {S60},
title = {{Dietary intake, sarcopenia and phase angle in cancer: New insights}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L71797569&from=export},
volume = {32},
year = {2013}
}
@article{Stellingwerf2020,
abstract = {Introduction: Phase angle, derived from bioimpedance analysis (BIA), reflects tissue quality and quantity in which cell mass, membrane integrity and hydration state are represented. Phase angle, as a measure of body composition, changes with the physical condition of patients and is associated with survival in several disease states and during ICU admission. We questioned whether phase angle can be used as a predictor of long term ICU outcome. Methods: A single-center prospective observational cohort with consecutive patients, admitted to the ICU between June 2018 and June 2019. Demographic data, APACHE III, comorbidity and phase angle in the first 6 hours after ICU admission were collected and the ICU-, hospital- and 6 months survival were registered. Results: Of 1025 patients included 8.5% (87) died within 6 months after ICU admission. Patients who died had a higher APACHE III score (95 [71- 128] versus 56 [46-68] p<0.001), and more often sepsis or malignancy. The phase angle was lower among non survivors (4.9° [4.0- 5.9] versus 5.4° [4.7-6.4] p< 0.001). The cut-off value for the phase angle, determined with the ROC is 4.6°. The Kaplan Meier curve (Figure 1) showed a significant difference (Log rank p<0.001) in mortality between patients with a phase angle beneath 4.6°. Conclusions: A low phase angle at time of ICU admission is a predictor of long term survival, with an optimal cut-off value of 4.6°.(Table Presented).},
address = {F. Stellingwerf, Medical Center Leeuwarden, Intensive Care, Leeuwarden, Netherlands},
author = {Stellingwerf, F and Koopmans, M and Buter, H and Boerma, E C},
doi = {10.1186/s13054-020-2772-3},
issn = {1466-609X},
journal = {Critical Care},
keywords = {APACHE,Kaplan Meier method,adult,cancer survival,cohort analysis,comorbidity,conference abstract,controlled study,demography,female,human,human tissue,long term survival,major clinical study,male,malignant neoplasm,mortality,predictive value,prospective study,sepsis,survivor},
language = {English},
title = {{The predictive value of phase angle on 6 month survival after ICU admission}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L633285355&from=export http://dx.doi.org/10.1186/s13054-020-2772-3},
volume = {24},
year = {2020}
}
@article{Pereira2018,
abstract = {Phase angle (PA) is a ratio between the reactance and resistance obtained by bioelectric impedance analysis and has been interpreted as a cell membrane integrity indicator and a predictor of total body cell mass. A low PA may suggest deterioration of the cell membrane, which in advanced cancer patients may result in a reduced overall survival (OS). This systematic review sought to investigate the current evidence regarding whether there is an association between PA and OS in patients with advanced cancer (ie, metastatic disease). The search was conducted on electronic databases in August 2017. A total of 34 articles were identified in the initial literature search. Nine studies reporting on 1496 patients were deemed eligible according to our inclusion criteria. PA data were analyzed as continuous variables or according to different cutoffs, under a frequency of 50 Khz. Low PA was associated with worse nutrition status evaluated by body mass index, serum albumin level, transferrin, and fat-free mass. The median OS of the included papers varied from 25.5–330 days, and all studies analyzed showed a significant association between PA and OS, in that patients with low PA had worse OS. Future studies are necessary to justify the use of PA in therapeutic decisions for this population and to evaluate whether nutrition status can influence the association between PA and survival.},
address = {L. Calixto-Lima, Palliative Care Unit, National Cancer Institute Jos{\'{e}} Alencar Gomes da Silva (INCA), Rio de Janeiro, RJ, Brazil},
author = {Pereira, M M E and Queiroz, M.D.S.C. and de Albuquerque, N M C and Rodrigues, J and Wiegert, E V M and Calixto-Lima, L and de Oliveira, L C},
doi = {10.1002/ncp.10100},
issn = {1941-2452},
journal = {Nutrition in Clinical Practice},
keywords = {C reactive protein,Karnofsky Performance Status,Palliative Prognostic Index,advanced cancer,albumin,body mass,cancer prognosis,fat free mass,hemoglobin,human,interleukin 6,metastasis,nutritional assessment,nutritional status,overall survival,phase angle,prognostic assessment,quality control,review,survival rate,systematic review,transferrin},
language = {English},
number = {6},
pages = {813--824},
title = {{The Prognostic Role of Phase Angle in Advanced Cancer Patients: A Systematic Review}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L622333528&from=export http://dx.doi.org/10.1002/ncp.10100},
volume = {33},
year = {2018}
}
@article{Kyriazis2016,
abstract = {Introduction and Aims: The PA parameter, obtained by BIA, as a reflection of body cell mass, tissue hydration, and membrane integrity, is a useful tool for identifying dialysis patients at high risk for malnutrition, a condition associated with high mortality in these patients. Also, elevated serum Ca (sCa) levels and treatment with high DCa (HDCa), both associated with an increased risk of Ca overload, have been linked with morbidity and mortality. In this study, we aimed to investigate the associations between DCa treatment and nutritional status, and specifically to explore the possibility whether treatment with a HDCa is causally associated with a low PA, indicative of malnutrition. Methods: A 18-month prospective longitudinal study was performed on 45 HD and 49 PD patients, with an additional follow-up period of 2.5 years. Biochemical markers of nutrition and body composition were measured at baseline, 6 and 18 months following enrollment. Half of the patients (n=47) were treated with a low DCa of 1.25 mmol/L (LDCa) and the other half (n=47) were treated with a HDCa of 1.75 mmol/L. DCa prescription remained constant during the whole study period. Results: In a linear mixed-effect model adjusted for age, dialysis vintage, cardiovascular disease and diabetes at baseline, gender, mode of dialysis and sCa, PA changes were inversely related to DCa concentration (r adjusted = -0.57; p<0.001) and linear changes of sCa (r adjusted = -0.10; p<0.05). PA levels decreased over time ( p<0.05) and treatment with a HDCa was associated with a decrease in PA levels of 0.15°/1.5 years (p = 0.005 for DCa × time interaction). During the 4-year follow-up, there were 24 deaths. In a Cox model , adjusted to the same covariates as mentioned above, the use of HDCa was significantly associated with a 3-fold increased all-cause mortality risk (HR: 3.06; 95% CI, 1.07-8.7), whereas, for each 1° increment of PA there was a significant 40% reduced risk for all-cause mortality (HR: 0.60; 95% CI, 0.39-0.92). Conclusions: This is the first study to show that the use of a HDCa is causally related to low Pa levels over time, thus identifying HDCa as a new inducer of malnutrition. The mechanisms underlying this association, which are independent of increased sCa levels, remain to be elucidated. Apart from being a significant predictor of malnutrition, a HDCa also constitutes a strong and independent risk factor for mortality, and thus, the use of DCa as high as 1.75 mmol/L should be strictly avoided and a LDCa be used instead.},
address = {P. Kyriazis, Transplant Institute, Beth Israel Deaconess Medical Center, Medicine, Boston, MA, United States},
author = {Kyriazis, P and Markaki, A and Rizos, A and Fragkiadakis, G and Pateinioti, M and Skoulikidi, S and Stylianou, K and Kyriazis, J},
doi = {10.1093/ndt/gfw176.18},
issn = {1460-2385},
journal = {Nephrology Dialysis Transplantation},
keywords = {biochemical marker,body composition,calcium,calcium blood level,cardiovascular disease,death,deoxycorticosterone acetate,diabetes mellitus,dialysate,dialysis,edetic acid,follow up,gender,hemodialysis,hemodialysis patient,human,hydration,impedance,longitudinal study,malnutrition,membrane,model,morbidity,mortality,nutrition,nutritional status,patient,peritoneal dialysis,prescription,proportional hazards model,risk,risk factor,tissues},
language = {English},
pages = {i304},
title = {{Effect of dialysate calcium concentration (DCA) on phase angle (PA), as assessed by bioelectrical impedance analysis (BIA), in hemodialysis (HD) and peritoneal dialysis (PD) patients}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L72326729&from=export http://dx.doi.org/10.1093/ndt/gfw176.18},
volume = {31},
year = {2016}
}
@article{Denneman2020,
abstract = {Background: Bioelectrical impedance analysis (BIA) is a validated method to assess body composition in persons with fluid homeostasis and reliable body weight. This is not the case during critical illness. The raw BIA markers resistance, reactance, phase angle, and vector length are body weight independent. Phase angle reflects cellular health and has prognostic significance. We aimed to assess the course of phase angle and vector length during intensive care unit (ICU) admission, and determine the relation between their changes ($\Delta$) and changes in body hydration. Methods: A prospective, dual-center observational study of adult ICU patients was conducted. Univariate and multivariable regression analyses were performed, including reactance as a marker of cellular mass and integrity and total body water according to the Biasioli equation (TBWBiasioli) and fluid balance as body weight independent markers of hydration. Results: One hundred and fifty-six ICU patients (mean ± SD age 62.5 ± 14.5 years, 67% male) were included. Between days 1 and 3, there was a significant decrease in reactance/m (−2.6 ± 6.0 $\Omega$), phase angle (−0.4 ± 1.1°), and vector length (−12.2 ± 44.3 $\Omega$/m). Markers of hydration significantly increased. $\Delta$phase angle and $\Delta$vector length were both positively related to $\Delta$reactance/m (r2 = 0.55, p ' 0.01; r2 = 0.38, p ' 0.01). Adding $\Delta$TBWBiasioli as explaining factor strongly improved the association between $\Delta$phase angle and $\Delta$reactance/m (r2 = 0.73, p ' 0.01), and $\Delta$vector length and $\Delta$reactance/m (r2 = 0.77, p ' 0.01). Conclusions: Our results show that during critical illness, changes in phase angle and vector length partially reflect changes in hydration.},
address = {N. Denneman, Department of Adult Intensive Care Medicine, Amsterdam UMC, VU University Medical Center, Amsterdam, Netherlands},
author = {Denneman, N and Hessels, L and Broens, B and Gjaltema, J and Stapel, S N and Stohlmann, J and Nijsten, M W and {Oudemans-van Straaten}, H M},
doi = {10.1038/s41430-020-0622-7},
issn = {1476-5640},
journal = {European Journal of Clinical Nutrition},
keywords = {Biasioli equation,adult,article,bioelectrical impedance analysis,body weight,cohort analysis,controlled study,critically ill patient,female,fluid balance,hospital admission,human,impedance,intensive care unit,limit of quantitation,major clinical study,male,mathematical analysis,observational study,phase angle,physical phase,prospective study,scoring system,standardization,total body water},
language = {English},
