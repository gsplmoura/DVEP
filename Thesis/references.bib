year = {2004}
}
@article{Sheibani2021,
abstract = {Background: Nutritional status (NS) is associated with respiratory outcomes in patients with cystic fibrosis (CF). Currently, NS in adults with CF is commonly assessed using body mass index (BMI) which can underestimate the prevalence of malnutrition. Muscle mass (MM) is positively associated with clinical outcomes and measures of body composition improve the nutritional assessment of patients with CF. Objectives: To evaluate the relationship between body composition and the key respiratory outcomes of FEV1 and IV antibiotic requirement. To examine the presence of normal weight obesity (NWO). This was defined as a high body fat percentage (>23% for females &?>30% for males) with a normal BMI (<25 kg/m2). Methods: 18 subjects with CF underwent bioelectrical impedance analysis (BIA), a form of body composition analysis. Lung function was determined as the percentage predicted forced expiratory volume in 1 second (FEV1%), as part of routine care. The number of days treatment with IV antibiotics was determined retrospectively for the previous (2019) calendar year. Data was analysed using a correlation coefficient (r). Results: MM% was negatively associated with the number of days treatment with IV antibiotics (r = 0.50, p = 0.04), but there was no significant association between MM% and FEV1% (r = 0.16, p = 0.06). Demographics: [Table Presented] Conclusion: The association of higher MM% with lower IV antibiotic requirement supports the importance of nutritional assessment in this patient group. The lack of significant association between MM% and FEV1% may be due to the limited sample size. The prevalence of NWO was high in these subjects. Identification of NWO can allow targeted intervention to increase MM and this may improve key respiratory outcomes in CF. This supports the use of body composition measures to assess nutritional status rather than BMI alone. Data collection will continue that may strengthen the current results.},
author = {Sheibani, L and Wilson, P and Watson, D and Shafi, N},
doi = {10.1016/S1569-1993(21)01200-5},
issn = {1873-5010},
journal = {Journal of Cystic Fibrosis},
keywords = {adult,antibiotic agent,body fat percentage,body mass,body weight,clinical article,conference abstract,controlled study,correlation coefficient,cystic fibrosis,demography,female,forced expiratory volume,human,impedance,lung function,male,muscle mass,normal weight obesity,nutritional assessment,nutritional status,prevalence,sample size},
language = {English},
pages = {S92},
title = {{The association between body composition and respiratory outcomes for patients with cystic fibrosis}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2012504442&from=export http://dx.doi.org/10.1016/S1569-1993(21)01200-5},
volume = {20},
year = {2021}
}
@article{Quirk1997,
abstract = {We have compared the use of bioelectrical impedance analysis (BIA) with anthropometry for the prediction of changes in total body potassium (TBK) in a group (n = 31) of children with cystic fibrosis. Linear regression analysis showed that TBK was highly correlated (r > 0.93) with height2/impedance, weight, height, and fat-free mass (FFM) estimated from skin-fold measurements. Changes in TBK were also correlated, but less well, with changes in height2/impedance, weight, height, and FFM (r = 0.69, 0.59, 0.44, and 0.40, respectively). The children were divided into two groups: those who had normal accretion of TBK (>5%/y) and those who had suboptimal accretion of TBK (<5%/y). Analysis of variance showed that the significant difference in the change in TBK between the groups was detectable by concomitant changes in impedance and weight but not by changes in height, FFM, or weight and height Z scores. The results of this study suggest that serial BIA measures may he useful as a predictor of progressive undernutrition and poor growth in children with cystic fibrosis.},
