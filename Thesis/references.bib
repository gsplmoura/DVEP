issn = {1178-6388},
journal = {Nutrition and metabolic insights},
keywords = {Helio Vannucchi,Julio Sergio Marchini,MEDLINE,NCBI,NIH,NLM,Nadia Juliana Beraldo Goulart Borges Haubert,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,PMC4425195,PubMed Abstract,choline,doi:10.4137/NMI.S24385,fructooligosaccharide,heart fat,hepatic steatosis,liver fat,pmid:25987847},
