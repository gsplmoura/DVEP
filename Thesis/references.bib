file = {:Users/gustavosplmoura/Library/Mobile Documents/com$\sim$apple$\sim$CloudDocs/Medicina/2024_Mendeley/Hemstreet et al.. 2006. Potassium and phosphorus repletion in hospitalized patients implications for clinical practice and the potential.pdf:pdf},
issn = {0300-7995},
journal = {Current Medical Research and Opinion},
keywords = {Healthcare information,Phosphate,Phosphorus,Potassium,Prescribing,Safety},
month = {dec},
