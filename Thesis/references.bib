journal = {Nutrition},
keywords = {accuracy,adult,analytical error,article,bioelectrical impedance analysis,bioenergy,body composition,body fat,body mass,controlled study,cystic fibrosis,dual energy X ray absorptiometry,fat,female,human,impedance,intermethod comparison,lean body weight,major clinical study,male,non invasive measurement,nutritional status,prediction,priority journal,sex difference,skinfold thickness},
