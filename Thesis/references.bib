pages = {84--96},
title = {{The evolution of nutritional support in long term ICU patients: From multisystem organ failure to persistent inflammation immunosuppression catabolism syndrome}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L608344950&from=export},
volume = {82},
