@article{King2017,
abstract = {Ivacaftor is associated with weight gain, including fat mass gain in cystic fibrosis (CF) patients with the CFTR-G551D mutation (1). Lumacaftor- ivacaftor is associated with reduced hospitalisation and intravenous antibiotic use, but its impact on body composition is not yet widely studied. This is particularly important in severe lung disease, as low FEV1% is associated with low body mass index (BMI) and fat-free mass (FFM) depletion. Aim: To evaluate changes in body composition in adults with CF with FEV1<40% predicted over the first six months of treatment with lumacaftor- ivacaftor. Methods: Data were analysed on 20 adults with CF (10 female, mean±SD age 34.1±8.9 years, baseline mean±SD FEV1 33.3±6.1%predicted, mean±SD BMI 19.8±2.2kg/m2), who received lumacaftor-ivacaftor for six months, and had body composition measured using multifrequency bioelectrical impedance analysis (mBCA, SECA, Germany) at baseline and 6 months. Changes($\Delta$) in weight, total body water (TBW), extra- and intra-cellular water (ECW, ICW), FFM and fat mass (FM) were determined. ICW reflects body cell mass (BCM). Paired t-tests, Pearson's correlations and multiple regression were undertaken. Results: At six months, mean weight and fat mass significantly increased: (2.2±3.9kg (p=0.02) and 2.1±2.6kg (p=0.02) respectively). No significant changes were observed in FFM (mean ±SD change +0.5±2.3kg), TBW (+0.4±1.7kg), ECW (0±0.7kg) or ICW (+0.4±1.2kg). There was wide variation in body composition changes: four patients gained ≥2kg FFM while three lost ≥2kg FFM. Ten patients gained ≥2kg FM. Gender, age and baseline FEV1% were not associated with body composition changes. Lower baseline BMI was correlated with greater gains in weight (r= -0.60, p=0.005) and FFM (r= -0.46, p=0.04). Of the seven adults with BMI<18.5kg/m2 at baseline, only one remained below BMI 18.5kg/m2 at six months. In multiple regression, $\Delta$fat mass(p<0.0001) and $\Delta$ICW(p=0.002), but not $\Delta$ECW, were significant predictors of $\Delta$weight. Conclusions: Weight gain was observed following six months of lumacaftor- ivacaftor treatment. However, there was a variable body composition response with 20% of adults achieving clinically important gains in FFM, and half gaining fat mass. Monitoring body composition permits more specific evaluation of the tissue composition of weight changes in CF patients on ivacaftor-lumacaftor. The independent association of change in ICW, but not ECW, with weight changes suggests that increases in BCM, rather than in extravascular fluid retention, explains changes in FFM. The mechanisms underlying body composition changes require further investigation, including mediation via improved exercise tolerance and/or appetite and amelioration of catabolism related to reduced exacerbations.},
address = {S.J. King, Nutrition Department, Alfred Hospital, Melbourne, VIC, Australia},
author = {King, S J and Williams, E and Allen-Graham, J and Finlayson, F and Button, B M and Clark, D and Kotsimbos, T and Keating, D and Tierney, A and Wilson, J W},
doi = {10.1002/ppul.23840},
issn = {1099-0496},
journal = {Pediatric Pulmonology},
keywords = {Germany,Student t test,adult,antibiotic agent,appetite,body mass,body weight gain,catabolism,cell body,clinical article,cystic fibrosis,disease exacerbation,drug therapy,exercise tolerance,extravascular fluid,fat free mass,female,fluid retention,forced expiratory volume,gender,human,human tissue,impedance,ivacaftor plus lumacaftor,lung disease,male,monitoring,multiple regression,total body water,water},
language = {English},
pages = {448--449},
title = {{Impact of lumacaftor-ivacaftor on body composition in adults with severe cystic fibrosis lung disease}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L619069506&from=export http://dx.doi.org/10.1002/ppul.23840},
volume = {52},
year = {2017}
}
@article{Azcue1993,
abstract = {The purpose of this study was to measure whole-body bioelectrical variables and to validate the technique of bioelectrical impedance analysis (BIA) as a measure of total body water (TBW) in patients with cystic fibrosis (CF). In 20 normally nourished males with CF, aged 7 to 39 years, bioelectrical impedance measurements were compared with TBW measured by isotopic distribution of H218O. The relationship between the two measurements was found to be highly significant (r = 0.96; p < 0.0001). The major genotype mutation $\Delta$F508 was found to have no effect on BIA. Comparison of the CF patients with 21 normal controls matched for age and TBW volumes as measured by H218O volume of distribution showed no difference in electrical parameters. However, the slope of the regression lines (H218O, H2/R) was significantly steeper for the CF subjects (p < 0.03) than for controls. These data show that BIA can be used to measure TBW in patients with CF. However, the predictive equations developed in healthy control subjects should not be used; rather, separate predictive equations for patients with CF will need to be developed.},
