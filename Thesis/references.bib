}
@article{Dierich2010,
abstract = {Rationale: End-stage pulmonary diseases and postoperative complications lead to impaired body composition (BC) and exercise performance. We hypothesized that pulmonary rehabilitation subsequently following lung transplantation (LTx) improves BC and exercise performance. Methods: between February 2008 and February 2009 patients after LTx underwent 3-week in-patient rehabilitation. Bio impedance analysis (BIA: lean body mass (LBM), body cell mass (BCM), BCM/LBM ratio, phase angle (PA), body weight, lung function testing (VC, FEV1, 6-min.-walk (6-MW), ergospirometry (max. workload (maxWL)), anaerobic threshold (AT) and activities of daily living (ADL) were recorded at baseline and at the end. Results: 73 patients (median age (IQR): 47 (35-59) years, m:f 41:32) after LTx (single 9, bilateral 58, heart-lung 6) were included. 24 suffered emphysema (COPD), 13 idiopathic pulmonary fibrosis (iPF), 17 cystic fibrosis (CF), 7 pulmonary hypertension (PH), 7 Re-do-LTx for BOS and 5 other diseases. Overall improvement (baseline vs. end) revealed: VC (median 58 (IQR 46 - 67) vs. 66 (54-78) %, p < 0.001), FEV1 (56 (45-69) vs. 67 (54 - 78)%, p < 0.001), 6-MW (321 (200-417) vs. 425 (332-515) m, p < 0.001), maxWL (33 (27-42) vs. 41 (34-50)%, p < 0.001), ADL (mean 95 (± 8) vs. 99 (±2 ), p < 0.001). AT did not enhance. Overall body weight (61.5 (50.9 - 72.2) vs. 61.7 (51.0 -71.8), p = 0.16) and LBM did not change. BCM (17.2 (14.2-21.3) vs. 17.4 (15.0-22.1), p < 0.01), BCM/LBM ratio (37.0 (33.6-40.5) vs. 38.8 (35.4-42.3), p < 0.001), PA (3.6 (3.2-4.1) vs. 3.8 (3.4-4.3), p < 0.001) improved significantly. In BIA subgroup analysis CF (BCM 16 (12-21) vs. 17 (13-23), p = 0.002, BCM/LBM (38 (36-42) vs. 41 (37-44), p = 0.001), PA (3.7 (3.4-4.2) vs. 4.1 (3.6-4.6), p = 0.001), PH (BCM/LBM (37 (34-40) vs. 41 (35-44), p = 0.018, PA 3.6 (3.2-3.9) vs. 4.1 (3.4-4.6), p = 0.017) and BOS (BCM 17 (13-17) vs. 18 (12-22), p = 0.04, BCM/LBM 33 (30-39) vs. 37 (33-40), p = 0.05, PA 3.1 (2.8- 3.8) vs. 3.5 (3.1-4.0), p = 0.05) demonstrated pronounced effects in BIA, while COPD and iPF did not. Conclusion: a pulmonary 3-week in-patient rehabilitation following lung-transplantation improves exercise performance overall. Significant improvement in body composition was observed in CF, PH and BOS patients only. COPD and iPF patients showed insignificant changes. Longer duration of pre-transplant disease, co-morbidity and advanced age may account for these results.},
address = {M.G. Dierich, Hannover Medical School, Hannover, Germany},
author = {Dierich, M G and Geldmacher, H and Fuehner, T and Simon, A and Welte, T and Gottlieb, J},
issn = {1073-449X},
journal = {American Journal of Respiratory and Critical Care Medicine},
keywords = {anaerobic threshold,body composition,body weight,cystic fibrosis,daily life activity,diseases,emphysema,exercise,fibrosing alveolitis,heart,hospital patient,human,impedance,lean body weight,lung,lung disease,lung function,lung transplantation,mannomustine,morbidity,pH,patient,postoperative complication,pulmonary hypertension,pulmonary rehabilitation,rehabilitation,society,transplantation,workload},
language = {English},
number = {1},
title = {{Effect of a 3 week in-patient rehabilitation program following lung transplantation on body composition and exercise performance}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L70841765&from=export},
volume = {181},
year = {2010}
}
@article{VukicDugac2020,
abstract = {Objectives: Pulmonary dysfunction is the main cause of mortality in cystic fibrosis and has a close relationship with malnutrition. Therefore, early detection of malnutrition is essential to starting nutritional interventions as soon as possible. Routinely BMI is used as a marker of malnutrition but Bio-electrical Impedance Analysis (BIA) is now widely used method in body composition assessment. Our aim was to evaluate relationship between BMI, pulmonary function tests and BIA variables - fat free mass (FFM), sarcopenic index (SI), phase angle (PA) and fat free mass index (FFMI). Methods: 30 adult CF patients treated in adult CF centre in Croatia were included. After obtaining basic demographic, anthropometric and clinical data, each patient had pulmonary function tests and BIA performed. Results: 30 CF patients (17F, 13M) entered the study. Mean age, FEV1, BMI and FFMI were 24.7 years (range 17–34), 2.03 L (range 0.53–4.38), 13.1 kg/m2 (range 13.1–28.7) and 16.9 kg/m2 (range 12.9–21.5), respectively. We found positive correlations between FEV1 and BMI, FFMI, PA and SI (r = 0.31, p = 0.09; r = 0.52, p = 0.0002; r = 0.45, p = 0.01; r = 0.65, p = 0.0001, respectively). Multiple linear regression showed statistically significant influence of sarcopenia on pulmonary function (p = 0.04). There was a 0.4 increase in SI for each extra litre in FEV1. Conclusion: We have presented the first evaluation of nutritional status on national level regarding adult CF population in Croatia. Sarcopenic index may be a better prognostic indicator of severe pulmonary dysfunction in adult CF patients than BMI, and the early recognition of malnutrition by BIA may improve clinical outcomes. These preliminary results point towards the importance of an interdisciplinary approach and obligatory body composition studies while assessing CF patients.},
author = {{Vukic Dugac}, A and Ladic, A and Tanackovic, L and Dobric, L T and Crnogorac, I K and Dobric, A and Bambir, I and Jalusic-Gluncic, T and {Odobasic Palkovic}, T and Tjesic-Drinkovic, D},
doi = {10.1016/S1569-1993(20)30313-1},
issn = {1873-5010},
journal = {Journal of Cystic Fibrosis},
keywords = {Croatia,adult,body composition,body mass,clinical article,clinical outcome,conference abstract,controlled study,cystic fibrosis,demography,fat free mass,female,forced expiratory volume,human,impedance,infant,lung disease,lung function test,male,malnutrition,molecular recognition,nutritional status,outcome assessment,preliminary data,sarcopenia},
language = {English},
pages = {S47},
title = {{Association of body composition and lung function in adult patients with cystic fibrosis - data from Croatia}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2006056831&from=export http://dx.doi.org/10.1016/S1569-1993(20)30313-1},
volume = {19},
year = {2020}
}
@article{Pichard1999,
abstract = {Assessment of fat-free mass (FFM) and fat mass in ambulatory and hospitalized patients permits optimal adaptation of nutrition support. Recent methods for the determination of FFM are dual-energy x-ray absorptiometry (DXA) and bioelectrical impedance analysis (BIA). The purpose of this study was to determine if the BIA-determined FFM as estimated by the formula by Kotler et al.1 and the Geneva formula2 could be validated when compared to DXA-determined FFM in subjects with various diagnoses. Body composition was measured by BIA and DXA in 480 subjects including healthy young men and women, elite female runners, and patients with various pathologies (including chronic obstructive and restrictive pulmonary disease patients; cystic fibrosis patients; lung, heart, and liver transplantation patients [both pre- and post-], and hemiplegic and AIDS patients). The present results suggest that BIA is relevant in the clinical assessment of body composition, but BIA formulas appropriate to the subject pool must be used for evaluation of FFM and fat mass. The BIA formula by Kotler et al.1 is appropriate for healthy subjects and patients with AIDS, cystic fibrosis, and pre- or postliver transplantation. The Geneva formula2 is more appropriate in pulmonary diseases and hemiplegic subjects with normal weight. Copyright (C) 1999 Elsevier Science Inc.},
address = {C. Pichard, Clinical Nutrition, Geneva University Hospital, 1211 Geneva, Switzerland},
