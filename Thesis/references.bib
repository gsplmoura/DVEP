author = {Lucchetti, Giancarlo and {Lamas Granero}, Alessandra and Bassi, Rodrigo Modena and Latorraca, Rafael and {Aparecida Da}, Salete and Nacif, Ponte and Da, Recebido and {M{\'{e}}dico-Esp{\'{i}}rita De}, Associa{\c{c}}{\~{a}}o and Paulo, S{\~{a}}o and Artigo, S P and Revis{\~{a}}o, D E},
