title = {{Phase angle assessment by bioelectrical impedance analysis and its predictive value for malnutrition risk in hospitalized geriatric patients}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L607841835&from=export http://dx.doi.org/10.1007/s40520-015-0528-8},
volume = {28},
year = {2016}
}
@article{Parepally2018,
abstract = {Introduction: Sarcopenia is common in liver transplant candidates and is associated with adverse clinical outcomes. Phase angle measurement from bioelectrical impedance analysis (BIA) provides a means to assess for sarcopenia. Unlike computed tomography (CT) measurement of skeletal muscle, BIA is inexpensive and can be easily performed in an office setting. We evaluated the association between BIA measurements and outcomes of patients on our liver transplant waiting list. Methods: 106 consecutive patients evaluated for liver transplant from February 2015 to March 2016 underwent nutritional assessment testing using BIA. Patients with a phase angle ≤ 4.9 were identified as having sarcopenia (Ruiz-Margain 2015). Clinical data was retrospectively evaluated 18 months after the enrollment period. Data included pretransplant survival, transplant listing status, complications of liver disease, laboratory values and hospitalizations. Results: At the time of review, 72 (68%) patients were alive and 34 (32%) patients were transplanted. The average age was 58 ± 9 years and 66% were men. The three most common causes of liver disease were alcohol (36%), viral hepatitis (36%) and NASH (18%). 40% had hepatocellular carcinoma. The mean Model for End-Stage Liver Disease (MELD) score was 14 ± 5 and complications of liver disease included hepatic encephalopathy in 57%, varices in 63%, refractory ascites in 20% and renal insufficiency in 29%. Diabetes was found in 39%, obesity in 55%, hypertension in 38% and dyslipidemia in 31%. Overall, sarcopenia was found in 31% of transplant candidates. Sarcopenic patients were older (61 ± 7 vs 57 ± 9 years, p=0.005) and had higher rates of renal insufficiency (45% vs 22%, p=0.014) and hepatic encephalopathy (73% vs 50%, p=0.029). There were no differences in MELD score, components of metabolic syndrome or other complications of liver disease based on muscle mass. Survival analysis is shown in Figure 1. Patients with normal muscle mass had improved survival compared to those with sarcopenia. Patients with sarcopenia had lower rates of transplant (15% vs 40%, p=0.012). In Cox regression analysis, age, pretransplant renal insufficiency, hepatic encephalopathy, MELD and sarcopenia were entered. Age and pretransplant renal insufficiency remained associated with survival, whereas sarcopenia was not. Conclusions: Patients with evidence of sarcopenia by BIA who underwent evaluation for liver transplantation were more likely to have complications of cirrhosis, increased mortality and a decreased likelihood of undergoing liver transplantation. As a measure of sarcopenia, BIA may be a useful prognostic tool in patients undergoing liver transplant evaluation. The possibility that sarcopenia may be more reflective of age and worsening renal function as causes of pretransplant mortality should be examined further. [Figure Presented]},
author = {Parepally, M and Sorenson, J A and Cotler, S J and Kallwitz, E},
doi = {10.1016/S0016-5085(18)33784-3},
issn = {1528-0012},
journal = {Gastroenterology},
keywords = {Model For End Stage Liver Disease Score,adult,alcohol,animal experiment,animal model,ascites,cancer patient,cancer prognosis,cancer survival,clinical evaluation,clinical outcome,complication,conference abstract,controlled study,diabetes mellitus,dyslipidemia,end stage liver disease,female,hepatic encephalopathy,hospital admission,hospitalization,impedance,kidney failure,kidney function,liver cell carcinoma,liver cirrhosis,liver graft,male,metabolic syndrome X,mortality,muscle mass,nonalcoholic steatohepatitis,nonhuman,nutritional assessment,obesity,outcome assessment,retrospective study,sarcopenia,surgery,varicosis},
language = {English},
number = {6},
pages = {S--1140},
title = {{PHASE ANGLE AND BIOELECTRICAL IMPEDANCE ANALYSIS AS PREDICTORS OF CLINICAL OUTCOMES IN LIVER TRANSPLANT CANDIDATES}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2000864138&from=export http://dx.doi.org/10.1016/S0016-5085(18)33784-3},
volume = {154},
year = {2018}
}
@article{Blake2013,
abstract = {Background. Upper gastrointestinal (UGI) surgery is by definition high risk (in hospital operative mortality risk of ≥5%) and contemporary validated risk assessment tools include the American Society of Anaesthesiologists (ASA) physical status classification system, Cardiopulmonary exercise (CPEX) testing and the Physiological and Operative Severity Score for the enUmeration of Mortality and Morbidity (POSSUM). More recently Bioelectrical Impedance Analysis (BIA) and the calculated phase angle (PhA) has been championed as a prognostic factor in colo-rectal, pancreatic and lung cancer. Aim. The aim of this study was to determine the value of PhA as a risk assessment tool in UGI cancer surgery when compared with established validated surgical algorithms. Methods. Consecutive 50 patients (median age 66, 35 males, 29 esophageal and 21 gastric cancers) were studied prospectively. All patients underwent BIA (Maltron Bioscan 920) assessment pre-operatively and on post-operative days 1, 3 and 5, and Cardiopulmonary exercise testing (MedgraphicsTM CPX UltimaTM hardware; BreezesuiteTM and WelchAllyn{\textregistered} software) pre-operatively within an enhanced recovery after surgery programme. Primary outcome measures were post operative morbidity, mortality and length of hospital stay. Results. The median PhA was 7.28° (4.94° to 15.13°). PhA correlated significantly with patient demographic variables of age (p= 0.010), gender (p=0.018), ASA grade (p=0.029), deprivation score (p=0.006), percent fat free mass (p= 0.029), percent body fat (p=0.002) and muscle mass in kg (p=0.000). There were significant correlations with disease-related variables including tumour site (esophageal cancer median PhA 8.56° versus gastric cancer PhA of 7.19°, p=0.025), pathological N stage (p=0.020) and number of lymph node metastases (p=0.031). There was a universal reduction in PhA on the first post-operative day (median 1.21°, 15.3% reduction, range 0.20° to 4.23°). Preop PhA did not correlate with the incidence of post-operative morbidity, but the level of reduction in PhA correlated significantly with total hospital length of stay (P < .001). Conclusion. BIA correlated significantly with some but not all prognostic variables derived from established risk assessment tools. Further research is warranted in this arena to establish critical prognostic values and the precise role of BIA versus CPEX as a risk assessment tool.},
address = {P.A. Blake},
author = {Blake, P A and Beamish, A J and Karran, A and Chan, D S and Howell, G and Lewis, W G},
issn = {0016-5085},
journal = {Gastroenterology},
keywords = {algorithm,body fat,cancer surgery,classification,computer,digestive system cancer,esophagus cancer,exercise test,fat free mass,gastrointestinal disease,gastrointestinal surgery,gender,hospital,hospitalization,human,impedance,length of stay,lung cancer,lymph node metastasis,male,morbidity,mortality,muscle mass,neoplasm,patient,risk,risk assessment,society,software,stomach cancer,surgery,surgical mortality},
language = {English},
number = {5},
pages = {S202},
title = {{Prognostic significance of the phase angle (PHA) determined by bioelectrical impedance analysis in upper gastrointestinal cancer surgery}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L71116506&from=export},
volume = {144},
year = {2013}
}
@article{Kyle2013,
abstract = {Background & aims: This study determined the association between phase angle (PhA), by bioelectrical impedance analysis (BIA) and nutritional risk by Nutritional Risk Screening (NRS-2002), Subjective Global Assessment (SGA), hospital length of stay (LOS) and 30 day non-survival in patients at hospital admission compared to healthy controls. Methods: PhA was determined by BIA in patients (n = 983, 52.7 ± 21.5 yrs, M 520) and compared to healthy age-, sex- and height-matched controls. Low PhA was set at <5.0° (men) and <4.6° (women) as previously determined (Kyle, in press). Results: PhA was lower in patients (men 6.0 ± 1.4°, women 5.0 ± 1.3°) than controls (men 7.1 ± 1.2°, women 6.0 ± 1.2°, un-paired t-test p < 0.001). Patients were more likely to have low PhA than controls: NRS-2002: no risk (relative risk (RR) 1.7, 95th confidence interval (CI) 1.2-2.3), moderate risk (RR 4.5, CI 3.4-5.8) and severe risk (RR 7.5, CI 5.9-9.4); similar results were obtained by SGA; LOS ≥21 days (RR 6.9, CI 5.1-9.1) and LOS 5-20 days (RR 5.2, CI 3.9-6.9) and non-survivors (RR 3.1, CI 2.1-3.4) compared to survivors. Conclusions: There is a significant association between low PhA and nutritional risk, LOS and non-survival. PhA is helpful to identify patients who are at nutritional risk at hospital admission in order to limit the number of in-depth nutritional assessments. {\textcopyright} 2012 Elsevier Ltd and European Society for Clinical Nutrition and Metabolism.},
address = {C. Pichard, Clinical Nutrition, Geneva University Hospital, 1211 Geneva, Switzerland},
author = {Kyle, U G and Genton, L and Pichard, C},
doi = {10.1016/j.clnu.2012.08.001},
issn = {0261-5614},
journal = {Clinical Nutrition},
keywords = {adult,analytic method,article,bioelectrical impedance analysis,clinical assessment tool,controlled study,female,hospital admission,human,length of stay,major clinical study,male,malnutrition,nutritional assessment,nutritional parameters,nutritional risk,phase angle,physical phenomena,risk assessment,subjective global assessment,survivor},
language = {English},
number = {2},
pages = {294--299},
title = {{Low phase angle determined by bioelectrical impedance analysis is associated with malnutrition and nutritional risk at hospital admission}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L52173073&from=export http://dx.doi.org/10.1016/j.clnu.2012.08.001},
volume = {32},
year = {2013}
}
@article{Chew2019,
abstract = {Introduction: BIA estimation of fat-free mass is an indirect method to assess body composition for diagnosis of sarcopenia, but its accuracy is questionable. BIA-derived phase angle (PhA) is obtained from direct impedance measures and is a potential marker for sarcopenia. We aim to examine the association between PhA and muscle mass, strength and physical performance in non-frail communitydwelling older adults. Methods: We recruited 189 individuals (mean age 67.6 ± 7.5) who were non-frail by modified Fried criteria. We measured PhA and muscle mass using single frequency Quantum IV Body Composition Analyzer (RJL Systems, USA). Muscle parameters measured included skeletal muscle mass index (SMI, as measured by BIA and DXA), muscle strength (grip strength and knee extension) and physical performance (Short Physical Performance Battery (SPPB) and gait speed). We examined the association of PhA with sarcopenia (EWGSOP criteria with Asian Working Group cutoffs). Multiple linear regression was performed for the association of PhA with muscle mass, strength and performance, adjusted for age and gender. Results: Thirty-one (16%) were sarcopenic. Mean PhA was lower in sarcopenic versus non-sarcopenic individuals (5.0 ± 0.6 vs 5.3 ± 0.7, P = 0.017). There was a strong positive correlation between PhA and BIA-derived SMI (r = 0.79, P<0.001), and a moderate positive correlation with DXA-derived SMI (r = 0.49, P<0.001). After adjusting for age and gender, PhA remained independently associated with BIA-and DXA-derived SMI (BIA: b = 1.2, 95% CI 1.0-1.4, P<0.001, DXA: b = 0.4, 95% CI 0.21-0.57, P<0.001), muscle strength (handgrip strength: b = 2.5, 95% CI 1.1-3.8, P<0.001, knee extension: b = 1.2, 95% CI 1.0-1.4, P<0.001) and total SPPB score (b = 0.3, 95% CI 0.2-0.6, P = 0.03), but not gait speed (b = 0.04, 95% CI-0.03 to 0.1, P = 0.26). Conclusions: Amongst non-frail community-dwelling adults, lower PhA is associated with impaired muscle mass, strength and physical performance. Further studies are required to assess the utility of PhA to diagnose sarcopenia.},
address = {J. Chew, Department of Geriatric Medicine, Tan Tock Seng Hospital, Singapore, Singapore},
author = {Chew, J and Lim, J P and Yeo, A and Yew, S and Tan, C N and Hafizah, N and Ding, Y Y and Yu, S C and Lim, W S},
doi = {10.1007/s41999-019-00221-0},
issn = {1878-7657},
journal = {European Geriatric Medicine},
keywords = {adult,aged,body composition analyzer,community dwelling person,conference abstract,controlled study,female,gender,grip strength,human,impedance,knee,major clinical study,male,muscle mass,muscle strength,physical performance,sarcopenia,skeletal muscle,walking speed},
language = {English},
pages = {S23},
title = {{The association of bioelectrical impedance analysis (BIA)-derived phase angle with sarcopenia in non-frail community-dwelling older adults}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L631721811&from=export http://dx.doi.org/10.1007/s41999-019-00221-0},
volume = {10},
year = {2019}
}
@article{Ballarin2022,
abstract = {Phase angle (PhA), a directly-measured bioelectrical impedance analysis variable, is suggested to be a proxy of body cell mass as well as extracellular/intracellular water ratio, and is related to cellular integrity and functions. The aim of this systematic review was to evaluate PhA in healthy youths in relation to sex, age, weight status, physical fitness, and sports activities. A systematic literature search (preferred reporting items for systematic reviews and meta-analyses criteria) until January 2022 was performed using PubMed, Embase, Scopus, and Web of Science regarding studies on PhA in healthy children and adolescents 4-18 years of age. Quality was assessed according to the National Institute of Health. After removing duplicates and studies not fulfilling the inclusion criteria, 22 cross-sectional and 1 longitudinal were considered appropriate. As for quality, 14 articles were rated fair and 9 good. Ten studies found that PhA increases with age: the increase was more marked after puberty, whereas changes in younger subjects are by far less defined. A clear sex difference was found in adolescents, likely due to pubertal development. Limited evidence suggests that PhA increases in participants with very high BMI. Limited data were reported on physically active youths without convincing findings. Positive associations of PhA with physical fitness and fat-free mass were found in few studies. In conclusion, partial and limited evidence suggests that changes in PhA over the first 2 decades of life reflect modification in body composition and fat-free mass composition. Further studies are needed for confirming PhA as a relevant marker of nutritional status in youths.},
address = {G. Ballarin, Department of Movement Sciences and Wellbeing, University of Naples Parthenope, Naples, Italy},
author = {Ballarin, G and Valerio, G and Alicante, P and {Di Vincenzo}, O and Scalfi, L},
doi = {10.1097/MPG.0000000000003488},
issn = {1536-4801},
journal = {Journal of Pediatric Gastroenterology and Nutrition},
keywords = {Embase,Medline,Preferred Reporting Items for Systematic Reviews a,Scopus,Web of Science,adolescent,adult,body composition,body mass,body weight,child,fat free mass,female,fitness,human,impedance,juvenile,male,meta analysis,muscle,national health organization,nutritional status,puberty,review,sex difference,sport,systematic review,young adult},
language = {English},
number = {2},
pages = {120--130},
title = {{Bioelectrical Impedance Analysis (BIA)- Derived Phase Angle in Children and Adolescents: A Systematic Review}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2019490827&from=export http://dx.doi.org/10.1097/MPG.0000000000003488},
volume = {75},
year = {2022}
}
@article{Player2019,
abstract = {Background: Nutritional status can be difficult to assess. Bioelectrical impedance analysis (BIA)-derived phase angle (PA), and the plasma markers citrulline and transthyretin (pre-albumin) have the potential to assist, but the protocol of fasting and resting for BIA renders the investigation impractical for routine use, especially so in populations at high risk of malnutrition. Aims: 1 To clarify whether starving and resting are necessary for reliable measurement of PA. 2 To identify whether PA, citrulline and transthyretin correlate with nutritional status. Methods: Eighty consenting adult in-patients were recruited. Nutritional status was determined by subjective global assessment (SGA) used as gold standard. The Malnutrition Universal Screening Tool (MUST) was used and anthropometric measurements were performed. Serum was analysed for citrulline and transthyretin. PA was measured using Bodystat 4000. The PA was considered to define malnutrition when lower than reference ranges for sex and age, and severe malnutrition if more than 2 integers below the lower limit. Anthropometric measurements were categorised according to WHO reference centiles. Ordinal logistic regression estimated the strength of association of PA, citrulline and transthyretin with SGA. PA values in the different metabolic states were compared using paired t tests. Results: All 80 subjects completed the BIA and the nutritional assessments in the 3 different states; 14 declined to provide blood samples for the biochemical assays. Malnutrition was identified in 32 cases, severe malnutrition in 14 cases, the remaining 34 cases were deemed not to be malnourished. PA was strongly inversely associated with SGA (Odds Ratio [OR] per unit increase = 0.21, CI 0.12–0.37, p < 0.001). PA was not influenced by exercise (p = 0.134) or food intake (p = 0.184). Transthyretin was inversely associated with malnourished/severely malnourished states (OR = 0.98, 95% CI 0.97–0.99, p = 0.001), but had poorer predictive values than PA. There was no significant association between citrulline concentration and SGA (OR = 1.01, 95% CI 0.99–1.04, p = 0.348). Conclusions: The BIA-derived PA reliably identifies malnutrition. It is strongly associated with SGA but requires less skill and experience, and out-performs circulating transthyretin, rendering it a promising and less operator-dependent tool for assessing nutritional status in hospital patients. Our novel demonstration that fasting and bed-rest are unnecessary consolidates that position.},
address = {A. Forbes, Norwich Medical School, Bob Champion Building, James Watson Road, Norwich, United Kingdom},
author = {Player, E L and Morris, P and Thomas, T and Chan, W Y and Vyas, R and Dutton, J and Tang, J and Alexandre, L and Forbes, A},
doi = {10.1016/j.clnu.2018.08.003},
issn = {1532-1983},
journal = {Clinical Nutrition},
keywords = {Global Assessment of Functioning,adult,aged,amino acid blood level,article,biochemical analysis,bioelectrical impedance analysis derived phase ang,blood analysis,citrulline,disease severity,female,hospital care,human,major clinical study,male,malnutrition,nutritional assessment,nutritional health,predictive value,protein blood level,risk assessment,skinfold,transthyretin},
language = {English},
number = {4},
pages = {1700--1706},
title = {{Bioelectrical impedance analysis (BIA)-derived phase angle (PA) is a practical aid to nutritional assessment in hospital in-patients}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2001058650&from=export http://dx.doi.org/10.1016/j.clnu.2018.08.003},
volume = {38},
year = {2019}
}
@article{Langer2021,
abstract = {Objective: This study aimed to examine associations between phase angle (PhA) and incident cardiovascular disease (CVD) morbidity and mortality in a healthy Danish subpopulation free of major chronic diseases. Methods: A random subset (n = 2601) of adult men and women born in 1922, 1932, 1942, and 1952 and examined in 1987 and 1988 were included, and followed over 24 y during which 643 men and 570 women developed CVD. Measures at baseline included age, weight, height, whole-body bioimpedance, from which PhA was calculated, and information on lifestyle, obtained by a self-administered questionnaire. The association between PhA and incident CVD was assessed by Cox proportional hazard model with age as the underlying time scale and with additional adjustment for covariates. To explore nonlinear associations, all results were presented using restricted cubic splines, with the median value of PhA as the reference. Results: PhA was lower among women who later developed CVD than among women who did not (6.3 vs. 6.0; P < 0.001). The highest risk of CVD was observed at the 5th percentile (hazard ratio: 1.33; 95% confidence interval, 1.11–1.60). Among men, PhA was not significantly associated with risk of CVD (7.1 vs. 7.0; P = 0.246). Conclusions: Among apparently healthy Danish men and women, a lower PhA value was associated with a higher incidence of CVD over 24 y, also after adjusting for potential confounders, and particularly among women. These findings may encourage the future use of PhA as an additional index in predicting CVD. However, more studies are needed to confirm our results.},
address = {R.D. Langer, Growth and Development Laboratory, Center for Investigation in Pediatrics, University of Campinas, Campinas, S{\~{a}}o Paulo, Brazil},
author = {Langer, R D and Larsen, S C and Ward, L C and Heitmann, B L},
doi = {10.1016/j.nut.2021.111280},
issn = {1873-1244},
journal = {Nutrition},
keywords = {Dane (people),adult,age,article,bioelectrical impedance analysis,body height,body weight,cardiovascular disease,cardiovascular mortality,cardiovascular risk,disease association,female,human,lifestyle,major clinical study,male,morbidity,phase angle,physical parameters,risk factor},
language = {English},
title = {{Phase angle measured by bioelectrical impedance analysis and the risk of cardiovascular disease among adult Danes}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L2012507256&from=export http://dx.doi.org/10.1016/j.nut.2021.111280},
volume = {89},
year = {2021}
}
@article{Kyle2012,
abstract = {Background & aims: Low phase angle (PhA) by bioelectrical impedance analysis (BIA), is associated with increased morbidity and nutritional risk. This study determined the cut-off values for PhA compared to Nutritional Risk Screening (NRS-2002) and Subjective Global Assessment (SGA) in patients at hospital admission, and evaluated the association between PhA and serum albumin. Methods: PhA was determined in patients (Men (M)/Women (W) = 382/267), and healthy age-, sex- and height-matched controls. Sensitivity and specificity were calculated for PhA compared to NRS-2002, SGA and serum albumin. The cut-off values were assessed by receiver operator characteristics area under the curve (ROC-AUC). Results: The best PhA cut-offs were 5.0° and 4.6° in M/W. The sensitivity for NRS-2002 was 70.0/58.1% (M/W); SGA: 73.3/64.5%; albumin: 58.8/23.5%; specificity for NRS-2002: 85.1/81.7% (M/W); SGA: 76.6/76.1% and albumin: 93.2/96.6%. The PhA showed a ROC-AUC for NRS-2002 of 0.85/0.80 (M/W); SGA: 0.83/0.80 and albumin: 0.85/0.91. Patients with albumin levels <35. g/L had a relative risk of 7.5 to have low PhA compared to patients with ≥35. g/L. Conclusions: The consistent sensitivity and specificity between PhA and three screening tools strengthens the validity of our study. PhA appears to be a useful screening tool to assess nutritional risk without having to measure weight or height. {\textcopyright} 2012 Elsevier Ltd and European Society for Clinical Nutrition and Metabolism.},
