url = {https://www.embase.com/search/results?subaction=viewrecord&id=L636086646&from=export http://dx.doi.org/10.1080/16070658.2021.1968126},
volume = {34},
year = {2021}
}
@article{Staufer2017a,
abstract = {Objectives: Nutritional status is an important prognostic factor in patients with cystic fibrosis (CF) and correlates with lung function (LF) prior to lung transplantation (LT). This study investigated the impact of nutritional status on LF in adult CF patients after LT. Methods: Consecutive adult double-lung transplanted CF patients attending our CF outpatient service between Jan 2012 and Jul 2015 were included. The impact of body composition (BC) as assessed by 3 compartment model bioelectrical impedance analysis (BIA), BMI, as well as 25-hydroxy (OH)-vitamin D-levels on LF was investigated at initial and follow-up visits. Results: In total 147 LF tests and BIAs were performed in 58 patients (59% female; median age: 30.1 y; median BMI: 19.6 kg/m2; 77.6% diabetes, 96.6% pancreatic insufficiency, 81% F508del carrier) at up to 5 time points per patient. Malnourished patients (BMI < 18.5 kg/m2; 28%) had a significantly reduced LF compared to normal/overweight patients (median FEV1%pred 57% vs. 77%, p = 0.024). During median follow up of 0.6 years (range: 0.3-2.4 y), FEV1%pred loss in malnourished patients was substantially higher than in normal/overweight patients. BMI, as well as the single BIA parameters phase angle, total body water, fat free mass, body cell mass (BCM), and extracellular mass (ECM)/BCM ratio were univariate predictors of LF as assessed by FEV1%pred. When included in a stepwise logistic regression model ECM/BCM ratio (OR =-15.427, 95%CI:-27.273;-3.581, p = 0.012) remained the only significant predictor of LF. Vitamin D-deficiency (median vitamin D-level: 37.2 nmol/L; range: 10.0-91.1) was present in 93.1% of patients and significantly correlated with VCmax and FEV1. Conclusion: Malnutrition and vitamin D-deficiency are highly prevalent in CF patients after LT. BC as assessed by BIA was superior to BMI in prediction of LF. Continuous care by dietitians and gastroenterologists trained in CF care might improve nutritional status and LF in CF patients after LT.},
address = {K. Staufer, Medical University of Vienna, Vienna, Austria},
author = {Staufer, K and Halilbasic, E and Hillebrand, P and Harm, S and Schwarz, S and Jaksch, P and Klepetko, W and Trauner, M and Kazemi-Shirazi, L},
issn = {1873-5010},
journal = {Journal of Cystic Fibrosis},
keywords = {adult,body mass,cell body,compartment model,conference abstract,controlled study,cystic fibrosis,diabetes mellitus,dietitian,endogenous compound,extracellular space,fat free mass,female,follow up,forced expiratory volume,gastroenterologist,human,impedance,lung function test,lung transplantation,major clinical study,male,malnutrition,nutritional status,obesity,outpatient care,pancreatic insufficiency,prediction,total body water,vitamin D,vitamin D deficiency},
language = {English},
pages = {S9},
title = {{Nutritional status as assessed by 3 compartment model bioelectrical impedance analysis predicts lung function in adult patients with cystic fibrosis after lung transplantation}},
url = {https://www.embase.com/search/results?subaction=viewrecord&id=L620749533&from=export},
volume = {16},
